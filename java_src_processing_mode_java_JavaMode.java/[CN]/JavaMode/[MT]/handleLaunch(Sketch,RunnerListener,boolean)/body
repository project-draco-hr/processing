{
  JavaBuild build=new JavaBuild(sketch);
  String appletClassName=build.build(false);
  if (appletClassName != null) {
    final Runner runtime=new Runner(build,listener);
    new Thread(new Runnable(){
      public void run(){
        runtime.launch(present);
      }
    }
).start();
    return runtime;
  }
  return null;
}
