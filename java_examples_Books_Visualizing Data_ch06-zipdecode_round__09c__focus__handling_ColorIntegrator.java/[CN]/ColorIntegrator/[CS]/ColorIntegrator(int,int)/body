{
  int a1=(color0 >> 24) & 0xff;
  int r1=(color0 >> 16) & 0xff;
  int g1=(color0 >> 8) & 0xff;
  int b1=(color0) & 0xff;
  int a2=(color1 >> 24) & 0xff;
  int r2=(color1 >> 16) & 0xff;
  int g2=(color1 >> 8) & 0xff;
  int b2=(color1) & 0xff;
  r0=(float)r1 / 255.0f;
  g0=(float)g1 / 255.0f;
  b0=(float)b1 / 255.0f;
  a0=(float)a1 / 255.0f;
  rs=(r2 - r1) / 255.0f;
  gs=(g2 - g1) / 255.0f;
  bs=(b2 - b1) / 255.0f;
  as=(a2 - a1) / 255.0f;
}
