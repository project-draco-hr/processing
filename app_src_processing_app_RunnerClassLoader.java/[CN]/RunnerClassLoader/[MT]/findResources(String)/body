{
  return new Enumeration(){
    URL resource=findResource(name);
    public boolean hasMoreElements(){
      return (resource != null ? true : false);
    }
    public Object nextElement(){
      if (!hasMoreElements()) {
        throw new NoSuchElementException();
      }
 else {
        URL result=resource;
        resource=null;
        return result;
      }
    }
  }
;
}
