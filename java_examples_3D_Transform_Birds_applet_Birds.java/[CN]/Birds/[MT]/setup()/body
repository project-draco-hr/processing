{
  size(640,360,P3D);
  noStroke();
  for (int i=0; i < birdCount; i++) {
    birds[i]=new Bird(random(-300,300),random(-300,300),random(-500,-2500),random(5,30),random(5,30));
    x[i]=random(20,340);
    y[i]=random(30,350);
    z[i]=random(1000,4800);
    rx[i]=random(-160,160);
    ry[i]=random(-55,55);
    rz[i]=random(-20,20);
    spd[i]=random(.1f,3.75f);
    rot[i]=random(.025f,.15f);
  }
}
