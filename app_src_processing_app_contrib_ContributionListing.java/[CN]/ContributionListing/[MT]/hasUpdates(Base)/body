{
  for (  ModeContribution m : base.getModeContribs())   if (hasUpdates(m))   return true;
  for (  Library l : base.getActiveEditor().getMode().contribLibraries)   if (hasUpdates(l))   return true;
  for (  ToolContribution t : base.getActiveEditor().getToolContribs())   if (hasUpdates(t))   return true;
  return false;
}
