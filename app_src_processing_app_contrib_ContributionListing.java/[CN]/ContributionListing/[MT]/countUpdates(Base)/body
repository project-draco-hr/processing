{
  int count=0;
  for (  ModeContribution mc : base.getModeContribs()) {
    if (hasUpdates(mc)) {
      count++;
    }
  }
  for (  Library lib : base.getActiveEditor().getMode().contribLibraries) {
    if (hasUpdates(lib)) {
      count++;
    }
  }
  for (  ToolContribution tc : base.getToolContribs()) {
    if (hasUpdates(tc)) {
      count++;
    }
  }
  return count;
}
