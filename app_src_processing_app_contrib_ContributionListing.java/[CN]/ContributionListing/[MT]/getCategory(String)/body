{
  if (category == null) {
    return "Unknown";
  }
  String normCatName=normalize(category);
  for (  String validCatName : validCategories) {
    String normValidCatName=normalize(validCatName);
    if (normValidCatName.equals(normCatName)) {
      return validCatName;
    }
  }
  return category;
}
