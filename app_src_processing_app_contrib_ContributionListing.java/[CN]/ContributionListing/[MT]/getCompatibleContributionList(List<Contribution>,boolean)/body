{
  ArrayList<Contribution> filteredList=new ArrayList<Contribution>(filteredLibraries);
  if (!filter)   return filteredList;
  Iterator<Contribution> it=filteredList.iterator();
  while (it.hasNext()) {
    Contribution libInfo=it.next();
    if (!libInfo.isCompatible(Base.getRevision())) {
      it.remove();
    }
  }
  return filteredList;
}
