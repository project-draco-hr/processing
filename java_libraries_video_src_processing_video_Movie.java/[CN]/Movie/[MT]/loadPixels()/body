{
  super.loadPixels();
  if (useBufferSink) {
    if (natBuffer != null) {
      IntBuffer buf=natBuffer.getByteBuffer().asIntBuffer();
      buf.rewind();
      buf.get(pixels);
      Video.convertToARGB(pixels,width,height);
    }
 else     if (sinkGetMethod != null) {
      try {
        sinkGetMethod.invoke(bufferSink,new Object[]{pixels});
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
    outdatedPixels=false;
  }
}
