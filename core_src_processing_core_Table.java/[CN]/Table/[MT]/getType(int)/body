{
  int tableType=UNSUPPORTED;
switch (mysqlType) {
case Types.TINYINT:
case Types.SMALLINT:
case Types.INTEGER:
    tableType=INT;
  break;
case Types.BIGINT:
tableType=LONG;
break;
case Types.FLOAT:
tableType=FLOAT;
break;
case Types.DECIMAL:
case Types.DOUBLE:
case Types.REAL:
tableType=DOUBLE;
break;
case Types.CHAR:
case Types.VARCHAR:
tableType=STRING;
break;
case Types.DATE:
case Types.TIME:
tableType=TIME;
break;
}
return tableType;
}
