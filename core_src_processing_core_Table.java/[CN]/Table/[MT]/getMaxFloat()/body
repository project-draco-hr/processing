{
  boolean found=false;
  float max=missingFloat;
  for (int row=0; row < getRowCount(); row++) {
    for (int col=0; col < getColumnCount(); col++) {
      float value=getFloat(row,col);
      if (!Float.isNaN(value)) {
        if (!found) {
          max=value;
          found=true;
        }
 else         if (value > max) {
          max=value;
        }
      }
    }
  }
  return max;
}
