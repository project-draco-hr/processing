{
  int[] outgoing=new int[rowCount];
  int count=0;
  if (what == null) {
    for (int row=0; row < rowCount; row++) {
      if (data[row][column] == null) {
        outgoing[count++]=row;
      }
    }
  }
 else {
    for (int row=0; row < rowCount; row++) {
      String value=data[row][column];
      if (value != null && value.equals(what)) {
        outgoing[count++]=row;
      }
    }
  }
  return PApplet.subset(outgoing,0,count);
}
