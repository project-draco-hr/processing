{
  if (texture == null || changedFrontTex) {
    if (USE_JOGL_FBOLAYER) {
      texture=new Texture(pg);
      texture.init(pg.width,pg.height,backTexAttach.getName(),TEXTURE_2D,RGBA,frontTexAttach.getWidth(),frontTexAttach.getHeight(),frontTexAttach.minFilter,frontTexAttach.magFilter,frontTexAttach.wrapS,frontTexAttach.wrapT);
      texture.invertedY(true);
      texture.colorBuffer(true);
    }
 else {
      texture=super.wrapFrontTexture(null);
    }
  }
 else {
    if (USE_JOGL_FBOLAYER) {
      texture.glName=frontTexAttach.getName();
    }
 else {
      texture=super.wrapFrontTexture(texture);
    }
  }
  return texture;
}
