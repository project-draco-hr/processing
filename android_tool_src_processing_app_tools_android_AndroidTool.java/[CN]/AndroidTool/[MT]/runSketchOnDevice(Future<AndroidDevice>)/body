{
  final Build build=getBuilder();
  if (!build.createProject()) {
    return;
  }
  if (!build.antBuild("debug")) {
    return;
  }
  final AndroidDevice device=waitForDevice(deviceFuture);
  if (device == null) {
    return;
  }
  if (!device.installApp(build.getPathForAPK("debug"),editor)) {
    return;
  }
  startSketch(device);
}
