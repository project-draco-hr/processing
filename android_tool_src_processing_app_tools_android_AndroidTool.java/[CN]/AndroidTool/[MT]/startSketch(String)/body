{
  try {
    final ProcessResult result=new ProcessHelper("adb","-s",device,"shell","am","start","-e","debug","true","-a","android.intent.action.MAIN","-c","android.intent.category.LAUNCHER","-n",build.getPackageName() + "/." + build.getClassName()).execute();
    if (result.succeeded()) {
      final boolean emu=device.startsWith("emulator");
      editor.statusNotice("Sketch started on the " + (emu ? "emulator" : "phone") + ".");
      return "";
    }
    editor.statusError("Could not start the sketch.");
    System.err.println(result);
  }
 catch (  final IOException e) {
    editor.statusError(e);
  }
catch (  final InterruptedException e) {
    e.printStackTrace();
  }
  return null;
}
