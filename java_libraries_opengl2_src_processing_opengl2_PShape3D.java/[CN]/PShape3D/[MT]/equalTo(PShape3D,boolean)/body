{
  boolean res=family == child.family && glMode == child.glMode && strokeWeight == child.strokeWeight && (ignoreNames || name.equals(child.name));
  if (!res)   return false;
  for (int i=0; i < textures.length; i++) {
    if (textures[i] != child.textures[i]) {
      res=false;
    }
  }
  return res;
}
