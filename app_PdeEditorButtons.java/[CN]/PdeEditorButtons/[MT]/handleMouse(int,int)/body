{
  if (currentRollover != -1) {
    if ((y > y1[currentRollover]) && (x > x1) && (y < y2[currentRollover])&& (x < x2)) {
      return;
    }
 else {
      setState(currentRollover,INACTIVE,true);
      messageClear(title[currentRollover]);
      currentRollover=-1;
    }
  }
  int sel=findSelection(x,y);
  if (sel == -1)   return;
  if (state[sel] != ACTIVE) {
    setState(sel,ROLLOVER,true);
    currentRollover=sel;
  }
}
