{
switch (currentSelection) {
case RUN:
    editor.doRun(e.isShiftDown());
  break;
case STOP:
setState(RUN,INACTIVE,true);
editor.handleStop();
break;
case OPEN:
setState(OPEN,INACTIVE,true);
break;
case NEW:
editor.skNew();
break;
case SAVE:
editor.doSave();
break;
case EXPORT:
editor.skExport();
break;
}
currentSelection=-1;
}
