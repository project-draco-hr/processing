{
switch (currentSelection) {
case RUN:
    if (!disableRun) {
      editor.handleRun(e.isShiftDown());
    }
  break;
case STOP:
if (!disableRun) {
  setState(RUN,INACTIVE,true);
  editor.handleStop();
}
break;
case OPEN:
setState(OPEN,INACTIVE,true);
break;
case NEW:
editor.handleNew(e.isShiftDown());
break;
case SAVE:
editor.handleSave();
break;
case EXPORT:
editor.handleExport();
break;
}
currentSelection=-1;
}
