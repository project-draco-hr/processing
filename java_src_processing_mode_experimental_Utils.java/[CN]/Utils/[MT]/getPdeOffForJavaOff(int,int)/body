{
  System.out.println("PDE <-> Java");
  for (int i=0; i < offsetMatch.size(); i++) {
    System.out.print(offsetMatch.get(i).pdeOffset + " <-> " + offsetMatch.get(i).javaOffset);
    System.out.println(", " + word1.charAt(offsetMatch.get(i).pdeOffset) + " <-> "+ word2.charAt(offsetMatch.get(i).javaOffset));
  }
  System.out.println("Length " + offsetMatch.size());
  System.out.println(start + " java start off, pde start off " + getPdeOffForJavaOff(start));
  System.out.println((start + length - 1) + " java end off, pde end off " + getPdeOffForJavaOff(start + length - 1));
}
