{
  System.out.println(element.toString());
  System.out.println(columnIndex);
switch (columnIndex) {
case 0:
    if (element instanceof ProjectTableEntryModel)     return ((ProjectTableEntryModel)element).getProject().getProject().getName();
  if (element != null)   element.toString();
return "";
case 1:
if (element instanceof ProjectTableEntryModel) return ((ProjectTableEntryModel)element).getAdditionalMessage();
return "";
default :
return "";
}
}
