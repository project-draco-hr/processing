{
  for (int row=0; row < rowCount; row++) {
    String s=get(row,column);
    if (s != null) {
      char[] c=s.toCharArray();
      int index=0;
      for (int j=0; j < c.length; j++) {
        if (tokens.indexOf(c[j]) == -1) {
          if (index != j) {
            c[index]=c[j];
          }
          index++;
        }
      }
      if (index != c.length) {
        set(row,column,new String(c,0,index));
      }
    }
  }
}
