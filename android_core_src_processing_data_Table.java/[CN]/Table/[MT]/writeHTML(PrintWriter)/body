{
  writer.println("<table>");
  for (int row=0; row < getRowCount(); row++) {
    writer.println("  <tr>");
    for (int col=0; col < getColumnCount(); col++) {
      String entry=get(row,col);
      writer.print("    <td>");
      writeEntryHTML(writer,entry);
      writer.println("</td>");
    }
    writer.println("  </tr>");
  }
  writer.println("</table>");
  writer.flush();
}
