{
  thread=null;
  try {
    if (input != null) {
      input.close();
      input=null;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    if (output != null) {
      output.close();
      output=null;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    if (socket != null) {
      socket.close();
      socket=null;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
