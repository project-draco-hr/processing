{
  if (this.runtime != null) {
    throw new BuildException("Runtime already specified.");
  }
  this.runtime=runtime;
  runtime.appendIncludes(new String[]{"jre/"});
  runtime.appendExcludes(new String[]{"bin/","jre/bin/","jre/lib/deploy/","jre/lib/deploy.jar","jre/lib/javaws.jar","jre/lib/libdeploy.dylib","jre/lib/libnpjp2.dylib","jre/lib/plugin.jar","jre/lib/security/javaws.policy"});
}
