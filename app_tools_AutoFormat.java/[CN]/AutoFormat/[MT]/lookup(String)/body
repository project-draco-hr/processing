{
  char r;
  int l, kk, k, i;
  String j_string=new String(string);
  if (j < 1)   return (0);
  kk=0;
  while (string[kk] == ' ')   kk++;
  l=0;
  l=j_string.indexOf(keyword);
  if (l < 0 || l != kk) {
    return 0;
  }
  r=string[kk + keyword.length()];
  if (r >= 'a' && r <= 'z')   return (0);
  if (r >= 'A' && r <= 'Z')   return (0);
  if (r >= '0' && r <= '9')   return (0);
  if (r == '_' || r == '&')   return (0);
  return (1);
}
