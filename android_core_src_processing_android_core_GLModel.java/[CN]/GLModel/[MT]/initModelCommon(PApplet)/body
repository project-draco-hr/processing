{
  this.parent=parent;
  pgl=(PGraphicsAndroid3D)parent.g;
  if (pgl.gl instanceof GL11) {
    gl=(GL11)pgl.gl;
  }
 else {
    throw new RuntimeException("GLModel requires GL 1.1");
  }
  tintR=tintG=tintB=tintA=1.0f;
  shininess[0]=0.0f;
  pointSize=1.0f;
  lineWidth=1.0f;
  usingPointSprites=false;
  blend=false;
  blendMode=ADD;
  tmpVertArray=null;
  tmpColorArray=null;
  tmpNormalsArray=null;
  tmpTexCoordsArray=null;
}
