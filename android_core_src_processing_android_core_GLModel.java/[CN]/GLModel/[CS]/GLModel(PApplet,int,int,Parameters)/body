{
  this.parent=parent;
  a3d=(PGraphicsAndroid3D)parent.g;
  gl=a3d.gl11;
  if (gl == null) {
    throw new RuntimeException("GLModel: OpenGL ES 1.1 required");
  }
  if (!a3d.vboSupported) {
    throw new RuntimeException("GLModel: Vertex Buffer Objects are not available");
  }
  numVertices=numVert;
  numTextures=numTex;
  setParameters(params);
  initBufferIDs();
  createVertexBuffer();
  createColorBuffer();
  createNormalBuffer();
  createTexCoordBuffer();
  initGroups();
  updateVertexArray=null;
  updateColorArray=null;
  updateNormalArray=null;
  updateTexCoordArray=null;
  updateElement=-1;
  selectedTexture=0;
  float[] tmp={0.0f};
  gl.glGetFloatv(GL11.GL_POINT_SIZE_MAX,tmp,0);
  MAX_POINTSIZE=tmp[0];
}
