{
  this.parent=parent;
  a3d=(PGraphicsAndroid3D)parent.g;
  if (a3d.gl instanceof GL11) {
    gl=(GL11)a3d.gl;
  }
 else {
    throw new RuntimeException("GLModel: OpenGL ES 1.1 required");
  }
  numVertices=numVert;
  numTextures=numTex;
  readParameters(params);
  initBufferIDs();
  createVertexBuffer();
  createColorBuffer();
  createNormalBuffer();
  createTexCoordBuffer();
  initGroups();
  updateVertexArray=null;
  updateColorArray=null;
  updateNormalArray=null;
  updateTexCoordArray=null;
  updateElement=-1;
  selectedTexture=0;
  imageMode=a3d.imageMode;
  tintR=a3d.tintR;
  tintR=tintG=tintB=tintA=1.0f;
  shininess[0]=0.0f;
  pointSize=1.0f;
  lineWidth=1.0f;
  usingPointSprites=false;
  blend=false;
  blendMode=ADD;
  float[] tmp={0.0f};
  gl.glGetFloatv(GL11.GL_POINT_SIZE_MAX,tmp,0);
  maxPointSize=tmp[0];
}
