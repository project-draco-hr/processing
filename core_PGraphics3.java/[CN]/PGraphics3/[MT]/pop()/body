{
  if (matrixStackDepth == 0) {
    throw new RuntimeException("too many calls to pop() " + "(and not enough to push)");
  }
  matrixStackDepth--;
  float mat[]=matrixStack[matrixStackDepth];
  m00=mat[0];
  m01=mat[1];
  m02=mat[2];
  m03=mat[3];
  m10=mat[4];
  m11=mat[5];
  m12=mat[6];
  m13=mat[7];
  m20=mat[8];
  m21=mat[9];
  m22=mat[10];
  m23=mat[11];
  m30=mat[12];
  m31=mat[13];
  m32=mat[14];
  m33=mat[15];
}
