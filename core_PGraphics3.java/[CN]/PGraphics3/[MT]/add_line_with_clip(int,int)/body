{
  float az=vertices[a][VZ];
  float bz=vertices[b][VZ];
  if (az > cameraNear) {
    if (bz > cameraNear) {
      return;
    }
    int cb=interpolate_clip_vertex(a,b);
    add_line_no_clip(cb,b);
    return;
  }
 else {
    if (bz <= cameraNear) {
      add_line_no_clip(a,b);
      return;
    }
    int cb=interpolate_clip_vertex(a,b);
    add_line_no_clip(a,cb);
    return;
  }
}
