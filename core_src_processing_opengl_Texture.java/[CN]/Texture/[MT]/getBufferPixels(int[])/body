{
  BufferData data=null;
  if (usedBuffers != null && 0 < usedBuffers.size()) {
    data=usedBuffers.getLast();
  }
 else   if (bufferCache != null && 0 < bufferCache.size()) {
    data=bufferCache.getFirst();
  }
  if (data != null) {
    data.rgbBuf.rewind();
    data.rgbBuf.get(pixels);
    convertToARGB(pixels);
  }
}
