{
  g.beginShape();
  g.vertex(x[0],y[0]);
  int i=1;
  while (i < count) {
switch (kind[i]) {
case MOVETO:
      g.breakShape();
    g.vertex(x[i],y[i]);
  i++;
break;
case LINETO:
g.vertex(x[i],y[i]);
i++;
break;
case QCURVETO:
g.bezierVertex(x[i],y[i],x[i + 1],y[i + 1],x[i + 1],y[i + 1]);
i+=2;
break;
case CURVETO:
g.bezierVertex(x[i],y[i],x[i + 1],y[i + 1],x[i + 2],y[i + 2]);
i+=3;
break;
}
}
g.endShape(closed ? PConstants.CLOSE : PConstants.OPEN);
}
