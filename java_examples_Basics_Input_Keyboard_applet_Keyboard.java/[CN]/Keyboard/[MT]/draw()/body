{
  if (keyPressed) {
    if (key >= 'A' && key <= 'z') {
      if (key <= 'Z') {
        keyIndex=key - 'A';
      }
 else {
        keyIndex=key - 'a';
      }
      fill(millis() % 255);
      float beginRect=rectWidth / 2 + keyIndex * keyScale - rectWidth / 2;
      rect(beginRect,0.0f,rectWidth,height);
    }
  }
}
