{
  if (vertexShader != 0) {
    renderer.deleteGLSLVertShaderObject(vertexShader);
    vertexShader=0;
  }
  if (fragmentShader != 0) {
    renderer.deleteGLSLFragShaderObject(fragmentShader);
    fragmentShader=0;
  }
  if (programObject != 0) {
    renderer.deleteGLSLProgramObject(programObject);
    programObject=0;
  }
}
