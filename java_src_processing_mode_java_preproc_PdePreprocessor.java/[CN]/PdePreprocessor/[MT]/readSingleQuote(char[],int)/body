{
  StringBuilder sb=new StringBuilder();
  try {
    sb.append(c[i++]);
    if (c[i] == '\\') {
      sb.append(c[i++]);
      if (c[i] == 'u') {
        for (int j=0; j < 4; j++) {
          sb.append(c[i++]);
        }
      }
    }
    sb.append(c[i++]);
    sb.append(c[i++]);
  }
 catch (  ArrayIndexOutOfBoundsException ignored) {
  }
  return sb.toString();
}
