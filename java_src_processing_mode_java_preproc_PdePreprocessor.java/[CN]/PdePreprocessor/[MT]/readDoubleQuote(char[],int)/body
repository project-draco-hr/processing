{
  StringBuilder sb=new StringBuilder();
  try {
    sb.append(c[i++]);
    while (i < c.length) {
      if (c[i] == '\\') {
        sb.append(c[i++]);
        sb.append(c[i++]);
      }
 else       if (c[i] == '\"') {
        sb.append(c[i++]);
        break;
      }
 else {
        sb.append(c[i++]);
      }
    }
  }
 catch (  ArrayIndexOutOfBoundsException ignored) {
  }
  return sb.toString();
}
