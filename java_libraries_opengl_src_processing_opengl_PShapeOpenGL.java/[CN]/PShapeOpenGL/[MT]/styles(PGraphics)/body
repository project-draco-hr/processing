{
  if (stroke) {
    stroke(g.strokeColor);
    strokeWeight(g.strokeWeight);
    strokeCap(g.strokeCap);
    strokeJoin(g.strokeJoin);
  }
 else {
    noStroke();
  }
  if (fill) {
    fill(g.fillColor);
  }
 else {
    noFill();
  }
  ambient(g.ambientColor);
  specular(g.specularColor);
  emissive(g.emissiveColor);
  shininess(g.shininess);
}
