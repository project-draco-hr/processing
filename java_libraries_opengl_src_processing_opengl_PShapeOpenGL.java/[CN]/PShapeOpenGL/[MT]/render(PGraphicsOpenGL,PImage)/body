{
  if (root == null) {
    return;
  }
  if (havePolys) {
    renderPolys(g,texture);
    if (g.haveRaw()) {
      rawPolys(g,texture);
    }
  }
  if (haveLines) {
    renderLines(g);
    if (g.haveRaw()) {
      rawLines(g);
    }
  }
  if (havePoints) {
    renderPoints(g);
    if (g.haveRaw()) {
      rawPoints(g);
    }
  }
}
