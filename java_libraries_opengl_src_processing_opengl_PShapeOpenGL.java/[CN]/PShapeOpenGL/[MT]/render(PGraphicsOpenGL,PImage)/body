{
  if (root == null) {
    return;
  }
  if (haveFill) {
    renderFill(g,texture);
    if (g.haveRaw()) {
      rawFill(g,texture);
    }
  }
  if (haveLines) {
    renderLines(g);
    if (g.haveRaw()) {
      rawLines(g);
    }
  }
  if (havePoints) {
    renderPoints(g);
    if (g.haveRaw()) {
      rawPoints(g);
    }
  }
}
