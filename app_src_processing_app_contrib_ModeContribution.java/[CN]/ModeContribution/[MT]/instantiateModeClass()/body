{
  try {
    Class<?> modeClass=Class.forName(className,true,loader);
    Constructor contr=modeClass.getConstructor(Base.class,File.class);
    mode=(Mode)contr.newInstance(base,folder);
    mode.setupGUI();
    return true;
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return false;
}
