{
  try {
    return new ModeContribution(base,folder,searchName);
  }
 catch (  IgnorableException ig) {
    Messages.log(ig.getMessage());
  }
catch (  Throwable err) {
    if (searchName == null) {
      throw new RuntimeException(err);
    }
 else {
      Messages.loge("ModeContribution.load() failed for " + searchName,err);
    }
  }
  return null;
}
