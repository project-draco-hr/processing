{
  super(folder);
  className=initLoader(className);
  if (className != null) {
    Class<?> modeClass=loader.loadClass(className);
    Constructor con=modeClass.getConstructor(Base.class,File.class);
    mode=(Mode)con.newInstance(base,folder);
    mode.setClassLoader(loader);
    mode.setupGUI();
  }
}
