{
  PShape shape=null;
  if (type == PConstants.GROUP) {
    shape=new PShape(pg,PConstants.GROUP);
  }
 else   if (type == PShape.PATH) {
    shape=new PShape(pg,PShape.PATH);
  }
 else   if (type == PShape.GEOMETRY) {
    shape=new PShape(pg,PShape.GEOMETRY);
  }
  shape.set3D(false);
  return shape;
}
