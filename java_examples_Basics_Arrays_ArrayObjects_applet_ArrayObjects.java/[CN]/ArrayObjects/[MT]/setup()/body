{
  size(200,200);
  background(176);
  noStroke();
  num=width / unit * width / unit;
  mods=new Module[num];
  for (int i=0; i < height / unit; i++) {
    for (int j=0; j < height / unit; j++) {
      int index=i * height / unit + j;
      mods[index]=new Module(j * unit,i * unit,unit / 2,unit / 2,random(0.05f,0.8f));
    }
  }
}
