{
  int f=(b & ALPHA_MASK) >>> 24;
  return (low(((a & ALPHA_MASK) >>> 24) + f,0xff) << 24 | low(((a & RED_MASK) + ((b & RED_MASK) >> 8) * f),RED_MASK) & RED_MASK | low(((a & GREEN_MASK) + ((b & GREEN_MASK) >> 8) * f),GREEN_MASK) & GREEN_MASK | low((a & BLUE_MASK) + (((b & BLUE_MASK) * f) >> 8),BLUE_MASK));
}
