{
  int f=(b & ALPHA_MASK) >>> 24;
  return (low(((a & ALPHA_MASK) >>> 24) + f,0xff) << 24 | mix(a & RED_MASK,low(a & RED_MASK,((b & RED_MASK) >> 8) * f),f) & RED_MASK | mix(a & GREEN_MASK,low(a & GREEN_MASK,((b & GREEN_MASK) >> 8) * f),f) & GREEN_MASK | mix(a & BLUE_MASK,low(a & BLUE_MASK,((b & BLUE_MASK) * f) >> 8),f));
}
