{
  char[] buffer=new char[100];
  int remaining=buffer.length;
  reader.mark(remaining);
  int commas=0;
  int tabs=0;
  for (int i=0; i < remaining; i++) {
    int c=reader.read();
    if (c == -1)     break;
    if (c == ',')     commas++;
    if (c == '\t')     tabs++;
  }
  reader.reset();
  return (commas > tabs);
}
