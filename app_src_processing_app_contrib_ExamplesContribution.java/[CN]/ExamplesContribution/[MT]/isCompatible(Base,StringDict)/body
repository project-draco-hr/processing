{
  String currentIdentifier=base.getActiveEditor().getMode().getIdentifier();
  StringList compatibleList=parseModeList(props);
  if (compatibleList.size() == 0) {
    return true;
  }
  for (  String c : compatibleList) {
    if (c.equals(currentIdentifier)) {
      return true;
    }
  }
  return false;
}
