{
  final ProcessHelper p=new ProcessHelper(AndroidTool.toolName,"create","avd","-n",name,"-t",target,"-c","64M");
  try {
    if (p.execute(true,false) == 0) {
      return true;
    }
    System.out.println("Attempted: '" + p.getCommand() + "'");
    p.dump();
  }
 catch (  final InterruptedException ie) {
  }
  return false;
}
