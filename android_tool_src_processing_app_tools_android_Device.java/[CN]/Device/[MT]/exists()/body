{
  final ProcessHelper p=new ProcessHelper(AndroidTool.toolName,"list","avds");
  try {
    if (p.execute(true) == 0) {
      for (      final String line : p.getStdout().split("\n")) {
        final String[] m=PApplet.match(line,"\\s+Name:\\s+(\\S+)");
        if (m != null) {
          if (m[1].equals(name)) {
            return true;
          }
        }
      }
    }
 else {
      p.dump();
    }
  }
 catch (  final InterruptedException ie) {
  }
  return false;
}
