{
  if (!url.startsWith("http://")) {
    url="file://" + url;
    if (url.indexOf(' ') != -1) {
      StringBuffer sb=new StringBuffer();
      char c[]=url.toCharArray();
      for (int i=0; i < c.length; i++) {
        if (c[i] == ' ') {
          sb.append("%20");
        }
 else {
          sb.append(c[i]);
        }
      }
      url=sb.toString();
    }
  }
  com.apple.eio.FileManager.openURL(url);
}
