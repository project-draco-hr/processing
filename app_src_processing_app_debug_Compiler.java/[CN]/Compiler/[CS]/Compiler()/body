{
  try {
    javacResources=ResourceBundle.getBundle(javacBundleName);
    compilerResources=ResourceBundle.getBundle(compilerBundleName);
  }
 catch (  MissingResourceException e) {
    e.printStackTrace();
    throw new RuntimeException("The JDK is not installed properly. " + "Please use the version of Processing " + "that includes Java.");
  }
}
