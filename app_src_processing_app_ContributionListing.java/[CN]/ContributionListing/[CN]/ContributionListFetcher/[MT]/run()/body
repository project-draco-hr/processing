{
  downloader=new FileDownloader(url,dest,progressMonitor);
  downloader.setPostOperation(new Runnable(){
    public void run(){
      File xmlFile=downloader.getFile();
      if (xmlFile != null) {
        hasDownloadedLatestList=true;
        setAdvertisedList(xmlFile);
      }
    }
  }
);
  downloader.run();
}
