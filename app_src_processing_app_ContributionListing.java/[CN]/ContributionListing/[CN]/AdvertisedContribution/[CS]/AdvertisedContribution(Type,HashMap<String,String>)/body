{
  this.type=type;
  name=exports.get("name");
  category=ContributionListing.getCategory(exports.get("category"));
  authorList=exports.get("authorList");
  url=exports.get("url");
  sentence=exports.get("sentence");
  paragraph=exports.get("paragraph");
  int v=0;
  try {
    v=Integer.parseInt(exports.get("version"));
  }
 catch (  NumberFormatException e) {
  }
  version=v;
  prettyVersion=exports.get("prettyVersion");
  String download=null;
  String hostPlatform=Base.getPlatformName();
  int nativeBits=Base.getNativeBits();
  String hostVersion=Base.getPlatformVersionName();
  if (!hostPlatform.isEmpty())   download=exports.get("download." + hostPlatform + nativeBits+ "."+ hostVersion);
  if (download == null)   download=exports.get("download." + hostPlatform + "."+ hostVersion);
  if (download == null)   download=exports.get("download." + hostPlatform + nativeBits);
  if (download == null)   download=exports.get("download." + hostPlatform);
  if (download == null)   download=exports.get("download");
  this.link=download;
}
