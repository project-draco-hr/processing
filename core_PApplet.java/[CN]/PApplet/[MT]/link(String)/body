{
  if (!online) {
    System.err.println("Can't open " + here);
    System.err.println("link() only works inside a web browser");
    return;
  }
  try {
    getAppletContext().showDocument(new URL(here));
  }
 catch (  Exception e) {
    System.err.println("Could not open " + here);
    e.printStackTrace();
  }
}
