{
  if (g != null) {
    size(iwidth,iheight,g.getClass().getName());
  }
 else {
    if (PApplet.javaVersion >= 1.3f) {
      try {
        Class c=Class.forName(JAVA2D);
        size(iwidth,iheight,JAVA2D);
        return;
      }
 catch (      ClassNotFoundException e) {
      }
    }
    size(iwidth,iheight,P2D);
  }
}
