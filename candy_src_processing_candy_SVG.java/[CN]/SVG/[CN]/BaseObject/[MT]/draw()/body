{
  if (!display)   return;
  if (!ignoreStyles) {
    drawStyles();
  }
  if (hasTransform) {
    parent.pushMatrix();
    parent.applyMatrix(transformation[0],transformation[1],transformation[2],transformation[3],transformation[4],transformation[5]);
  }
  drawShape();
  if (hasTransform) {
    parent.popMatrix();
  }
}
