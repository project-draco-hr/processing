{
  try {
    frame.setValue(var,value);
  }
 catch (  InvalidTypeException ex) {
    Logger.getLogger(LocalVariableNode.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  ClassNotLoadedException ex) {
    Logger.getLogger(LocalVariableNode.class.getName()).log(Level.SEVERE,null,ex);
  }
  this.value=value;
}
