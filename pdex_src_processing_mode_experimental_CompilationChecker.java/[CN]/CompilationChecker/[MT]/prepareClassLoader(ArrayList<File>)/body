{
  URL urls[]=new URL[jarList.size()];
  for (int i=0; i < urls.length; i++) {
    try {
      urls[i]=jarList.get(i).toURI().toURL();
    }
 catch (    MalformedURLException e) {
      e.printStackTrace();
    }
  }
  urlClassLoader=new URLClassLoader(urls);
  System.out.println("URL Classloader ready");
}
