{
  statusNotice(Language.text("editor.status.saving"));
  try {
    if (sketch.saveAs()) {
      if (false && Preferences.getBoolean("editor.watcher")) {
        initFileChangeListener();
      }
    }
 else {
      statusNotice(Language.text("editor.status.saving.canceled"));
      return false;
    }
  }
 catch (  Exception e) {
    statusError(e);
  }
  return true;
}
