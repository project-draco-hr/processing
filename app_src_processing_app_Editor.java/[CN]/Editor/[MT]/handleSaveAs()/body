{
  handleStop();
  toolbar.activate(EditorToolbar.SAVE);
  message("Saving...");
  try {
    if (sketch.saveAs()) {
      message("Done Saving.");
    }
 else {
      message("Save Canceled.");
      return false;
    }
  }
 catch (  Exception e) {
    error(e);
  }
 finally {
    toolbar.deactivate(EditorToolbar.SAVE);
  }
  return true;
}
