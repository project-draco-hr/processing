{
  didReload=false;
  for (  WatchEvent<?> e : events) {
    boolean openedFile=false;
    if (((Path)e.context()).getFileName().toString().startsWith(".goutputstream")) {
      openedFile=true;
    }
    if (!openedFile) {
      for (      SketchCode sc : sketch.getCode()) {
        if (sc.getFileName().equals(((Path)e.context()).getFileName().toString())) {
          openedFile=true;
          break;
        }
      }
    }
    if (!openedFile) {
      continue;
    }
    if (didReload) {
      break;
    }
    if (e.kind().equals(StandardWatchEventKinds.ENTRY_MODIFY)) {
      int response=Base.showYesNoQuestion(Editor.this,"File Modified","Your sketch has been modified externally","Would you like to reload the sketch?");
      if (response == 0) {
        sketch.reload();
        header.rebuild();
        didReload=true;
      }
    }
 else {
    }
  }
  saved=false;
}
