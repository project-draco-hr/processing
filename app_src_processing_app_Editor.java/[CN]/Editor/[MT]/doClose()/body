{
  try {
    if (runtime.window != null) {
      appletLocation=runtime.window.getLocation();
    }
  }
 catch (  NullPointerException e) {
  }
  doStop();
  try {
    if (runtime != null) {
      runtime.close();
      runtime=null;
    }
  }
 catch (  Exception e) {
  }
  sketch.cleanup();
  toFront();
}
