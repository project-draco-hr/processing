{
  try {
    boolean success=sketch.exportApplet();
    if (success) {
      File appletFolder=new File(sketch.getFolder(),"applet");
      Base.openFolder(appletFolder);
      statusNotice("Done exporting.");
    }
 else {
    }
  }
 catch (  Exception e) {
    statusError(e);
  }
  toolbar.deactivate(EditorToolbar.EXPORT);
}
