{
  filter="\\p{Alnum}*" + filter.toLowerCase() + "\\p{Alnum}*";
  if (filter.isEmpty()) {
    return true;
  }
  for (  Author author : libInfo.authors) {
    if (author.name.toLowerCase().matches(filter)) {
      return true;
    }
  }
  return libInfo.name.toLowerCase().matches(filter) || libInfo.name.toLowerCase().matches(filter);
}
