{
  SAXParserFactory spf=SAXParserFactory.newInstance();
  spf.setValidating(false);
  try {
    SAXParser sp=spf.newSAXParser();
    InputSource input=new InputSource(new FileReader(xmlFile));
    sp.parse(input,this);
  }
 catch (  IOException e) {
    Base.showWarning("Error reading library list","A error occured while reading the list of available libraries.\n" + "Try restarting the Library Manager.\n",e);
  }
catch (  Exception e) {
    Base.showWarning("Error reading library list","The list of libraries downloaded from Processing.org\n" + "appears to be malformed. You can still install libraries\n" + "manually while we work on fixing this.",e);
    librariesByCategory=null;
    allLibraries=null;
  }
}
