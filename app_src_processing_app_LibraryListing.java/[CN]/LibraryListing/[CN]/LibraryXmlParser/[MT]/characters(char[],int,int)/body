{
  if (doingAuthor) {
    currentAuthor.name=new String(ch,start,length).trim();
    currentLibInfo.authors.add(currentAuthor);
    currentAuthor=null;
    doingAuthor=false;
  }
 else   if (doingDescription) {
    currentLibInfo.description=new String(ch,start,length).trim();
    doingDescription=false;
  }
}
