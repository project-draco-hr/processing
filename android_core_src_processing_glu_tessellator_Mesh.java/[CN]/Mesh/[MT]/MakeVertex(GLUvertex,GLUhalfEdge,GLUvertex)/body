{
  GLUhalfEdge e;
  GLUvertex vPrev;
  GLUvertex vNew=newVertex;
  assert(vNew != null);
  vPrev=vNext.prev;
  vNew.prev=vPrev;
  vPrev.next=vNew;
  vNew.next=vNext;
  vNext.prev=vNew;
  vNew.anEdge=eOrig;
  vNew.data=null;
  e=eOrig;
  do {
    e.Org=vNew;
    e=e.Onext;
  }
 while (e != eOrig);
}
