{
  String currentLine;
  try {
    while ((currentLine=streamReader.readLine()) != null) {
      consumer.message(currentLine);
    }
  }
 catch (  Exception e) {
    if ((PdeBase.platform != PdeBase.LINUX) || (e.getMessage().indexOf("Bad file descriptor") == -1)) {
      System.err.println("PdeMessageSiphon err " + e);
      e.printStackTrace();
    }
  }
}
