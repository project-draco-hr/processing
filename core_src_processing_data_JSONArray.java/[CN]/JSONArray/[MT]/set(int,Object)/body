{
  JSONObject.testValidity(value);
  if (index < 0) {
    throw new RuntimeException("JSONArray[" + index + "] not found.");
  }
  if (index < this.length()) {
    this.myArrayList.set(index,value);
  }
 else {
    while (index != this.length()) {
      this.append(JSONObject.NULL);
    }
    this.append(value);
  }
  return this;
}
