{
  int currentTab=ta.editor.getSketch().getCurrentCodeIndex();
  boolean change=false;
  for (  ColorControlBox box : colorBoxes[currentTab]) {
    if (box.setMouseY(y)) {
      change=true;
    }
  }
  if (colorSelector != null) {
    colorSelector.colorBox.visible=true;
  }
  if (change) {
    repaint();
  }
}
