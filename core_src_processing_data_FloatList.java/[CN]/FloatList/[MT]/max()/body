{
  if (count == 0) {
    throw new ArrayIndexOutOfBoundsException("Cannot use max() on IntList of length 0.");
  }
  if (data.length == 0) {
    return Float.NaN;
  }
  float m=Float.NaN;
  for (int i=0; i < data.length; i++) {
    if (data[i] == data[i]) {
      m=data[i];
      for (int j=i + 1; j < data.length; j++) {
        float d=data[j];
        if (!Float.isNaN(d) && (d > m)) {
          m=data[j];
        }
      }
      break;
    }
  }
  return m;
}
