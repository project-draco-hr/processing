{
  int ii=0;
  if (Float.isNaN(value)) {
    for (int i=0; i < count; i++) {
      if (!Float.isNaN(data[i])) {
        data[ii++]=data[i];
      }
    }
  }
 else {
    for (int i=0; i < count; i++) {
      if (data[i] != value) {
        data[ii++]=data[i];
      }
    }
  }
  if (count == ii) {
    return false;
  }
  count=ii;
  return true;
}
