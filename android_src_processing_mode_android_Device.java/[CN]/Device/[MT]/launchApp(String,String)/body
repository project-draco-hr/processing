{
  if (!isAlive()) {
    return false;
  }
  String[] cmd={"shell","am","start","-e","debug","true","-a","android.intent.action.MAIN","-c","android.intent.category.LAUNCHER","-n",packageName + "/." + className};
  ProcessResult pr=adb(cmd);
  if (Base.DEBUG) {
    System.out.println(pr.toString());
  }
  if (pr.getStdout().contains("android.util.AndroidException")) {
    System.err.println(pr.getStdout());
    return false;
  }
  return pr.succeeded();
}
