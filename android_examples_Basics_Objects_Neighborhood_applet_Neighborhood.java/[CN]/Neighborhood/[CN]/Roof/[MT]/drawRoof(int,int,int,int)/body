{
switch (style) {
case 0:
    beginShape();
  vertex(x,y);
vertex(x + w / 2,y - h / 3);
vertex(x + w,y);
endShape(CLOSE);
break;
case 1:
beginShape();
vertex(x,y);
vertex(x + w / 7,y - h / 4);
vertex(x + w / 2,y - h / 2);
vertex(x + (w - w / 7),y - h / 4);
vertex(x + w,y);
endShape(CLOSE);
break;
case 2:
ellipseMode(CORNER);
arc(x,y - h / 2,w,h,PI,TWO_PI);
line(x,y,x + w,y);
break;
}
}
