{
  Advapi32 advapi32;
  int handle=0;
  boolean ret=false;
  advapi32=Advapi32.INSTANCE;
  handle=openKey(rootKey,parent,WINNT.KEY_READ);
  if (handle != 0) {
    if (advapi32.RegDeleteKey(handle,name) == WINERROR.ERROR_SUCCESS) {
      ret=true;
    }
 else {
      ret=false;
    }
    advapi32.RegCloseKey(handle);
  }
  return (ret);
}
