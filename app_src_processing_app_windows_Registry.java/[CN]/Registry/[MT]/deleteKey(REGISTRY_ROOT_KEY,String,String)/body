{
  boolean ret=false;
  Advapi32 advapi32=Advapi32.INSTANCE;
  HKEY handle=openKey(rootKey,parent,WinNT.KEY_READ);
  if (handle != null) {
    if (advapi32.RegDeleteKey(handle,name) == WinError.ERROR_SUCCESS) {
      ret=true;
    }
 else {
      ret=false;
    }
    advapi32.RegCloseKey(handle);
  }
  return ret;
}
