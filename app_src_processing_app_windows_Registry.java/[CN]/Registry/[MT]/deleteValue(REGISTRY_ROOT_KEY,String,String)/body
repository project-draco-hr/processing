{
  Advapi32 advapi32=Advapi32.INSTANCE;
  boolean ret=true;
  HKEY handle=openKey(rootKey,subKeyName,WinNT.KEY_READ | WinNT.KEY_WRITE);
  if (handle != null) {
    if (advapi32.RegDeleteValue(handle,name) == WinError.ERROR_SUCCESS) {
      ret=true;
    }
    advapi32.RegCloseKey(handle);
  }
  return ret;
}
