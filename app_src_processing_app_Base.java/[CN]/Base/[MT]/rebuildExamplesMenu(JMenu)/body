{
  boolean nativeButBroken=Base.isMacOS() ? Preferences.getBoolean("apple.laf.useScreenMenuBar") : false;
  try {
    if (nativeButBroken) {
      menu.setEnabled(false);
    }
 else {
      menu.removeAll();
      addSketches(menu,examplesFolder,false);
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
