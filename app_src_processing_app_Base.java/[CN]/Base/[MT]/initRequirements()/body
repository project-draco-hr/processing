{
  try {
    Class.forName("com.sun.jdi.VirtualMachine");
  }
 catch (  ClassNotFoundException cnfe) {
    Base.openURL("http://wiki.processing.org/w/Supported_Platforms");
    Base.showError("Missing required files","Processing requires a JRE with tools.jar (or a\n" + "full JDK) installed in (or linked to) a folder\n" + "named ???java??? next to the Processing application.\n"+ "More information can be found on the Wiki.",cnfe);
  }
}
