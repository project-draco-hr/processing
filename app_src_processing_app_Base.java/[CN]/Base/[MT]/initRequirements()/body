{
  try {
    Class.forName("com.sun.jdi.VirtualMachine");
  }
 catch (  ClassNotFoundException cnfe) {
    Base.openURL("http://wiki.processing.org/w/Supported_Platforms");
    Base.showError("Please install JDK 1.6 or later","Processing requires a full JDK (not just a JRE)\n" + "to run. Please install JDK 1.6 or later.\n" + "More information can be found in the reference.",cnfe);
  }
}
