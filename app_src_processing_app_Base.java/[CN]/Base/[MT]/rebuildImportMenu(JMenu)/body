{
  importMenu.removeAll();
  try {
    boolean found=addLibraries(importMenu,new File(getSketchbookPath()));
    if (found)     importMenu.addSeparator();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  try {
    addLibraries(importMenu,librariesFolder);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
