{
  importMenu.removeAll();
  importToLibraryTable=new HashMap<String,File>();
  try {
    boolean found=addLibraries(importMenu,getSketchbookFolder());
    if (found)     importMenu.addSeparator();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  try {
    addLibraries(importMenu,librariesFolder);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
