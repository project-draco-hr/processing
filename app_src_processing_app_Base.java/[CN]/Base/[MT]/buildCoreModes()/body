{
  defaultMode=ModeContribution.getCoreMode(this,"processing.mode.java.JavaMode",getContentFile("modes/java"));
  Mode androidMode=ModeContribution.getCoreMode(this,"processing.mode.android.AndroidMode",getContentFile("modes/android"));
  Mode javaScriptMode=ModeContribution.getCoreMode(this,"processing.mode.javascript.JavaScriptMode",getContentFile("modes/javascript"));
  coreModes=new Mode[]{defaultMode,androidMode,javaScriptMode};
}
