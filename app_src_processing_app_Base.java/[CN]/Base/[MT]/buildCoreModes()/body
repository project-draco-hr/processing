{
  Mode javaMode=ModeContribution.load(this,getContentFile("modes/java"),getDefaultModeIdentifier()).getMode();
  coreModes=new Mode[]{javaMode};
  Mode pdexMode=ModeContribution.load(this,getContentFile("modes/ExperimentalMode"),"processing.mode.experimental.ExperimentalMode").getMode();
  coreModes=new Mode[]{pdexMode,javaMode};
}
