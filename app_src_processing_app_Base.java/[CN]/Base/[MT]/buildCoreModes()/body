{
  Mode javaMode=ModeContribution.load(this,getContentFile("modes/java"),"processing.mode.java.JavaMode").getMode();
  Mode androidMode=ModeContribution.load(this,getContentFile("modes/android"),"processing.mode.android.AndroidMode").getMode();
  Mode javaScriptMode=ModeContribution.load(this,getContentFile("modes/javascript"),"processing.mode.javascript.JavaScriptMode").getMode();
  Mode debugMode=ModeContribution.load(this,getContentFile("modes/java2"),"processing.mode.java2.DebugMode").getMode();
  coreModes=new Mode[]{javaMode,androidMode,javaScriptMode,debugMode};
}
