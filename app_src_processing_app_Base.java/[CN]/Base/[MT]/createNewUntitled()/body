{
  File newbieDir=null;
  String newbieName=null;
  File sketchbookDir=getSketchbookFolder();
  File newbieParentDir=untitledFolder;
  int index=0;
  Calendar cal=Calendar.getInstance();
  int day=cal.get(Calendar.DAY_OF_MONTH);
  int month=cal.get(Calendar.MONTH);
  String purty=months[month] + PApplet.nf(day,2);
  do {
    if (index == 26) {
      if (!breakTime) {
        Base.showWarning("Time for a Break","You've reached the limit for auto naming of new sketches\n" + "for the day. How about going for a walk instead?",null);
        breakTime=true;
      }
 else {
        Base.showWarning("Sunshine","No really, time for some fresh air for you.",null);
      }
      return null;
    }
    newbieName="sketch_" + purty + ((char)('a' + index));
    newbieDir=new File(newbieParentDir,newbieName);
    index++;
  }
 while (newbieDir.exists() || new File(sketchbookDir,newbieName).exists());
  newbieDir.mkdirs();
  File newbieFile=new File(newbieDir,newbieName + ".pde");
  new FileOutputStream(newbieFile);
  return newbieFile.getAbsolutePath();
}
