{
  if (!inputFile.exists()) {
    if (DEBUG)     System.err.println(inputFile + " does not exist.");
    return null;
  }
  String lines[]=PApplet.loadStrings(inputFile);
  if (lines == null) {
    System.err.println("Could not read " + inputFile);
    return null;
  }
  return readSettings(inputFile.toString(),lines);
}
