{
  storeSketches();
  boolean canceled=false;
  for (int i=0; i < editorCount; i++) {
    Editor editor=editors[i];
    if (!handleClose(editor,true)) {
      canceled=true;
      break;
    }
 else {
      storeSketchPath(editor,i);
    }
  }
  for (int i=0; i < editorCount; i++) {
    editors[i].closeRunner();
  }
  if (!canceled) {
    Preferences.save();
    if (PApplet.platform != PConstants.MACOSX) {
      System.exit(0);
    }
  }
  return !canceled;
}
