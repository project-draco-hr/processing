{
  try {
    String path=createNewUntitled();
    if (path != null) {
      Editor editor=handleOpen(path);
      editor.untitled=true;
    }
 else {
    }
  }
 catch (  IOException e) {
    if (activeEditor != null) {
      activeEditor.statusError(e);
    }
 else {
      e.printStackTrace();
    }
  }
}
