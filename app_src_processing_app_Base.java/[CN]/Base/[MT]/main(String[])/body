{
  EventQueue.invokeLater(new Runnable(){
    public void run(){
      try {
        createAndShowGUI(args);
      }
 catch (      Throwable t) {
        Messages.showBadnessTrace("It was not meant to be","A serious problem happened during startup. Please report:\n" + "http://github.com/processing/processing/issues/new",t,true);
      }
    }
  }
);
}
