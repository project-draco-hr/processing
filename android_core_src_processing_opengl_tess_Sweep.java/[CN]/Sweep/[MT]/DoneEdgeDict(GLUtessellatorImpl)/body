{
  ActiveRegion reg;
  int fixedEdges=0;
  while ((reg=(ActiveRegion)Dict.dictKey(Dict.dictMin(tess.dict))) != null) {
    if (!reg.sentinel) {
      assert(reg.fixUpperEdge);
      assert(++fixedEdges == 1);
    }
    assert(reg.windingNumber == 0);
    DeleteRegion(tess,reg);
  }
  Dict.dictDeleteDict(tess.dict);
}
