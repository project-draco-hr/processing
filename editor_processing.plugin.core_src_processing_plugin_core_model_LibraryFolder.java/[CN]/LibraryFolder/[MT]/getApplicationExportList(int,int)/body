{
  String platformName=platformNames[platform];
  if (bits == 32) {
    String[] pieces=exportList.get(platformName + "32");
    if (pieces != null)     return pieces;
  }
 else   if (bits == 64) {
    String[] pieces=exportList.get(platformName + "64");
    if (pieces != null)     return pieces;
  }
  return exportList.get(platformName);
}
