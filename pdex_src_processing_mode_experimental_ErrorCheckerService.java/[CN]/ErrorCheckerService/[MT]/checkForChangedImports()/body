{
  if (programImports.size() != previousImports.size()) {
    loadCompClass=true;
    previousImports=programImports;
  }
 else {
    for (int i=0; i < programImports.size(); i++) {
      if (!programImports.get(i).getImportName().equals(previousImports.get(i).getImportName())) {
        loadCompClass=true;
        previousImports=programImports;
        break;
      }
    }
  }
  if (loadCompClass)   astGenerator.loadJars();
}
