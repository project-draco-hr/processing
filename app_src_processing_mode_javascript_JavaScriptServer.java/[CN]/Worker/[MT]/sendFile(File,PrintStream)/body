{
  InputStream is=null;
  ps.write(EOL);
  if (targ.isDirectory()) {
    listDirectory(targ,ps);
    return;
  }
 else {
    is=new FileInputStream(targ.getAbsolutePath());
  }
  try {
    int n;
    while ((n=is.read(buf)) > 0) {
      ps.write(buf,0,n);
    }
  }
  finally {
    is.close();
  }
}
