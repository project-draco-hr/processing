{
  PriorityQ.PQnode[] n=nodes;
  PriorityQ.PQhandleElem[] h=handles;
  int hMin=n[1].handle;
  Object min=h[hMin].key;
  if (size > 0) {
    n[1].handle=n[size].handle;
    h[n[1].handle].node=1;
    h[hMin].key=null;
    h[hMin].node=freeList;
    freeList=hMin;
    if (--size > 0) {
      FloatDown(1);
    }
  }
  return min;
}
