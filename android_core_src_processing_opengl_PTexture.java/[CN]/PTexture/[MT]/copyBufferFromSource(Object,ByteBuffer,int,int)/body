{
  if (bufferCache == null) {
    bufferCache=new LinkedList<BufferData>();
  }
  if (bufferCache.size() + 1 <= MAX_BUFFER_CACHE_SIZE) {
    bufferCache.add(new BufferData(natRef,byteBuf.asIntBuffer(),w,h));
  }
 else {
    try {
      disposeBufferMethod.invoke(bufferSource,new Object[]{natRef});
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
