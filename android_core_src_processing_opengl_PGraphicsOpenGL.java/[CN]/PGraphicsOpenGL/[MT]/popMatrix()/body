{
  if (hints[DISABLE_TRANSFORM_CACHE]) {
    flush();
  }
  PMatrix3D mat;
  mat=modelviewStack.pop();
  modelview.set(mat);
  mat=modelviewInvStack.pop();
  modelviewInv.set(mat);
  mat=cameraStack.pop();
  camera.set(mat);
  mat=cameraInvStack.pop();
  cameraInv.set(mat);
  calcProjmodelview();
}
