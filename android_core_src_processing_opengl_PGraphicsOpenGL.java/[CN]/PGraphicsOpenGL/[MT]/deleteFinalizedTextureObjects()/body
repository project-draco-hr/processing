{
  Set<GLResource> finalized=new HashSet<GLResource>();
  for (  GLResource res : glTextureObjects.keySet()) {
    if (glTextureObjects.get(res)) {
      finalized.add(res);
      int[] temp={res.id};
      pgl.deleteTextures(1,temp,0);
    }
  }
  for (  GLResource res : finalized) {
    glTextureObjects.remove(res);
  }
}
