{
  flush();
  if (kind == TRIANGLES || kind == QUADS || kind == POLYGON) {
    polyTexShader=null;
    polyColorShader=null;
    polyTexlightShader=null;
    polyLightShader=null;
  }
 else   if (kind == LINES) {
    lineShader=null;
  }
 else   if (kind == POINTS) {
    pointShader=null;
  }
 else {
    PGraphics.showWarning(UNKNOWN_SHADER_KIND_ERROR);
  }
}
