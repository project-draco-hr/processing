{
  beginTex();
  int nInVert=in.lastVertex - in.firstVertex + 1;
  if (fill && 3 <= nInVert) {
    int nInInd=nInVert;
    setRawSize(nInInd);
    int idx=0;
    for (int i=in.firstVertex; i <= in.lastVertex; i++) {
      rawIndices[idx++]=i;
    }
    splitRawIndices();
  }
  endTex();
  tessellateEdges();
}
