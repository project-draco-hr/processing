{
  boolean res=clamp2D();
  if (res) {
    for (int i=in.firstEdge; i <= in.lastEdge; i++) {
      int[] edge=in.edges[i];
      if (edge[2] == EDGE_CLOSE)       continue;
      int i0=edge[0];
      int i1=edge[1];
      res=segmentIsAxisAligned(i0,i1);
      if (!res)       break;
    }
  }
  return res;
}
