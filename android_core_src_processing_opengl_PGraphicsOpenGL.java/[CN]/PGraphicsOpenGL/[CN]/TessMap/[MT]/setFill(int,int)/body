{
  int[] colors=tess.fillColors;
  if (-1 < firstFillIndex) {
    int tessIdx=firstFillIndex + inIdx;
    colors[tessIdx]=fill;
  }
 else {
    int[] indices=fillIndices[inIdx];
    float[] weights=fillWeights[inIdx];
    int fill0=in.colors[inIdx];
    setColorARGB(colors,fill,fill0,indices,weights);
  }
}
