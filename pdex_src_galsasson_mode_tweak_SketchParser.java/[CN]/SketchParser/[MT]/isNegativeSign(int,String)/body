{
  for (int i=pos; i >= 0; i--) {
    char c=code.charAt(i);
    if (c == ' ' || c == '\t') {
      continue;
    }
    if (c == ',' || c == '{' || c == '[' || c == '(' || c == '=' || c == '?' || c == '+' || c == '-' || c == '/' || c == '*' || c == '%' || c == '<' || c == '>' || c == ':' || c == '&' || c == '|' || c == '^' || c == '!' || c == '~') {
      return true;
    }
 else {
      return false;
    }
  }
  return false;
}
