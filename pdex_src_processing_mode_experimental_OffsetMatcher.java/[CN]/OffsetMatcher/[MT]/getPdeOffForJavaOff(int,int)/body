{
  log("PDE :" + pdeCodeLine + "\nJAVA:"+ javaCodeLine);
  if (!matchingNeeded)   return start;
  int ans=getPdeOffForJavaOff(start), end=getPdeOffForJavaOff(start + length - 1);
  log(start + " java start off, pde start off " + ans);
  log((start + length - 1) + " java end off, pde end off " + end);
  log("J: " + javaCodeLine.substring(start,start + length) + "\nP: "+ pdeCodeLine.substring(ans,end + 1));
  return ans;
}
