{
  kind=ELLIPSE;
  family=PRIMITIVE;
  params=new float[4];
  params[0]=getFloatWithUnit(element,"cx");
  params[1]=getFloatWithUnit(element,"cy");
  float rx, ry;
  if (circle) {
    rx=ry=getFloatWithUnit(element,"r");
  }
 else {
    rx=getFloatWithUnit(element,"rx");
    ry=getFloatWithUnit(element,"ry");
  }
  params[0]-=rx;
  params[1]-=ry;
  params[2]=rx * 2;
  params[3]=ry * 2;
}
