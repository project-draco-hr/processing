{
  kind=ELLIPSE;
  family=PRIMITIVE;
  params=new float[4];
  params[0]=element.getFloatAttribute("cx");
  params[1]=element.getFloatAttribute("cy");
  float rx, ry;
  if (circle) {
    rx=ry=element.getFloatAttribute("r");
  }
 else {
    rx=element.getFloatAttribute("rx");
    ry=element.getFloatAttribute("ry");
  }
  params[0]-=rx;
  params[1]-=ry;
  params[2]=rx * 2;
  params[3]=ry * 2;
}
