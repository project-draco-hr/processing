{
  IContextInformation[] result=new IContextInformation[5];
  for (int i=0; i < result.length; i++)   result[i]=new ContextInformation(MessageFormat.format("proposal {0} at position {1}",new Object[]{new Integer(i),new Integer(documentOffset)}),MessageFormat.format("proposal {0} valid from {1} to {2}",new Object[]{new Integer(i),new Integer(documentOffset - 5),new Integer(documentOffset + 5)}));
  return result;
}
