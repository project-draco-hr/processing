{
  final String potentialFileList[]={"reference/index.html","reference/index.htm","documentation/index.html","documentation/index.htm","docs/index.html","docs/index.htm","documentation.html","documentation.htm","reference.html","reference.htm","docs.html","docs.htm","readme.txt"};
  int i=0;
  File potentialRef=new File(folder,potentialFileList[i]);
  while (!potentialRef.exists() && ++i < potentialFileList.length) {
    potentialRef=new File(folder,potentialFileList[i]);
  }
  return potentialRef;
}
