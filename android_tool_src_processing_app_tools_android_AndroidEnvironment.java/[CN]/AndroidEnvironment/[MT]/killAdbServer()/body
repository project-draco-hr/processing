{
  System.err.print("Shutting down any existing adb server...");
  System.err.flush();
  try {
    new ProcessHelper("adb","kill-server").execute();
    System.err.println("OK.");
  }
 catch (  final Exception e) {
    System.err.println("failed.");
    System.err.println();
    e.printStackTrace(System.err);
  }
}
