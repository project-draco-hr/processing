{
  try {
    final BufferedInputStream bis=new BufferedInputStream(input);
    final ByteArrayOutputStream out=new ByteArrayOutputStream();
    int c=bis.read();
    while (c != -1) {
      out.write(c);
      c=bis.read();
    }
    return out.toByteArray();
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
  return null;
}
