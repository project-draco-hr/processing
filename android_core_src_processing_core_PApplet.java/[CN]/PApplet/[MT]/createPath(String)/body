{
  try {
    final File file=new File(path);
    final String parent=file.getParent();
    if (parent != null) {
      final File unit=new File(parent);
      if (!unit.exists()) {
        unit.mkdirs();
      }
    }
  }
 catch (  final SecurityException se) {
    System.err.println("You don't have permissions to create " + path);
  }
}
