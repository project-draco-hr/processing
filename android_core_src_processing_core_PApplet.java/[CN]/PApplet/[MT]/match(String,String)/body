{
  final Pattern p=Pattern.compile(regexp,Pattern.MULTILINE | Pattern.DOTALL);
  final Matcher m=p.matcher(what);
  if (m.find()) {
    final int count=m.groupCount() + 1;
    final String[] groups=new String[count];
    for (int i=0; i < count; i++) {
      groups[i]=m.group(i);
    }
    return groups;
  }
  return null;
}
