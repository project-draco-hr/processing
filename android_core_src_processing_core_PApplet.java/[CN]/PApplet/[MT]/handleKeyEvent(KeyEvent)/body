{
  key=event.getKey();
  keyCode=event.getKeyCode();
switch (event.getAction()) {
case KeyEvent.PRESS:
    keyPressed=true;
  keyPressed(event);
break;
case KeyEvent.RELEASE:
keyPressed=false;
keyReleased(event);
break;
}
handleMethods("keyEvent",new Object[]{event});
if (event.getAction() == KeyEvent.PRESS && event.getKeyCode() == android.view.KeyEvent.KEYCODE_BACK) {
exit();
}
}
