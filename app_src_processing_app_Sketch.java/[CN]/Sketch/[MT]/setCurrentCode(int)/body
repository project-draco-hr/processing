{
  if ((currentIndex == which) && (current != null)) {
    return;
  }
  if (current != null) {
    current.setState(editor.getText(),editor.getSelectionStart(),editor.getSelectionStop(),editor.getScrollPosition());
  }
  current=code[which];
  currentIndex=which;
  editor.setCode(current);
  editor.header.rebuild();
}
