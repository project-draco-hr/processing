{
  SketchCode unhideCode=null;
  for (int i=0; i < hiddenCount; i++) {
    if (hidden[i].getPrettyName().equals(what)) {
      unhideCode=hidden[i];
      for (int j=i; j < hiddenCount - 1; j++) {
        hidden[j]=hidden[j + 1];
      }
      hiddenCount--;
      break;
    }
  }
  if (unhideCode == null) {
    System.err.println("internal error: could find " + what + " to unhide.");
    return;
  }
  if (!unhideCode.getFile().exists()) {
    Base.showMessage("Can't unhide","The file \"" + what + "\" no longer exists.");
    return;
  }
  if (!unhideCode.unhideFile()) {
    Base.showMessage("Can't unhide","The file \"" + what + "\" could not be"+ "renamed and unhidden.");
    return;
  }
  insertCode(unhideCode);
  sortCode();
  setCurrent(unhideCode.getFileName());
  editor.header.repaint();
}
