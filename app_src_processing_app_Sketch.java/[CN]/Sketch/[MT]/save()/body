{
  ensureExistence();
  current.setProgram(editor.getText());
  if (isReadOnly()) {
    Base.showMessage(Language.text("save_file.messages.is_read_only"),Language.text("save_file.messages.is_read_only.description"));
    if (!saveAs())     return false;
  }
  for (  SketchCode sc : code) {
    if (sc.isModified()) {
      sc.save();
    }
  }
  calcModified();
  return true;
}
