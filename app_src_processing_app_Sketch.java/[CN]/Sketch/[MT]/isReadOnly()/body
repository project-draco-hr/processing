{
  String apath=folder.getAbsolutePath();
  if (apath.startsWith(Base.getExamplesPath()) || apath.startsWith(Base.getLibrariesPath())) {
    return true;
  }
 else {
    for (int i=0; i < codeCount; i++) {
      if (code[i].isModified() && code[i].fileReadOnly() && code[i].fileExists()) {
        return true;
      }
    }
  }
  return false;
}
