{
  String apath=folder.getAbsolutePath();
  if (apath.startsWith(Base.examplesPath) || apath.startsWith(Base.librariesPath)) {
    return true;
  }
 else {
    for (int i=0; i < codeCount; i++) {
      if (code[i].modified && !code[i].file.canWrite() && code[i].file.exists()) {
        return true;
      }
    }
  }
  return false;
}
