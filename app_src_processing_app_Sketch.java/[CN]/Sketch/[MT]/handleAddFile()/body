{
  ensureExistence();
  if (isReadOnly()) {
    Base.showMessage("Sketch is Read-Only","Some files are marked \"read-only\", so you'll\n" + "need to re-save the sketch in another location,\n" + "and try again.");
    return;
  }
  String prompt="Select an image or other data file to copy to your sketch";
  FileDialog fd=new FileDialog(editor,prompt,FileDialog.LOAD);
  fd.setVisible(true);
  String directory=fd.getDirectory();
  String filename=fd.getFile();
  if (filename == null)   return;
  File sourceFile=new File(directory,filename);
  boolean result=addFile(sourceFile);
  if (result) {
    editor.statusNotice("One file added to the sketch.");
  }
}
