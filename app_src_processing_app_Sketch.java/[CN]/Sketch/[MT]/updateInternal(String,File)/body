{
  String oldPath=getMainFilePath();
  primaryFile=code[0].getFile();
  name=sketchName;
  folder=sketchFolder;
  codeFolder=new File(folder,"code");
  dataFolder=new File(folder,"data");
  calcModified();
  editor.updateTitle();
  editor.base.rebuildSketchbookMenus();
  editor.base.handleRecentRename(editor,oldPath);
}
