{
  int p=pos + 1;
  while (p < chars.length && Character.isWhitespace(chars[p]))   p++;
  if (p == chars.length)   return false;
  for (int i=0; i < s.length(); i++, p++) {
    if (p == chars.length)     return false;
    if (s.charAt(i) != chars[p])     return false;
  }
  if (p == chars.length)   return false;
  if (Character.isLetter(chars[p]))   return false;
  return true;
}
