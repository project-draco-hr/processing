{
  buf[j]='\0';
  if (j > 0) {
    if (s_flg != 0) {
      if ((tabs > 0) && (buf[0] != '{') && (a_flg == 1)) {
        tabs++;
      }
      p_tabs();
      s_flg=0;
      if ((tabs > 0) && (buf[0] != '{') && (a_flg == 1)) {
        tabs--;
      }
      a_flg=0;
    }
    strOut.append(buf,0,j);
    j=0;
  }
 else {
    if (s_flg != 0) {
      s_flg=0;
      a_flg=0;
    }
  }
}
