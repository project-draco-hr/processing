{
  final boolean save_s_flg=s_flag;
  buf.append(c=next());
  while (true) {
    buf.append(c=next());
    while ((c != '/')) {
      if (c == '\n') {
        lineNumber++;
        writeIndentedComment();
        s_flag=true;
      }
      buf.append(c=next());
    }
    if (buf.length() >= 2 && buf.charAt(buf.length() - 2) == '*') {
      jdoc_flag=false;
      break;
    }
  }
  writeIndentedComment();
  s_flag=save_s_flg;
  jdoc_flag=false;
  return;
}
