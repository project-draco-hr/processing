{
  int save_s_flg;
  save_s_flg=s_flg;
  int done=0;
  c=buf[j++]=getchr();
  while (done == 0) {
    c=buf[j++]=getchr();
    while ((c != '/') && (j < buf.length)) {
      if (c == '\n' || c == '\r') {
        lineNumber++;
        putcoms();
        s_flg=1;
      }
      c=buf[j++]=getchr();
    }
    if (j > 1 && buf[j - 2] == '*') {
      done=1;
      jdoc=0;
    }
  }
  putcoms();
  s_flg=save_s_flg;
  jdoc=0;
  return;
}
