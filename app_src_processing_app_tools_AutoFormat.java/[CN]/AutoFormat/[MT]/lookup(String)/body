{
  char r;
  int l, kk;
  if (j < 1)   return false;
  kk=0;
  while (buf[kk] == ' ')   kk++;
  l=0;
  l=new String(buf).indexOf(keyword);
  if (l < 0 || l != kk) {
    return false;
  }
  r=buf[kk + keyword.length()];
  if (r >= 'a' && r <= 'z')   return false;
  if (r >= 'A' && r <= 'Z')   return false;
  if (r >= '0' && r <= '9')   return false;
  if (r == '_' || r == '&')   return false;
  return true;
}
