{
  super(requestWidth,requestHeight,RGB);
  if (devicePropertyName.equals("")) {
    initGStreamer(parent,requestWidth,requestHeight,capturePlugin,new String[]{indexPropertyName},new int[]{PApplet.parseInt(cameraName)},new String[]{},new String[]{},frameRate + "/1");
  }
 else {
    initGStreamer(parent,requestWidth,requestHeight,capturePlugin,new String[]{},new int[]{},new String[]{devicePropertyName},new String[]{cameraName},frameRate + "/1");
  }
}
