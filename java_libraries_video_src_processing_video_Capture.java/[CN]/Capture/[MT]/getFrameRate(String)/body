{
  String fps="";
  String[] parts=PApplet.split(config,',');
  for (  String part : parts) {
    if (-1 < part.indexOf("fps")) {
      String[] values=PApplet.split(part,'=');
      if (0 < values.length) {
        fps=values[1];
        if (fps.indexOf("/") == -1) {
          fps+="/1";
        }
      }
    }
  }
  return fps;
}
