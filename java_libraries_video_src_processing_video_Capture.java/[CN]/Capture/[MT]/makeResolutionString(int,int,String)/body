{
  String res="size=" + width + "x"+ height;
  String[] parts=fpsStr.split("/");
  if (parts.length == 2) {
    int fpsNumerator=PApplet.parseInt(parts[0]);
    int fpsDenominator=PApplet.parseInt(parts[1]);
    res+=",fps=" + fpsNumerator;
    if (fpsDenominator != 1) {
      res+="/" + fpsDenominator;
    }
  }
  return res;
}
