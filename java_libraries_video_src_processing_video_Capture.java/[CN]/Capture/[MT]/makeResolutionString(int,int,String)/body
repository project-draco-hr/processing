{
  String res="size=" + width + "x"+ height;
  String[] parts=fpsStr.split("/");
  if (parts.length == 2) {
    int fpsDenominator=PApplet.parseInt(parts[0]);
    int fpsNumerator=PApplet.parseInt(parts[1]);
    res+=",fps=" + fpsDenominator;
    if (fpsNumerator != 1) {
      res+="/" + fpsNumerator;
    }
  }
  return res;
}
