{
  single[0]=(byte)b;
  if (currentConsole != null) {
    currentConsole.write(single,0,1,err);
  }
 else {
    write(new byte[]{(byte)b},0,1);
  }
  OutputStream echo=err ? stderrFile : stdoutFile;
  if (echo != null) {
    try {
      echo.write(b);
      echo.flush();
    }
 catch (    IOException e) {
      e.printStackTrace();
      echo=null;
    }
  }
}
