{
  if (currentConsole != null) {
    currentConsole.write(b,offset,length,err);
  }
 else {
    try {
      if (err) {
        systemErr.write(b);
      }
 else {
        systemOut.write(b);
      }
    }
 catch (    IOException e) {
    }
  }
  OutputStream echo=err ? stderrFile : stdoutFile;
  if (echo != null) {
    try {
      echo.write(b,offset,length);
      echo.flush();
    }
 catch (    IOException e) {
      e.printStackTrace();
      echo=null;
    }
  }
}
