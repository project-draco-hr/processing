{
  if (openContour) {
    endContour();
    PGraphics.showWarning("Missing endContour() before endShape()");
  }
  if (workPath != null) {
    if (shape == POLYGON) {
      if (mode == CLOSE) {
        workPath.getElements().add(new ClosePath());
      }
      if (auxPath != null) {
        workPath.getElements().addAll(auxPath.getElements());
      }
    }
  }
  shape=0;
}
