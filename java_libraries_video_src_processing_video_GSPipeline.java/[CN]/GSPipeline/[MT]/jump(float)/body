{
  if (playing) {
    gpipeline.pause();
  }
  boolean res;
  long start=GSVideo.secToNanoLong(where);
  long stop=-1;
  res=gpipeline.seek(1.0,Format.TIME,SeekFlags.FLUSH,SeekType.SET,start,SeekType.SET,stop);
  if (!res) {
    System.err.println("Seek operation failed.");
  }
  if (playing) {
    gpipeline.play();
  }
}
