{
  if (!found)   return;
  String sel=editor.textarea.getSelectedText();
  if (sel.equals(replaceField.getText())) {
    found=false;
    replaceButton.setEnabled(false);
    return;
  }
  editor.textarea.setSelectedText(replaceField.getText());
  editor.sketch.setModified(true);
  replaceButton.setEnabled(false);
}
