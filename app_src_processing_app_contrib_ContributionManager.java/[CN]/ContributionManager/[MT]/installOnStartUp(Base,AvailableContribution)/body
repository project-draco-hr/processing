{
  if (availableContrib.link == null) {
    Base.showWarning(Language.interpolate("contrib.errors.update_on_restart_failed",availableContrib.getName()),Language.text("contrib.unsupported_operating_system"));
    return;
  }
  try {
    URL downloadUrl=new URL(availableContrib.link);
    ContributionManager.downloadAndInstallOnStartup(base,downloadUrl,availableContrib);
  }
 catch (  MalformedURLException e) {
    Base.showWarning(Language.interpolate("contrib.errors.update_on_restart_failed",availableContrib.getName()),Language.text("contrib.errors.malformed_url"),e);
  }
}
