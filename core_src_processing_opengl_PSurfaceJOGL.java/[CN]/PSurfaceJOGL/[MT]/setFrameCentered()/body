{
  int sketchX=displayDevice.getViewportInWindowUnits().getX();
  int sketchY=displayDevice.getViewportInWindowUnits().getY();
  int w=sketchWidth;
  int h=sketchHeight;
  window.setTopLevelPosition(sketchX + screenRect.x + (screenRect.width - w) / 2,sketchY + screenRect.y + (screenRect.height - h) / 2);
}
