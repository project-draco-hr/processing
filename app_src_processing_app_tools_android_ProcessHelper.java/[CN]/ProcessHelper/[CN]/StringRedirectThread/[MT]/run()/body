{
  try {
    String line;
    while ((line=in.readLine()) != null) {
      if (line.trim().length() == 0) {
        continue;
      }
      out.println(line);
      if (tee != null) {
        tee.println(line);
      }
    }
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
 finally {
    latch.countDown();
  }
}
