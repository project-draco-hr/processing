{
  indeterminate=true;
  thread=new Thread(){
    public void run(){
      while (Thread.currentThread() == thread) {
        System.out.println("knight rider");
        repaint();
        try {
          Thread.sleep(1000 / 10);
        }
 catch (        InterruptedException e) {
        }
      }
    }
  }
;
  thread.start();
}
