{
  while (x < 0) {
    x+=width;
  }
  while (x > width - 1) {
    x-=width;
  }
  while (y < 0) {
    y+=height;
  }
  while (y > height - 1) {
    y-=height;
  }
  if (w.getpix(x + 1,y) == black) {
    move(0,1);
  }
 else   if (w.getpix(x,y - 1) != black && w.getpix(x,y + 1) != black) {
    move((int)random(9) - 4,(int)random(9) - 4);
  }
}
