{
  if (glLineVertex != 0) {
    pg.finalizeVertexBufferObject(glLineVertex,context);
  }
  if (glLineColor != 0) {
    pg.finalizeVertexBufferObject(glLineColor,context);
  }
  if (glLineAttrib != 0) {
    pg.finalizeVertexBufferObject(glLineAttrib,context);
  }
  if (glLineIndex != 0) {
    pg.finalizeVertexBufferObject(glLineIndex,context);
  }
}
