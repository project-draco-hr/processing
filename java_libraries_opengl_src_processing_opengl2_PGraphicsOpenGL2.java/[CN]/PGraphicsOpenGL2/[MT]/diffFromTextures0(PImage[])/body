{
  if (1 < numTextures) {
    for (int i=0; i < numTextures; i++) {
      if (textureImages0[i] != images[i]) {
        return true;
      }
    }
    return false;
  }
 else   if (0 < numTextures) {
    return textureImages0[0] != images[0];
  }
 else {
    return textureImages0[0] != null;
  }
}
