{
  char ch;
  while (true) {
    buf.append(ch=next());
    if (ch == '\\') {
      buf.append(next());
      continue;
    }
    if (ch == '\'' || ch == '"') {
      buf.append(cc=next());
      while (!EOF && cc != ch) {
        if (cc == '\\') {
          buf.append(next());
        }
        buf.append(cc=next());
      }
      continue;
    }
    if (ch == '\n') {
      writeIndentedLine();
      a_flg=true;
      continue;
    }
    return ch;
  }
}
