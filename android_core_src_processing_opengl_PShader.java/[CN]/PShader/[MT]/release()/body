{
  if (vertexShader != 0) {
    pg.deleteGLSLVertShaderObject(vertexShader);
    vertexShader=0;
  }
  if (fragmentShader != 0) {
    pg.deleteGLSLFragShaderObject(fragmentShader);
    fragmentShader=0;
  }
  if (programObject != 0) {
    pg.deleteGLSLProgramObject(programObject);
    programObject=0;
  }
}
