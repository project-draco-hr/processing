{
  this.parent=parent;
  pgMain=(PGraphicsOpenGL)parent.g;
  pgl=pgMain.pgl;
  this.vertexURL=null;
  this.fragmentURL=null;
  this.vertexFilename=vertFilename;
  this.fragmentFilename=fragFilename;
  glProgram=0;
  glVertex=0;
  glFragment=0;
  intBuffer=PGL.allocateDirectIntBuffer(1);
  floatBuffer=PGL.allocateDirectFloatBuffer(1);
}
