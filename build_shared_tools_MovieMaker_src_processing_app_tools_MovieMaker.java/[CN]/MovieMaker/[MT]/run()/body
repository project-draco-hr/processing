{
  if (Base.isMacOS()) {
    String classPath=getClass().getProtectionDomain().getCodeSource().getLocation().getPath();
    try {
      Runtime.getRuntime().exec(new String[]{"java","-cp",classPath,"processing.app.tools.MovieMakerFrame"});
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
 else {
  }
}
