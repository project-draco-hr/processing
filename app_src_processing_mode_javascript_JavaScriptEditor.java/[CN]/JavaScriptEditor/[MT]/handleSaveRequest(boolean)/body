{
  if (untitled) {
    return handleSaveAs();
  }
 else   if (immediately) {
    handleSave();
    statusEmpty();
    startServer(getExportFolder());
  }
 else {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        handleSave();
        statusEmpty();
        startServer(getExportFolder());
      }
    }
);
  }
  return true;
}
