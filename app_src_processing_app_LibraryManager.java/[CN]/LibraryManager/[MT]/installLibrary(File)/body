{
  try {
    String libName=guessLibraryName(libFile.getPath());
    File tmpFolder=Base.createTempFolder(libName,"uncompressed");
    unzip(libFile,tmpFolder);
    return installLibraries(Library.list(tmpFolder));
  }
 catch (  IOException e) {
    Base.showError("Trouble creating temporary folder","Could not create a place to store libary's uncompressed contents.\n" + "That's gonna prevent us from continuing.",e);
  }
  return 0;
}
