{
  try {
    String[] paths=url.getFile().split("/");
    if (paths.length != 0) {
      String fileName=paths[paths.length - 1];
      File tmpFolder=Base.createTempFolder(fileName.split("\\.")[0],"library");
      File libFile=new File(tmpFolder,fileName);
      libFile.setWritable(true);
      downloadFile(url,libFile);
    }
  }
 catch (  IOException e) {
    Base.showError("Trouble creating temporary folder","Could not create a place to store libraries being downloaded.\n" + "That's gonna prevent us from continuing.",e);
  }
}
