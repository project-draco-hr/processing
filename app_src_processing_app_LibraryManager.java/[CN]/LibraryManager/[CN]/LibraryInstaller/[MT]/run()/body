{
  File libFile=fileDownloader.getFile();
  if (libFile != null) {
    progressMonitor.startTask("Installing",ProgressMonitor.UNKNOWN);
    ArrayList<Library> info=installLibrary(libFile);
    if (info != null) {
      if (info.isEmpty()) {
        Base.showWarning("Trouble discovering libraries","Maybe it's just us, but it looks like there are no\n" + "libraries in the file we just downloaded.\n",null);
      }
 else {
        libraryPanel.info=info.get(0).info;
      }
    }
    refreshInstalled();
  }
  dialog.pack();
  progressMonitor.finished();
}
