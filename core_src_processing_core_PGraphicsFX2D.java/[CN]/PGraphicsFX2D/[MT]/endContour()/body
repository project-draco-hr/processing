{
  if (!openContour) {
    PGraphics.showWarning("Need to call beginContour() first");
    return;
  }
  if (workPath != null) {
    auxPath.getElements().addAll(workPath.getElements());
    auxPath.getElements().add(new ClosePath());
  }
  Path temp=workPath;
  workPath=auxPath;
  auxPath=temp;
  openContour=false;
}
