{
  try {
    String parent=file.getParent();
    if (parent != null) {
      File unit=new File(parent);
      if (!unit.exists())       unit.mkdirs();
    }
  }
 catch (  SecurityException se) {
    ProcessingLog.logError("You don't have permissions to create " + file.getAbsolutePath(),se);
  }
}
