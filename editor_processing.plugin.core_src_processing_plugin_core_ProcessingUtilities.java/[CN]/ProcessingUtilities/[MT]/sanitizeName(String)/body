{
  char c[]=origName.toCharArray();
  StringBuffer buffer=new StringBuffer();
  if ((c[0] >= '0') && (c[0] <= '9')) {
    buffer.append('_');
  }
  for (int i=0; i < c.length; i++) {
    if (((c[i] >= '0') && (c[i] <= '9')) || ((c[i] >= 'a') && (c[i] <= 'z')) || ((c[i] >= 'A') && (c[i] <= 'Z'))) {
      buffer.append(c[i]);
    }
 else {
      buffer.append('_');
    }
  }
  if (buffer.length() > 63) {
    buffer.setLength(63);
  }
  return buffer.toString();
}
