{
  Pattern p=null;
  if (matchPatterns == null) {
    matchPatterns=new HashMap<String,Pattern>();
  }
 else {
    p=matchPatterns.get(regexp);
  }
  if (p == null) {
    if (matchPatterns.size() == 10) {
      matchPatterns.clear();
    }
    p=Pattern.compile(regexp,Pattern.MULTILINE | Pattern.DOTALL);
    matchPatterns.put(regexp,p);
  }
  return p;
}
