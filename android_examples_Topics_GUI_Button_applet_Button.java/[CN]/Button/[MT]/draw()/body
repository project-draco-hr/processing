{
  update(mouseX,mouseY);
  background(currentColor);
  if (rectOver) {
    fill(rectHighlight);
  }
 else {
    fill(rectColor);
  }
  stroke(255);
  rect(rectX,rectY,rectSize,rectSize);
  if (circleOver) {
    fill(circleHighlight);
  }
 else {
    fill(circleColor);
  }
  stroke(0);
  ellipse(circleX,circleY,circleSize,circleSize);
}
