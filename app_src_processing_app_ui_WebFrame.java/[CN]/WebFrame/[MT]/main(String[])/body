{
  Base.initPlatform();
  final File indexFile=new File("../build/shared/lib/welcome/index.html");
  EventQueue.invokeLater(new Runnable(){
    public void run(){
      new WebFrame(indexFile,400){
        public void handleSubmit(        StringDict dict){
          dict.print();
          handleClose();
        }
        public void handleClose(){
          System.exit(0);
        }
      }
;
    }
  }
);
}
