{
  try {
    if (proj.hasNature("processing.plugin.core.sketchNature"))     launch(createConfiguration(proj),mode);
 else     ProcessingLog.logInfo("Sketch could not be launched. " + "The selected project does not have the required Sketch nature.");
  }
 catch (  CoreException e) {
    ProcessingLog.logError("Launch aborted. CoreException error occured while accessing the project.",e);
  }
}
