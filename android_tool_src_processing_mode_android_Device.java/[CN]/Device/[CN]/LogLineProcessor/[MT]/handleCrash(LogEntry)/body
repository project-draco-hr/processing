{
  final Matcher m=SIG.matcher(entry.message);
  if (m.find()) {
    final int pid=Integer.parseInt(m.group(1));
    final int signal=Integer.parseInt(m.group(2));
    if (activeProcesses.contains(pid)) {
      if (signal == 3) {
        endProc(pid);
        reportStackTrace(entry);
      }
    }
  }
}
