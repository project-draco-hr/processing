{
  if (e.getSource() == noButton) {
    unprompt();
    editor.checkModified2();
  }
 else   if (e.getSource() == yesButton) {
    unprompt();
    editor.handleSave2();
    editor.checkModified2();
  }
 else   if (e.getSource() == cancelButton) {
    if (mode == PROMPT)     unprompt();
 else     if (mode == EDIT)     unedit();
    editor.buttons.clear();
  }
}
