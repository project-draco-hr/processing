{
  for (int i=0; i < count; i++) {
    try {
      methods[i].invoke(objects[i],oargs);
    }
 catch (    Exception e) {
      if (e instanceof InvocationTargetException) {
        InvocationTargetException ite=(InvocationTargetException)e;
        ite.getTargetException().printStackTrace();
      }
 else {
        e.printStackTrace();
      }
    }
  }
}
