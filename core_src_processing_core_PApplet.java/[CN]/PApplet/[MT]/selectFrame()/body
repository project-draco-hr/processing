{
  if (frame != null) {
    selectFrame=frame;
  }
 else   if (selectFrame == null) {
    Component comp=getParent();
    while (comp != null) {
      if (comp instanceof Frame) {
        selectFrame=(Frame)comp;
        break;
      }
      comp=comp.getParent();
    }
    if (selectFrame == null) {
      selectFrame=new Frame();
    }
  }
  return selectFrame;
}
