{
  try {
    String ext=checkExtension(filename);
    if (ext != null) {
      if (ext.equals("csv") || ext.equals("tsv")) {
        if (options == null) {
          options=ext;
        }
 else {
          options=ext + "," + options;
        }
      }
    }
    return new Table(createInput(filename),options);
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
