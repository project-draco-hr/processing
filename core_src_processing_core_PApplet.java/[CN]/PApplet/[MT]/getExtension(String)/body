{
  String extension;
  String lower=filename.toLowerCase();
  int dot=filename.lastIndexOf('.');
  if (dot == -1) {
    extension="unknown";
  }
  extension=lower.substring(dot + 1);
  int question=extension.indexOf('?');
  if (question != -1) {
    extension=extension.substring(0,question);
  }
  return extension;
}
