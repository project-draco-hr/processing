{
  Pattern p=Pattern.compile(regexp,Pattern.MULTILINE | Pattern.DOTALL);
  Matcher m=p.matcher(what);
  if (m.find()) {
    int count=m.groupCount();
    String[] groups=new String[count];
    for (int i=0; i < count; i++) {
      groups[i]=m.group(i + 1);
    }
    return groups;
  }
  return null;
}
