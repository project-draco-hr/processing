{
  try {
    Graphics screen=this.getGraphics();
    if (screen != null) {
      if ((g != null) && (g.image != null)) {
        screen.drawImage(g.image,0,0,null);
      }
      Toolkit.getDefaultToolkit().sync();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    if (g != null) {
      g.dispose();
    }
  }
}
