{
  super.validate();
  newSize.width=getWidth();
  newSize.height=getHeight();
  if (oldSize.equals(newSize)) {
    System.out.println("validate() return " + oldSize);
    return;
  }
 else {
    System.out.println("validate() render old=" + oldSize + " -> new="+ newSize);
    oldSize=newSize;
    render();
  }
}
