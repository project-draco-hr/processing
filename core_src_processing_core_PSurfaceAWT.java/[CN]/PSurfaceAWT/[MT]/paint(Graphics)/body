{
  if (frameCount == 0) {
    return;
  }
  if (!insideDraw && (g != null) && (g.image != null)) {
    if (useStrategy) {
      render();
    }
 else {
      screen.drawImage(g.image,0,0,width,height,null);
    }
  }
 else {
    debug(insideDraw + " " + g+ " "+ ((g != null) ? g.image : "-"));
  }
}
