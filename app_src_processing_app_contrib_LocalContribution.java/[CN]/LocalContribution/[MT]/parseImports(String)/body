{
  List<String> outgoing=new ArrayList<String>();
  if (importsStr != null) {
    String[] listing=PApplet.trim(PApplet.split(importsStr,','));
    for (    String imp : listing) {
      if (imp.endsWith(".*")) {
        imp=imp.substring(0,imp.length() - 2);
      }
      outgoing.add(imp);
    }
  }
  return (outgoing.size() > 0) ? outgoing : null;
}
