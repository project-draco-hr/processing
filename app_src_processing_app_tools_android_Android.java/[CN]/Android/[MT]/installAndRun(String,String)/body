{
  boolean success;
  Build build=getBuilder();
  success=build.createProject();
  if (!success)   return;
  success=build.antBuild("debug");
  if (!success)   return;
  int port=waitUntilReady(device);
  if (port != 0) {
    success=installSketch(device);
    if (!success)     return;
    success=startSketch(device,port);
  }
}
