{
  if (this.ignoreCase) {
    name=name.toUpperCase();
  }
  Object value=this.attributes.get(name);
  if (value == null) {
    return defaultValue;
  }
 else   if (value.equals(trueValue)) {
    return true;
  }
 else   if (value.equals(falseValue)) {
    return false;
  }
 else {
    throw this.invalidValue(name,(String)value);
  }
}
