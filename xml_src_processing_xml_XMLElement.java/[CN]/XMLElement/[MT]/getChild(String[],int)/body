{
  int childCount=getChildCount();
  for (int i=0; i < childCount; i++) {
    XMLElement kid=getChild(i);
    if (kid.getName().equals(items[offset])) {
      if (offset == items.length - 1) {
        return kid;
      }
 else {
        return kid.getChild(items,offset + 1);
      }
    }
  }
  return null;
}
