{
  if (this.ignoreCase) {
    name=name.toUpperCase();
  }
  String value=(String)this.attributes.get(name);
  if (value == null) {
    return defaultValue;
  }
 else {
    try {
      return Integer.parseInt(value);
    }
 catch (    NumberFormatException e) {
      throw this.invalidValue(name,value);
    }
  }
}
