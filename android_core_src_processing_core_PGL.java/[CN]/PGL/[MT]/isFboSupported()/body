{
  String ext=gl.glGetString(GL10.GL_EXTENSIONS);
  if (-1 < ext.indexOf("framebuffer_object") && gl11xp != null) {
    try {
      gl11xp.glCheckFramebufferStatusOES(GL11ExtensionPack.GL_FRAMEBUFFER_OES);
      return true;
    }
 catch (    UnsupportedOperationException e) {
      return false;
    }
  }
  return false;
}
