{
  gl=igl;
  try {
    gl11=(GL11)gl;
  }
 catch (  ClassCastException cce) {
    gl11=null;
  }
  try {
    gl11x=(GL11Ext)gl;
  }
 catch (  ClassCastException cce) {
    gl11x=null;
  }
  try {
    gl11xp=(GL11ExtensionPack)gl;
  }
 catch (  ClassCastException cce) {
    gl11xp=null;
  }
  pg.parent.handleDraw();
}
