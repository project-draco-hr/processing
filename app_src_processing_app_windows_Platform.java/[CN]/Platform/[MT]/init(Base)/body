{
  super.init(base);
  checkAssociations();
  checkPath();
  File f=new File("C:\\recycle-test.txt");
  System.out.println(f.getAbsolutePath());
  java.io.PrintWriter writer=PApplet.createWriter(f);
  writer.println("blah");
  writer.flush();
  writer.close();
  try {
    deleteFile(f);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
