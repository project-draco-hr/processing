{
  super.init(base);
  try {
    String knownCommand=Registry.getStringValue(REGISTRY_ROOT_KEY.CLASSES_ROOT,DOC + "\\shell\\open\\command","");
    if (knownCommand == null) {
      if (Preferences.getBoolean("platform.auto_file_type_associations")) {
        setAssociations();
      }
    }
 else     if (!knownCommand.equals(openCommand)) {
      if (Preferences.getBoolean("platform.auto_file_type_associations")) {
        setAssociations();
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
