{
  checkMinMax("maxIndex");
  float m=Float.NaN;
  int mi=-1;
  for (int i=0; i < count; i++) {
    if (values[i] == values[i]) {
      m=values[i];
      mi=i;
      for (int j=i + 1; j < count; j++) {
        float d=values[j];
        if (!Float.isNaN(d) && (d > m)) {
          m=values[j];
          mi=j;
        }
      }
      break;
    }
  }
  return mi;
}
