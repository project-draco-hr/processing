{
  if (!openShape) {
    PGraphics.showWarning(OUTSIDE_BEGIN_END_ERROR,"beginContour()");
    return;
  }
  if (family == GROUP) {
    PGraphics.showWarning("Cannot begin contour in GROUP shapes");
    return;
  }
  if (openContour) {
    PGraphics.showWarning("Already called beginContour().");
    return;
  }
  openContour=true;
}
