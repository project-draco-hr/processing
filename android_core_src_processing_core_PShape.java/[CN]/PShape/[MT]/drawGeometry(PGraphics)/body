{
  g.beginShape(kind);
  if (style) {
    for (int i=0; i < vertexCount; i++) {
      g.vertex(vertices[i]);
    }
  }
 else {
    for (int i=0; i < vertexCount; i++) {
      float[] vert=vertices[i];
      if (vert[PGraphics.Z] == 0) {
        g.vertex(vert[X],vert[Y]);
      }
 else {
        g.vertex(vert[X],vert[Y],vert[Z]);
      }
    }
  }
  g.endShape();
}
