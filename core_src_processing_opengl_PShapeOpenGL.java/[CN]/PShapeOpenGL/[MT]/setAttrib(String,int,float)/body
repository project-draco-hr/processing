{
  if (openShape) {
    PGraphics.showWarning(INSIDE_BEGIN_END_ERROR,"setNormal()");
    return;
  }
  VertexAttribute attrib=polyAttribs.get(name);
  float[] array=inGeo.fattribs.get(name);
  for (int i=0; i < values.length; i++) {
    array[attrib.size * index + 0]=values[i];
  }
  markForTessellation();
}
