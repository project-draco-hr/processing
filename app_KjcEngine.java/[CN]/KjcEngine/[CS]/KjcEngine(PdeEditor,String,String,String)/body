{
  super(editor);
  this.program=program;
  this.buildPath=buildPath;
  File buildDir=new File(buildPath);
  if (dataPath != null) {
    File dataDir=new File(dataPath);
    if (dataDir.exists()) {
      PdeEditor.copyDir(dataDir,buildDir);
    }
  }
  usingExternal=PdeBase.getBoolean("play.external",false);
}
