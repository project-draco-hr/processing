{
  try {
    parser=ASTParser.newParser(AST.JLS4);
  }
 catch (  Exception e) {
    System.err.println("XQMode initialization failed. " + "Are you running the right version of Processing? ");
    pauseThread();
  }
catch (  Error e) {
    System.err.println("XQMode initialization failed. ");
    e.printStackTrace();
    pauseThread();
  }
}
