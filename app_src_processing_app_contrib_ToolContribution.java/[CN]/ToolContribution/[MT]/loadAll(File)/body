{
  File[] list=ContributionType.TOOL.listCandidates(toolsFolder);
  ArrayList<ToolContribution> outgoing=new ArrayList<ToolContribution>();
  if (list != null) {
    for (    File folder : list) {
      try {
        ToolContribution tc=load(folder);
        if (tc != null) {
          outgoing.add(tc);
        }
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
  return outgoing;
}
