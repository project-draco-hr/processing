{
  this.fullName=fullName;
  if (namespace == null) {
    this.name=fullName;
  }
 else {
    int index=fullName.indexOf(':');
    if (index >= 0) {
      this.name=fullName.substring(index + 1);
    }
 else {
      this.name=fullName;
    }
  }
  this.namespace=namespace;
  this.lineNr=lineNr;
  this.systemID=systemID;
}
