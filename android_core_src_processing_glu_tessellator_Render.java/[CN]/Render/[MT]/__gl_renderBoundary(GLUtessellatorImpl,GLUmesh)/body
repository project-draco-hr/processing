{
  GLUface f;
  GLUhalfEdge e;
  for (f=mesh.fHead.next; f != mesh.fHead; f=f.next) {
    if (f.inside) {
      tess.callBeginOrBeginData(GLES20.GL_LINE_LOOP);
      e=f.anEdge;
      do {
        tess.callVertexOrVertexData(e.Org.data);
        e=e.Lnext;
      }
 while (e != f.anEdge);
      tess.callEndOrEndData();
    }
  }
}
