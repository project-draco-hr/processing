{
  width=w;
  height=h;
  if (sketchRenderer().equals(A2D)) {
    g=new PGraphicsAndroid2D();
  }
 else   if (sketchRenderer().equals(A3D)) {
    g=new PGraphicsAndroid3D();
  }
  g.setSize(w,h);
  g.setParent(PApplet.this);
  g.setPrimary(true);
  init();
  setFocusable(true);
  setFocusableInTouchMode(true);
  requestFocus();
}
