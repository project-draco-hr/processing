{
  internalCloseRunner();
  statusEmpty();
  for (int i=0; i < 10; i++)   System.out.println();
  if (Preferences.getBoolean("console.auto_clear")) {
    console.clear();
  }
  sketch.ensureExistence();
  for (  SketchCode sc : sketch.getCode()) {
    try {
      sc.setProgram(sc.getDocumentText());
    }
 catch (    BadLocationException e) {
    }
  }
}
