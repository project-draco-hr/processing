{
  if (mode == Mode.JAVA) {
    out.println();
    out.println();
  }
 else   if (mode == Mode.ACTIVE) {
    out.println("public class " + className + " extends PApplet {");
    out.println();
  }
 else   if (mode == Mode.STATIC) {
    out.println("public class " + className + " extends PApplet {");
    out.println(indent + "public void setup() {");
  }
}
