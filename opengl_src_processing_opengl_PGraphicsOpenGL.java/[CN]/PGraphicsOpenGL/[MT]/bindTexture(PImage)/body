{
  ImageCache cash=(ImageCache)texture.cache;
  if (cash == null) {
    cash=new ImageCache();
    texture.cache=cash;
    texture.setModified(true);
  }
  if (texture.isModified()) {
    cash.rebind(texture);
    texture.setModified(false);
  }
 else {
    gl.glBindTexture(GL.GL_TEXTURE_2D,cash.tindex);
  }
}
