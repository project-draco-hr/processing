{
  for (int i=0; i < maxcells; i++) {
    int cX=PApplet.parseInt(random(width));
    int cY=PApplet.parseInt(random(height));
    int c;
    float r=random(1);
    if (r < 0.25f)     c=spore1;
 else     if (r < 0.5f)     c=spore2;
 else     if (r < 0.75f)     c=spore3;
 else     c=spore4;
    if (w.getpix(cX,cY) == black) {
      w.setpix(cX,cY,c);
      cells[numcells]=new Cell(cX,cY);
      numcells++;
    }
  }
}
