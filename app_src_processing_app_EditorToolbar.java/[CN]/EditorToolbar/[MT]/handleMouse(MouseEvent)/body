{
  int x=e.getX();
  int y=e.getY();
  if (currentRollover != -1) {
    if ((x > x1[currentRollover]) && (y > y1) && (x < x2[currentRollover])&& (y < y2)) {
      return;
    }
 else {
      setState(currentRollover,INACTIVE,true);
      currentRollover=-1;
    }
  }
  int sel=findSelection(x,y);
  if (sel == -1)   return;
  if (state[sel] != ACTIVE) {
    setState(sel,ROLLOVER,true);
    currentRollover=sel;
  }
}
