{
  if (!isEnabled())   return;
  if (offscreen == null)   return;
  int x=e.getX();
  int y=e.getY();
  if (rollover != null) {
    if (y > 0 && y < getHeight() && x > rollover.left && x < rollover.right) {
      return;
    }
 else {
      if (rollover.state == ROLLOVER) {
        rollover.setState(INACTIVE,true);
      }
      rollover=null;
    }
  }
  checkRollover(x,y);
}
