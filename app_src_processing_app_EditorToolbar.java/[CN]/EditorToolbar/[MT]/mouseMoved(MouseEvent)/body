{
  if (!isEnabled())   return;
  if (offscreen == null)   return;
  int x=e.getX();
  int y=e.getY();
  if (rollover != null) {
    if (y > TOP && y < BOTTOM && x > rollover.left && x < rollover.right) {
      return;
    }
 else {
      rollover.setState(INACTIVE,true);
      rollover=null;
    }
  }
  checkRollover(x,y);
}
