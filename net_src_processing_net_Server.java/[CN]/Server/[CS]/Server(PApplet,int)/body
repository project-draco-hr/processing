{
  this.parent=parent;
  this.port=port;
  try {
    server=new ServerSocket(this.port);
    clients=new Vector();
    thread=new Thread(this);
    thread.start();
    parent.registerDispose(this);
    try {
      serverEventMethod=parent.getClass().getMethod("serverEvent",new Class[]{Server.class,Client.class});
    }
 catch (    Exception e) {
    }
  }
 catch (  IOException e) {
    errorMessage("<init>",e);
  }
}
