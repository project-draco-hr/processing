{
  int index=0;
  int xindex=mult * (width - 1);
  for (int x=0; x < width / 2; x++) {
    for (int y=0; y < height; y++) {
      int i=index + mult * y * width;
      int j=xindex + mult * y * width;
      for (int c=0; c < mult; c++) {
        float temp=floatArray[i];
        floatArray[i]=floatArray[j];
        floatArray[j]=temp;
        i++;
        j++;
      }
    }
    index+=mult;
    xindex-=mult;
  }
}
