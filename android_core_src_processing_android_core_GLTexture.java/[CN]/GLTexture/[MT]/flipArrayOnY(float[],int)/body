{
  int index=0;
  int yindex=mult * (height - 1) * width;
  for (int y=0; y < height / 2; y++) {
    for (int x=0; x < mult * width; x++) {
      float temp=floatArray[index];
      floatArray[index]=floatArray[yindex];
      floatArray[yindex]=temp;
      index++;
      yindex++;
    }
    yindex-=mult * width * 2;
  }
}
