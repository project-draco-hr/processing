{
  if (JavaMode.errorCheckEnabled) {
    LineMarker errorMarker=editor.findError(editor.getTextArea().getCaretLine());
    if (errorMarker != null) {
      if (errorMarker.getType() == LineMarker.WARNING) {
        editor.statusMessage(errorMarker.getProblem().getMessage(),EditorStatus.WARNING);
      }
 else {
        editor.statusMessage(errorMarker.getProblem().getMessage(),EditorStatus.COMPILER_ERROR);
      }
      return;
    }
  }
}
