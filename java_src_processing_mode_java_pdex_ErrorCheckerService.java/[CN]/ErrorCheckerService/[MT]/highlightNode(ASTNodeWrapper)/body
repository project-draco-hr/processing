{
  Messages.log("Highlighting: " + awrap);
  try {
    int pdeoffsets[]=awrap.getPDECodeOffsets(this);
    int javaoffsets[]=awrap.getJavaCodeOffsets(this);
    Messages.log("offsets: " + pdeoffsets[0] + ","+ pdeoffsets[1]+ ","+ javaoffsets[1]+ ","+ javaoffsets[2]);
    scrollToErrorLine(editor,pdeoffsets[0],pdeoffsets[1],javaoffsets[1],javaoffsets[2]);
    return true;
  }
 catch (  Exception e) {
    Messages.loge("Scrolling failed for " + awrap);
  }
  return false;
}
