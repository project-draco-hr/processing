{
  isEnabled=!hasJavaTabs;
  if (isEnabled) {
    notifySketchChanged();
  }
 else {
    preprocessingTask.cancel(false);
    setProblemList(Collections.emptyList(),false);
    if (astGenerator.getGui().showUsageBinding != null) {
      astGenerator.getGui().showUsageWindow.setVisible(false);
    }
  }
}
