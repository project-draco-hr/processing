{
  size(640,240);
  frameRate(30);
  fill(128);
  smooth();
  baseX1=0;
  baseY1=height - 150;
  baseX2=width;
  baseY2=height;
  ellipseX=width / 2;
  directionX=random(0.1f,0.99f);
  directionY=random(0.1f,0.99f);
  float directionVectLength=sqrt(directionX * directionX + directionY * directionY);
  directionX/=directionVectLength;
  directionY/=directionVectLength;
}
