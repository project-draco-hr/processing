{
  try {
    PFont font=new PFont(name,this);
    return font;
  }
 catch (  IOException e) {
    System.err.println("Could not load font " + name);
    System.err.println("Make sure that the font has been copied");
    System.err.println("to the data folder of your sketch.");
    e.printStackTrace();
  }
  return null;
}
