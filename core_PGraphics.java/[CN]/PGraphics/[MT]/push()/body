{
  if (matrixStackDepth + 1 == MATRIX_STACK_DEPTH) {
    message(COMPLAINT,"matrix stack overflow, to much pushmatrix");
    return;
  }
  float cm[]=matrixStack[matrixStackDepth];
  cm[0]=m00;
  cm[1]=m01;
  cm[2]=m02;
  cm[3]=m03;
  cm[4]=m10;
  cm[5]=m11;
  cm[6]=m12;
  cm[7]=m13;
  cm[8]=m20;
  cm[9]=m21;
  cm[10]=m22;
  cm[11]=m23;
  cm[12]=m30;
  cm[13]=m31;
  cm[14]=m32;
  cm[15]=m33;
  matrixStackDepth++;
}
