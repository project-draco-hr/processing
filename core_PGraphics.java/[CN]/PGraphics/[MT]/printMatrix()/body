{
  int big=(int)abs(max(max(abs(m00),abs(m01),abs(m02)),max(abs(m10),abs(m11),abs(m12))));
  int d=1;
  while ((big/=10) != 0)   d++;
  System.out.println(PApplet.nfs(m00,d,4) + " " + PApplet.nfs(m01,d,4)+ " "+ PApplet.nfs(m02,d,4));
  System.out.println(PApplet.nfs(m10,d,4) + " " + PApplet.nfs(m11,d,4)+ " "+ PApplet.nfs(m12,d,4));
  System.out.println();
}
