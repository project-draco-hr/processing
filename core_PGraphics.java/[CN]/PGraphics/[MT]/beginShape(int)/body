{
  shape=true;
  shapeKind=kind;
  shape_index=shape_index + 1;
  if (shape_index == -1) {
    shape_index=0;
  }
  if (z_order == true) {
    vertex_start=vertex_count;
    vertex_end=0;
  }
 else {
    vertex_count=0;
    line.reset();
    lines_count=0;
    triangle.reset();
    triangleCount=0;
  }
  textureImage=null;
  cvertexIndex=0;
  cverticesFlat=true;
  unchangedZ=true;
  strokeChanged=false;
  fillChanged=false;
  normalChanged=false;
}
