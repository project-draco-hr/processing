{
  try {
    IPath guess=proj.getLocation().removeLastSegments(1).append("libraries");
    File folder=new File(guess.toOSString());
    if (folder.isDirectory())     for (    File file : folder.listFiles()) {
      if (file.isDirectory())       if (ProcessingCore.isLibrary(file))       return guess;
    }
  }
 catch (  Exception e) {
    ProcessingLog.logError(e);
  }
  return null;
}
