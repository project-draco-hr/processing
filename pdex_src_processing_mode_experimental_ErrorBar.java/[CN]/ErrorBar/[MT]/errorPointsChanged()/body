{
  if (errorPointsOld.size() != errorPoints.size()) {
    editor.getTextArea().repaint();
    return true;
  }
 else {
    for (int i=0; i < errorPoints.size(); i++) {
      if (errorPoints.get(i).getY() != errorPointsOld.get(i).getY()) {
        editor.getTextArea().repaint();
        return true;
      }
    }
  }
  return false;
}
