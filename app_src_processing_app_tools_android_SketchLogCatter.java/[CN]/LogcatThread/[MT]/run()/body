{
  try {
    String line;
    while ((line=in.readLine()) != null) {
      final Matcher m=pattern.matcher(line);
      if (!m.matches()) {
        continue;
      }
      out.println(m.group(2));
    }
  }
 catch (  final IOException e) {
    e.printStackTrace(System.err);
  }
 finally {
    latch.countDown();
  }
}
