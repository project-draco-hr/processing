{
  if (handleExportCheckModified()) {
    statusNotice("Exporting application...");
    try {
      if (exportApplicationPrompt()) {
        Base.openFolder(sketch.getFolder());
        statusNotice("Done exporting.");
      }
 else {
      }
    }
 catch (    Exception e) {
      statusNotice("Error during export.");
      e.printStackTrace();
    }
  }
}
