{
  if (shapeEnded && matrix != null) {
    if (family == GROUP) {
      updateTessellation();
    }
    boolean res=matrix.invert();
    if (res) {
      if (tessellated) {
        applyMatrixImpl(matrix);
      }
      matrix=null;
    }
 else {
      PGraphics.showWarning("The transformation matrix cannot be inverted");
    }
  }
}
