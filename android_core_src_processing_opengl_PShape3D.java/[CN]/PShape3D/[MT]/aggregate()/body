{
  if (root == this && parent == null) {
    fillIndexOffset=0;
    fillVertexOffset=0;
    fillVertexAbs=0;
    fillVertexRel=0;
    lineIndexOffset=0;
    lineVertexOffset=0;
    lineVertexAbs=0;
    lineVertexRel=0;
    pointIndexOffset=0;
    pointVertexOffset=0;
    pointVertexAbs=0;
    pointVertexRel=0;
    aggregateImpl();
    needBufferInit=true;
  }
}
