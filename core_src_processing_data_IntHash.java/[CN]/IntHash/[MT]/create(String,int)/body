{
  if (count == keys.length) {
    String ktemp[]=new String[count << 1];
    System.arraycopy(keys,0,ktemp,0,count);
    keys=ktemp;
    int vtemp[]=new int[count << 1];
    System.arraycopy(values,0,vtemp,0,count);
    values=vtemp;
  }
  indices.put(what,new Integer(count));
  keys[count]=what;
  values[count]=much;
  count++;
}
