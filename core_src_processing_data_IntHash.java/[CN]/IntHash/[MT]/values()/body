{
  return new Iterable<Integer>(){
    @Override public Iterator<Integer> iterator(){
      return new Iterator<Integer>(){
        int index=-1;
        public void remove(){
          removeIndex(index);
        }
        public Integer next(){
          return value(++index);
        }
        public boolean hasNext(){
          return index + 1 < size();
        }
      }
;
    }
  }
;
}
