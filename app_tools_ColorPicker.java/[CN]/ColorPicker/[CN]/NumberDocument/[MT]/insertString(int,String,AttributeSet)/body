{
  if (str == null)   return;
  char chars[]=str.toCharArray();
  int charCount=0;
  for (int i=0; i < chars.length; i++) {
    if (Character.isDigit(chars[i])) {
      if (charCount != i) {
        chars[charCount]=chars[i];
      }
      charCount++;
    }
  }
  super.insertString(offs,new String(chars,0,charCount),a);
}
