{
  if ((g == null) || (g.pixels == null))   return;
  if ((width != WIDE) || (height != HIGH)) {
    return;
  }
  int index=0;
  for (int j=0; j < 256; j++) {
    for (int i=0; i < 256; i++) {
      g.pixels[index++]=color(hue,i,255 - j);
    }
  }
  stroke((brightness > 50) ? 0 : 255);
  rect(lastX,lastY,9,9);
}
