{
  if (props == null) {
    props=new Properties();
  }
  propsFile=Base.getSettingsFile(LANG_FILE);
  if (!propsFile.exists()) {
    try {
      Base.saveFile("",propsFile);
    }
 catch (    IOException e) {
      e.printStackTrace();
      return false;
    }
  }
  InputStream is=null;
  try {
    is=new FileInputStream(propsFile);
  }
 catch (  Exception e) {
    return false;
  }
  try {
    props.load(is);
  }
 catch (  IOException e) {
    e.printStackTrace();
    return false;
  }
  try {
    is.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
    return false;
  }
  return true;
}
