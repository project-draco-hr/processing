{
  if (props != null) {
    try {
      OutputStream out=new FileOutputStream(propsFile);
      props.store(out,"language preferences");
    }
 catch (    FileNotFoundException e) {
      e.printStackTrace();
      return false;
    }
catch (    IOException e) {
      e.printStackTrace();
      return false;
    }
  }
  return true;
}
