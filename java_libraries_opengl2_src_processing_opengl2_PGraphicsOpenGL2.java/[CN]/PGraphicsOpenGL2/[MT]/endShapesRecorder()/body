{
  if (recordingShape) {
    PShape3D shape=null;
    if (0 < recordedVertices.size()) {
      shape=new PShape3D(parent,recordedVertices.size());
    }
    endShapeRecorderImpl(shape);
    return shape;
  }
 else {
    System.err.println("OPENGL2: Start recording with beginShapesRecorder().");
    return null;
  }
}
