{
  if (recordingShape) {
    System.err.println("OPENGL2: Already recording.");
    return recordedShape;
  }
 else {
    if (USE_GBUFFER) {
      if (gbuffer != null && 0 < gbuffer.vertCount) {
        gbuffer.pre();
        gbuffer.render();
        gbuffer.post();
      }
    }
    recordedShape=new PShape3D(parent);
    beginShapeRecorderImpl();
    return recordedShape;
  }
}
