{
  if (USE_GEO_BUFFER && GEO_BUFFER_ACCUM_ALL && UPDATE_GEO_BUFFER_MATRIX_STACK) {
    geoBuffer.stack.pop();
    if (!UPDATE_GL_MATRIX_STACK)     return;
  }
  gl2f.glPopMatrix();
  if (usingGLMatrixStack) {
    if (matrixMode == PROJECTION) {
      projectionStack.pop();
      projectionUpdated=false;
    }
 else {
      modelviewStack.pop();
      modelviewUpdated=false;
    }
  }
}
