{
  int len=uv.length / 2;
  if (len <= maxTextureUnits) {
    float u, v;
    for (int t=0; t < len; t++) {
      u=uv[t];
      v=uv[2 * t];
      vertexTexture(u,v,t);
    }
    vertex(x,y);
    setTextureData(len);
  }
 else {
    System.err.println("OPENGL2: insufficient texture units.");
  }
}
