{
  if (USE_GEO_BUFFER && GEO_BUFFER_ACCUM_ALL && UPDATE_GEO_BUFFER_MATRIX_STACK) {
    geoBuffer.stack.push();
    return;
  }
  gl2f.glPushMatrix();
  if (usingGLMatrixStack) {
    if (matrixMode == PROJECTION) {
      projectionStack.push();
    }
 else {
      modelviewStack.push();
    }
  }
}
