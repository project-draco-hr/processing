{
  setTextures(textures,tc);
  indicesBuffer.rewind();
  verticesBuffer.rewind();
  colorsBuffer.rewind();
  normalsBuffer.rewind();
  for (int t=0; t < texCount; t++) {
    texcoordsBuffer[t].rewind();
  }
  idxCount=0;
  vertCount=0;
  minVertIndex=100000;
  maxVertIndex=0;
  stack.setIdentity();
}
