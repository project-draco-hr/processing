{
  setBackground(Color.RED);
  if (selected) {
    setForeground(textColorSelected);
    setBackground(bgColorSelected);
  }
 else {
    setForeground(textColor);
    setBackground(bgColor);
  }
  if (column == 0 || value == null) {
    setText("");
  }
 else {
    setText(value.toString());
  }
  return this;
}
