{
  try {
    if (glVertex != 0) {
      pgMain.finalizeGLSLVertShaderObject(glVertex,context.id());
    }
    if (glFragment != 0) {
      pgMain.finalizeGLSLFragShaderObject(glFragment,context.id());
    }
    if (glProgram != 0) {
      pgMain.finalizeGLSLProgramObject(glProgram,context.id());
    }
  }
  finally {
    super.finalize();
  }
}
