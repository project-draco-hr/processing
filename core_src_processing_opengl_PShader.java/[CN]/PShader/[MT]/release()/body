{
  if (glVertex != 0) {
    pgMain.deleteGLSLVertShaderObject(glVertex,context.id());
    glVertex=0;
  }
  if (glFragment != 0) {
    pgMain.deleteGLSLFragShaderObject(glFragment,context.id());
    glFragment=0;
  }
  if (glProgram != 0) {
    pgMain.deleteGLSLProgramObject(glProgram,context.id());
    glProgram=0;
  }
}
