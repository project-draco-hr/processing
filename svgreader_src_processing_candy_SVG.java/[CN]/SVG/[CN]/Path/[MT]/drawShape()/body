{
  parent.beginShape();
  parent.vertex(x[0],y[0]);
  int i=1;
  while (i < count) {
switch (kind[i]) {
case MOVETO:
      parent.breakShape();
    parent.vertex(x[i],y[i]);
  i++;
break;
case LINETO:
parent.vertex(x[i],y[i]);
i++;
break;
case CURVETO:
parent.bezierVertex(x[i],y[i],x[i + 1],y[i + 1],x[i + 2],y[i + 2]);
i+=3;
break;
}
}
parent.endShape(closed ? PConstants.CLOSE : PConstants.OPEN);
}
