{
  program=null;
  if (file.length() != 0) {
    BufferedReader reader=new BufferedReader(new InputStreamReader(new FileInputStream(files[i])));
    StringBuffer buffer=new StringBuffer();
    String line=null;
    while ((line=reader.readLine()) != null) {
      buffer.append(line);
      buffer.append('\n');
    }
    reader.close();
    program=buffer.toString();
  }
 else {
    program="";
  }
}
