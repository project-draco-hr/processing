{
  if (((gray & 0xff000000) == 0) && (gray <= colorModeX)) {
    if (colorRgb255) {
      if (gray > 255)       gray=255;
 else       if (gray < 0)       gray=0;
      return 0xff000000 | (gray << 16) | (gray << 8)| gray;
    }
 else {
      colorCalc(gray);
    }
  }
 else {
    colorCalcARGB(gray,colorModeA);
  }
  return calcColor;
}
