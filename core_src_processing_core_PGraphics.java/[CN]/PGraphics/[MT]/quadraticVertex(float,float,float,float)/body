{
  float[] prev=vertices[vertexCount - 1];
  float prevX=prev[X];
  float prevY=prev[Y];
  float cp1x=prevX + 2.0f / 3.0f * (cpx - prevX);
  float cp1y=prevY + 2.0f / 3.0f * (cpy - prevY);
  float cp2x=cp1x + (x - prevX) / 3.0f;
  float cp2y=cp1y + (y - prevY) / 3.0f;
  bezierVertex(cp1x,cp1y,cp2x,cp2y,x,y);
}
