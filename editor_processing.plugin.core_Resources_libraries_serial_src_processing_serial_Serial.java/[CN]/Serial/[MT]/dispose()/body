{
  try {
    if (input != null)     input.close();
    if (output != null)     output.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  input=null;
  output=null;
  try {
    if (port != null)     port.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  port=null;
}
