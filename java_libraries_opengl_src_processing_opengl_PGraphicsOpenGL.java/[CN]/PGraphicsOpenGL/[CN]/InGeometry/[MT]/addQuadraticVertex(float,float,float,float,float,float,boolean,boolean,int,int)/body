{
  float x1=getLastVertexX();
  float y1=getLastVertexY();
  float z1=getLastVertexZ();
  addBezierVertex(x1 + ((cx - x1) * 2 / 3.0f),y1 + ((cy - y1) * 2 / 3.0f),z1 + ((cz - z1) * 2 / 3.0f),x3 + ((cx - x3) * 2 / 3.0f),y3 + ((cy - y3) * 2 / 3.0f),z3 + ((cz - z3) * 2 / 3.0f),x3,y3,z3,fill,stroke,detail,shape);
}
