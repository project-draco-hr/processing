{
  int nInVert=in.lastVertex - in.firstVertex + 1;
  if (stroke && 1 <= nInVert) {
    int quadCount=nInVert;
    int nvertTot=5 * quadCount;
    int nindTot=12 * quadCount;
    if (is3D()) {
      tessellateSquarePoints3D(nvertTot,nindTot);
    }
 else     if (is2D()) {
      beginNoTex();
      tessellateSquarePoints2D(nvertTot,nindTot);
      endNoTex();
    }
  }
}
