{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer idx : glslPrograms.keySet()) {
    if (glslPrograms.get(idx)) {
      finalized.add(idx);
      int id=idx.intValue();
      gl2x.glDeleteProgram(id);
    }
  }
  for (  Integer idx : finalized) {
    glslPrograms.remove(idx);
  }
}
