{
  if (lineShader == null) {
    lineShader=new PShader(parent);
    lineShader.loadVertexShader(PGraphicsOpenGL.class.getResource("LineShaderVert.glsl"));
    lineShader.loadFragmentShader(PGraphicsOpenGL.class.getResource("LineShaderFrag.glsl"));
    lineShader.setup();
  }
  lineShader.start();
}
