{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer idx : glFrameBuffers.keySet()) {
    if (glFrameBuffers.get(idx)) {
      finalized.add(idx);
      int id=idx.intValue();
      int[] temp={id};
      gl.glDeleteFramebuffers(1,temp,0);
    }
  }
  for (  Integer idx : finalized) {
    glFrameBuffers.remove(idx);
  }
}
