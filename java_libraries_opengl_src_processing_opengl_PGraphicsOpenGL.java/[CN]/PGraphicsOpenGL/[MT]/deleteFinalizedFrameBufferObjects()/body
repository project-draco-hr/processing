{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer id : glFrameBuffers.keySet()) {
    if (glFrameBuffers.get(id)) {
      finalized.add(id);
      int[] temp=new int[1];
      temp[0]=id.intValue();
      pgl.glDeleteFramebuffers(1,temp,0);
    }
  }
  for (  Integer id : finalized) {
    glFrameBuffers.remove(id);
  }
}
