{
  if (pointShader == null) {
    pointShader=new PShader(parent);
    pointShader.loadVertexShader(PGraphicsOpenGL.class.getResource("PointShaderVert.glsl"));
    pointShader.loadFragmentShader(PGraphicsOpenGL.class.getResource("PointShaderFrag.glsl"));
    pointShader.setup();
  }
  pointShader.start();
}
