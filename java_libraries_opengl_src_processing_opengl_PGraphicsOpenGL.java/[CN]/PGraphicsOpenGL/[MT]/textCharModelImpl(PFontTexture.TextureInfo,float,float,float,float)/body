{
  PImage tex=textTex.getTexture(info.texIndex);
  int mode0=textureMode;
  noStroke();
  textureMode(NORMAL);
  beginShape(QUADS);
  texture(tex);
  normal(0,0,1);
  vertex(x0,y0,info.u0,info.v0);
  vertex(x1,y0,info.u1,info.v0);
  vertex(x1,y1,info.u1,info.v1);
  vertex(x0,y1,info.u0,info.v1);
  endShape();
  textureMode(mode0);
}
