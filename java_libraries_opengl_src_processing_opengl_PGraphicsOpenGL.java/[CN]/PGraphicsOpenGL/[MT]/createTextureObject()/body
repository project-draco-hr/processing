{
  deleteFinalizedTextureObjects();
  int[] temp=new int[1];
  gl.glGenTextures(1,temp,0);
  int id=temp[0];
  PApplet.println("Created texture object: " + id);
  if (glTextureObjects.containsKey(id)) {
    showWarning("Adding same texture twice");
  }
 else {
    glTextureObjects.put(id,false);
  }
  return id;
}
