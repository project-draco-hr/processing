{
  if (textureImage0 != null && textureImage == null && flushMode == FLUSH_WHEN_FULL) {
    textureImage=textureImage0;
    flush();
    textureImage=null;
  }
  tessellate(mode);
  if (flushMode == FLUSH_AFTER_SHAPE || (flushMode == FLUSH_WHEN_FULL && tess.isFull()) || (flushMode == FLUSH_WHEN_FULL && drawing2D && textureImage != null && stroke)) {
    flush();
  }
}
