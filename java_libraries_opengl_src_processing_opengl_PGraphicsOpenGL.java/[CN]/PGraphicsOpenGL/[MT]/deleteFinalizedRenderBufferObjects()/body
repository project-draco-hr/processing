{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer id : glRenderBuffers.keySet()) {
    if (glRenderBuffers.get(id)) {
      finalized.add(id);
      int[] temp=new int[1];
      temp[0]=id.intValue();
      pgl.glDeleteRenderbuffers(1,temp,0);
    }
  }
  for (  Integer id : finalized) {
    glRenderBuffers.remove(id);
  }
}
