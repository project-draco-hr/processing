{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer idx : glTextureObjects.keySet()) {
    if (glTextureObjects.get(idx)) {
      finalized.add(idx);
      int id=idx.intValue();
      int[] temp={id};
      gl.glDeleteTextures(1,temp,0);
    }
  }
  for (  Integer idx : finalized) {
    glTextureObjects.remove(idx);
  }
}
