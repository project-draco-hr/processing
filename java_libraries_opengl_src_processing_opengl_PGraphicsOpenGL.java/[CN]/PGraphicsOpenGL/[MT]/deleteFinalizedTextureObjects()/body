{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer id : glTextureObjects.keySet()) {
    if (glTextureObjects.get(id)) {
      finalized.add(id);
      int[] temp=new int[1];
      temp[0]=id.intValue();
      pgl.glDeleteTextures(1,temp,0);
    }
  }
  for (  Integer id : finalized) {
    glTextureObjects.remove(id);
  }
}
