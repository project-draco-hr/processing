{
  float sw=vertices[lines[start][VERTEX1]][SW];
  if (sw > 0) {
    gl.glPointSize(sw);
    gl.glBegin(GL.GL_POINTS);
    for (int i=start; i < stop; i++) {
      float[] a=vertices[points[i][VERTEX1]];
      gl.glColor4f(a[SR],a[SG],a[SB],a[SA]);
      gl.glVertex3f(a[VX],a[VY],a[VZ]);
    }
    gl.glEnd();
  }
}
