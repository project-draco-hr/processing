{
  if (modelview.m00 != 1.0f || modelview.m11 != 1.0f || modelview.m22 != 1.0f || modelview.m01 != 0.0f || modelview.m02 != 0.0f || modelview.m10 != 0.0f || modelview.m12 != 0.0f || modelview.m20 != 0.0f || modelview.m21 != 0.0f || !((camera.m23 - modelview.m23) <= EPSILON && (camera.m23 - modelview.m23) >= -EPSILON)) {
    return false;
  }
 else {
    return true;
  }
}
