{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer id : glVertexBuffers.keySet()) {
    if (glVertexBuffers.get(id)) {
      finalized.add(id);
      int[] temp=new int[1];
      temp[0]=id.intValue();
      pgl.glDeleteBuffers(1,temp,0);
    }
  }
  for (  Integer id : finalized) {
    glVertexBuffers.remove(id);
  }
}
