{
  Set<Integer> finalized=new HashSet<Integer>();
  for (  Integer idx : glslVertexShaders.keySet()) {
    if (glslVertexShaders.get(idx)) {
      finalized.add(idx);
      int id=idx.intValue();
      gl2x.glDeleteShader(id);
    }
  }
  for (  Integer idx : finalized) {
    glslVertexShaders.remove(idx);
  }
}
