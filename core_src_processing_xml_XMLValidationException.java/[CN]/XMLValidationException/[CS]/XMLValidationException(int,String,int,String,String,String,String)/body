{
  super(systemID,lineNr,null,msg + ((elementName == null) ? "" : (", element=" + elementName)) + ((attributeName == null) ? "" : (", attribute=" + attributeName))+ ((attributeValue == null) ? "" : (", value='" + attributeValue + "'")),false);
  this.elementName=elementName;
  this.attributeName=attributeName;
  this.attributeValue=attributeValue;
}
