{
  try {
    String line;
    while ((line=in.readLine()) != null) {
      final Matcher m=interestingLine.matcher(line);
      if (!m.matches()) {
        continue;
      }
      final PrintWriter w=m.group(1).equals("W") ? stderr : stdout;
      w.println("<" + m.group(3) + ">");
    }
  }
 catch (  final IOException e) {
    e.printStackTrace(System.err);
  }
 finally {
    latch.countDown();
  }
}
