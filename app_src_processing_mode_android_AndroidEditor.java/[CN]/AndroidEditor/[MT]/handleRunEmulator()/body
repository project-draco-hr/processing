{
  new Thread(){
    public void run(){
      prepareRun();
      AVD.ensureEclairAVD(amode.getSDK());
      try {
        runSketchOnDevice(Environment.getInstance().getEmulator(),"debug",AndroidEditor.this);
      }
 catch (      final MonitorCanceled ok) {
        sketchStopped();
        statusNotice("Canceled.");
      }
    }
  }
.start();
}
