{
  new Thread(){
    public void run(){
      toolbar.activate(AndroidToolbar.RUN);
      startIndeterminate();
      prepareRun();
      try {
        androidMode.handleRunDevice(sketch,AndroidEditor.this);
      }
 catch (      SketchException e) {
        statusError(e);
      }
catch (      IOException e) {
        statusError(e);
      }
      stopIndeterminate();
    }
  }
.start();
}
