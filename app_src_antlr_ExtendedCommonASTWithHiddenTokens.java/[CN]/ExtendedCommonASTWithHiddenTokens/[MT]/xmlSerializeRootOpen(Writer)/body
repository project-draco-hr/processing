{
  StringBuilder sb=new StringBuilder(100);
  sb.append("<");
  sb.append(getClass().getName() + " ");
  sb.append("hiddenBeforeString=\"" + encode(getHiddenBeforeString()) + "\" text=\""+ encode(getText())+ "\" type=\""+ getType()+ "\" hiddenAfterString=\""+ encode(getHiddenAfterString())+ "\">\n");
  out.write(sb.toString());
}
