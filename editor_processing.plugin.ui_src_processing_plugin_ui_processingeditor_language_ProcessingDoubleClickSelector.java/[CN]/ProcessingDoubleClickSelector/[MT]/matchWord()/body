{
  IDocument doc=fText.getDocument();
  try {
    int pos=fPos;
    char c;
    while (pos >= 0) {
      c=doc.getChar(pos);
      if (!Character.isJavaIdentifierPart(c))       break;
      --pos;
    }
    fStartPos=pos;
    pos=fPos;
    int length=doc.getLength();
    while (pos < length) {
      c=doc.getChar(pos);
      if (!Character.isJavaIdentifierPart(c))       break;
      ++pos;
    }
    fEndPos=pos;
    return true;
  }
 catch (  BadLocationException x) {
  }
  return false;
}
