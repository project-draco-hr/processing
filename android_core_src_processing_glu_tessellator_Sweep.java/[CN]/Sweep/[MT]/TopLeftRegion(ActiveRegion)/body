{
  GLUvertex org=reg.eUp.Org;
  GLUhalfEdge e;
  do {
    reg=RegionAbove(reg);
  }
 while (reg.eUp.Org == org);
  if (reg.fixUpperEdge) {
    e=Mesh.__gl_meshConnect(RegionBelow(reg).eUp.Sym,reg.eUp.Lnext);
    if (e == null)     return null;
    if (!FixUpperEdge(reg,e))     return null;
    reg=RegionAbove(reg);
  }
  return reg;
}
