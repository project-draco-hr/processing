{
  if (applet != null) {
    applet.stop();
    applet=null;
  }
 else   if (process != null) {
    try {
      System.out.println("writing to stop process");
      processOutput.write('s');
      System.out.println("written");
      processOutput.flush();
      System.out.println("flushing");
    }
 catch (    IOException e) {
      close();
    }
    System.out.println("out");
  }
}
