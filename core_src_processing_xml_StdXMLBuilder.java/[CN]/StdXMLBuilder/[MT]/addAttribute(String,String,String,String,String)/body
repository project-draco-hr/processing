{
  String fullName=key;
  if (nsPrefix != null) {
    fullName=nsPrefix + ':' + key;
  }
  XMLElement top=(XMLElement)this.stack.peek();
  if (top.hasAttribute(fullName)) {
    throw new XMLParseException(top.getSystemID(),top.getLine(),"Duplicate attribute: " + key);
  }
  top.set(fullName,value);
}
