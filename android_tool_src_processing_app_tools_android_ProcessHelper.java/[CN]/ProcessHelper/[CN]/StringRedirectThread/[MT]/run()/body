{
  try {
    String line;
    while ((line=reader.readLine()) != null) {
      if (line.trim().length() == 0) {
        continue;
      }
      for (      final PrintWriter out : outs) {
        out.println(line);
      }
    }
  }
 catch (  final IOException e) {
    e.printStackTrace(System.err);
  }
 finally {
    latch.countDown();
  }
}
