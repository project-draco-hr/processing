{
  try {
    if (input != null)     input.close();
    if (output != null)     output.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  thread=null;
  input=null;
  output=null;
  try {
    if (socket != null)     socket.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  socket=null;
}
