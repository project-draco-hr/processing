{
  if (!handleExportCheckModified())   return;
  try {
    boolean success=sketch.exportApplet();
    if (success) {
      File appletFolder=new File(sketch.folder,"applet");
      Base.openFolder(appletFolder);
      message("Done exporting.");
    }
 else {
    }
  }
 catch (  Exception e) {
    error(e);
  }
  buttons.clear();
}
