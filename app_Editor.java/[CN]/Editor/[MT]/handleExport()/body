{
  String what=sketch.isLibrary() ? "Applet" : "Library";
  message("Exporting " + what + "...");
  try {
    boolean success=sketch.isLibrary() ? sketch.exportLibrary() : sketch.exportApplet();
    if (success) {
      message("Done exporting.");
    }
 else {
    }
  }
 catch (  Exception e) {
    message("Error during export.");
    e.printStackTrace();
  }
  buttons.clear();
}
