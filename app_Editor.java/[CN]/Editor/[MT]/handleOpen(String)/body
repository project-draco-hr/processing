{
  buttons.activate(EditorButtons.OPEN);
  SwingUtilities.invokeLater(new Runnable(){
    public void run(){
      String path=ipath;
      if (path == null) {
        path=sketchbook.handleOpen();
        if (path == null)         return;
      }
      doClose();
      handleOpenPath=path;
      checkModified(HANDLE_OPEN);
    }
  }
);
}
