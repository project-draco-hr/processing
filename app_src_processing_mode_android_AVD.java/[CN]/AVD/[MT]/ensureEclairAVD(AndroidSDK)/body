{
  try {
    if (defaultAVD.exists(sdk)) {
      return true;
    }
    if (defaultAVD.badness()) {
      Base.showWarning("Android Error",AVD_CANNOT_LOAD,null);
      return false;
    }
    if (defaultAVD.create(sdk)) {
      return true;
    }
  }
 catch (  final Exception e) {
    Base.showWarning("Android Error",AVD_CREATE_ERROR,e);
  }
  return false;
}
