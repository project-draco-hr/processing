{
switch (c) {
case ';':
    return Language.text("editor.status.missing.semi_colon");
case '[':
  return Language.text("editor.status.missing.open_sq_bracket");
case ']':
return Language.text("editor.status.missing.closing_sq_bracket");
case '(':
return Language.text("editor.status.missing.open_paren");
case ')':
return Language.text("editor.status.missing.close_paren");
case '{':
return Language.text("editor.status.missing.open_curly_bracket");
case '}':
return Language.text("editor.status.missing.closing_curly_bracket");
}
return Language.interpolate("editor.status.missing.default",c);
}
