{
  int spaceCount=0;
  boolean finished=false;
  while ((index != -1) && (!finished)) {
    if ((contents[index] == 10) || (contents[index] == 13)) {
      finished=true;
      index++;
    }
 else {
      index--;
    }
  }
  while ((index < contents.length) && (index >= 0) && (contents[index++] == ' ')) {
    spaceCount++;
  }
  return spaceCount;
}
