{
  try {
    String[] cmd=new String[]{"java","-cp",System.getProperty("java.class.path"),"org.apache.tools.ant.Main",target};
    ProcessHelper helper=new ProcessHelper(tmpFolder,cmd);
    ProcessResult pr=helper.execute();
    if (pr.getResult() != 0) {
      System.err.println(pr.getStderr());
      System.out.println(pr.getStdout());
      antBuildProblems(pr.getStdout());
      return false;
    }
  }
 catch (  InterruptedException e) {
    return false;
  }
catch (  IOException e) {
    e.printStackTrace();
    return false;
  }
  return true;
}
