{
  if (!Base.isMacOS()) {
    if (iconImages == null) {
      iconImages=new ArrayList<Image>();
      final int[] sizes={16,32,48,64,128,256,512};
      for (      int sz : sizes) {
        iconImages.add(Toolkit.getLibImage("icons/pde-" + sz + ".png"));
      }
    }
    window.setIconImages(iconImages);
  }
}
