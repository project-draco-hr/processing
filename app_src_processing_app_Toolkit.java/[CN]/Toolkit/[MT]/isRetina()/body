{
  if (Base.isMacOS()) {
    if (retinaProp == null) {
      Float prop=(Float)awtToolkit.getDesktopProperty("apple.awt.contentScaleFactor");
      if (prop != null) {
        retinaProp=prop == 2;
      }
 else {
        retinaProp=false;
      }
    }
    return retinaProp;
  }
  return false;
}
