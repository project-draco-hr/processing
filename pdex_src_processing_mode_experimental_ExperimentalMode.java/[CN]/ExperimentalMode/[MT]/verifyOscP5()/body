{
  for (  Library l : contribLibraries) {
    if (l.getName().equals("oscP5")) {
      return true;
    }
  }
  Base.showWarning("Tweak Mode","Tweak Mode needs the 'oscP5' library.\n" + "Please install this library by clicking \"Sketch --> Import Library --> Add Library ...\" and choose 'ocsP5'",null);
  return false;
}
