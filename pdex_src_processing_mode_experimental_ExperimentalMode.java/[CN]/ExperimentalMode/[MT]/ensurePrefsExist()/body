{
  if (Preferences.get(prefErrorCheck) == null)   Preferences.setBoolean(prefErrorCheck,errorCheckEnabled);
  if (Preferences.get(prefWarnings) == null)   Preferences.setBoolean(prefWarnings,warningsEnabled);
  if (Preferences.get(prefCodeCompletionEnabled) == null)   Preferences.setBoolean(prefCodeCompletionEnabled,codeCompletionsEnabled);
  if (Preferences.get(prefDebugOP) == null)   Preferences.setBoolean(prefDebugOP,debugOutputEnabled);
  if (Preferences.get(prefErrorLogs) == null)   Preferences.setBoolean(prefErrorLogs,errorLogsEnabled);
}
