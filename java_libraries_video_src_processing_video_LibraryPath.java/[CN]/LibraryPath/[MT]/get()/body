{
  URL url=this.getClass().getResource("LibraryPath.class");
  if (url != null) {
    String path=url.toString().replace("%20"," ");
    int n0=path.indexOf('/');
    int n1=-1;
    if (Platform.isLinux()) {
      return "";
    }
 else {
      n1=path.indexOf("video.jar");
      if (Platform.isWindows()) {
        n0++;
      }
    }
    if ((-1 < n0) && (-1 < n1)) {
      return path.substring(n0,n1);
    }
 else {
      return "";
    }
  }
  return "";
}
