{
  try {
    if (input != null) {
      input.close();
      input=null;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    if (output != null) {
      output.close();
      output=null;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    if (port != null) {
      port.close();
      port=null;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
