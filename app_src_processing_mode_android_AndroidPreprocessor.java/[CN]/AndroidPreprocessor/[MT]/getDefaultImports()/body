{
  final String prefsLine=Preferences.get("android.preproc.imports");
  if (prefsLine != null) {
    return PApplet.splitTokens(prefsLine,", ");
  }
  final String[] androidImports=new String[]{"java.io.*","java.util.*"};
  Preferences.set("android.preproc.imports",PApplet.join(androidImports,","));
  return androidImports;
}
