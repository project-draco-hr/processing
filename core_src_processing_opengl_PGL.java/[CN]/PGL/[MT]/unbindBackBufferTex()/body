{
  if (textureIsBound(GL.GL_TEXTURE_2D,glColorTex[backTex])) {
    if (!texturingIsEnabled(GL.GL_TEXTURE_2D)) {
      enableTexturing(GL.GL_TEXTURE_2D);
      gl.glBindTexture(GL.GL_TEXTURE_2D,0);
      disableTexturing(GL.GL_TEXTURE_2D);
    }
 else {
      gl.glBindTexture(GL.GL_TEXTURE_2D,0);
    }
  }
}
