{
  if (USE_JOGL_FBOLAYER) {
    if (!texturingIsEnabled(TEXTURE_2D)) {
      enableTexturing(TEXTURE_2D);
    }
    bindTexture(TEXTURE_2D,frontTexAttach.getName());
  }
 else {
    if (!texturingIsEnabled(TEXTURE_2D)) {
      enableTexturing(TEXTURE_2D);
    }
    bindTexture(TEXTURE_2D,glColorTex.get(frontTex));
  }
}
