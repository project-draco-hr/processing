{
  if (texture == null) {
    PGraphicsOpenGL g=graphics.get();
    if (g == null)     return null;
    texture=new Texture(g);
    texture.init(g.width,g.height,glColorTex.get(frontTex),TEXTURE_2D,RGBA,fboWidth,fboHeight,NEAREST,NEAREST,CLAMP_TO_EDGE,CLAMP_TO_EDGE);
    texture.invertedY(true);
    texture.colorBuffer(true);
  }
 else {
    texture.glName=glColorTex.get(frontTex);
  }
  return texture;
}
