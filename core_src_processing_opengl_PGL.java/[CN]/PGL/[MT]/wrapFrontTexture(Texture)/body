{
  if (texture == null) {
    texture=new Texture(pg);
    texture.init(pg.width,pg.height,glColorTex.get(frontTex),TEXTURE_2D,RGBA,fboWidth,fboHeight,NEAREST,NEAREST,CLAMP_TO_EDGE,CLAMP_TO_EDGE);
    texture.invertedY(true);
    texture.colorBuffer(true);
  }
 else {
    texture.glName=glColorTex.get(frontTex);
  }
  return texture;
}
