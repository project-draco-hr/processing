{
  if (version >= 130) {
    String[] search=new String[]{"varying","attibute","texture","texMap2D","texMap3D","texMap2DRect","texMapCube"};
    String[] replace=new String[]{"out","in","texMap","texture","texture","texture","texture"};
    String[] vertSrc=preprocessShaderSource(vertSrc0,search,replace,1);
    vertSrc[0]="#version " + version;
    return vertSrc;
  }
  return vertSrc0;
}
