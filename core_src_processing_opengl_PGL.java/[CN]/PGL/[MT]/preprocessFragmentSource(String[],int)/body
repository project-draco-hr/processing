{
  if (version >= 130) {
    String[] search=new String[]{"varying","attibute","texture","texMap2D","texMap3D","texMap2DRect","texMapCube","gl_FragColor"};
    String[] replace=new String[]{"in","in","texMap","texture","texture","texture","texture","fragColor"};
    String[] fragSrc=preprocessShaderSource(fragSrc0,search,replace,2);
    fragSrc[0]="#version " + version;
    fragSrc[1]="out vec4 fragColor;";
    return fragSrc;
  }
  return fragSrc0;
}
