{
switch (index) {
case RUN:
    return shift ? "Run" : "Present";
case STOP:
  return "Stop";
case NEW:
return shift ? "New" : "New Editor Window";
case OPEN:
return shift ? "Open" : "Open in Another Window";
case SAVE:
return "Save";
case EXPORT:
return shift ? "Export Applet" : "Export Application";
}
return null;
}
