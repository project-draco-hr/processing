{
  gl=a3d.gl11;
  if (gl == null) {
    throw new RuntimeException("PShape3D: OpenGL ES 1.1 required");
  }
  if (!PGraphicsAndroid3D.vboSupported) {
    throw new RuntimeException("PShape3D: Vertex Buffer Objects are not available");
  }
  setParameters(params);
  createShape(numVert);
  initChildren();
  updateElement=-1;
  resetBounds();
}
