{
  if (readFromOBJ) {
    recordOBJ(objVertices,objNormal,objTexCoords,objFaces,objMaterials);
    centerAt(0,0,0);
    readFromOBJ=false;
    objVertices=null;
    objNormal=null;
    objTexCoords=null;
    objFaces=null;
    objMaterials=null;
  }
  if (children == null) {
    addDefaultChild();
  }
  super.drawGroup(g);
}
