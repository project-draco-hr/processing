{
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  final LineID other=(LineID)obj;
  if ((this.fileName == null) ? (other.fileName != null) : !this.fileName.equals(other.fileName)) {
    return false;
  }
  if (this.lineIdx != other.lineIdx) {
    return false;
  }
  return true;
}
