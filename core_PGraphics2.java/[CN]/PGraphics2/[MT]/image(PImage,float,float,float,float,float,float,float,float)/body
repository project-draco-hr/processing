{
  if (imageMode == CORNER) {
    x2+=x1;
    y2+=y1;
  }
 else   if ((imageMode == CENTER) || (imageMode == CENTER_RADIUS)) {
    x1-=image.width / 2f;
    y1-=image.height / 2f;
  }
  check_image_cache();
  graphics.drawImage((Image)image.cache,(int)x1,(int)y1,(int)x2,(int)y2,(int)u1,(int)v1,(int)u2,(int)v2,null);
}
