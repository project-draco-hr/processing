{
  if (who.cache == null) {
    who.cache=new ImageCache(who);
    who.updatePixels();
  }
  ImageCache cash=(ImageCache)who.cache;
  if ((tint && !cash.tinted) || (tint && (cash.tintedColor != tintColor)) || (!tint && cash.tinted)) {
    who.updatePixels();
  }
  if (who.modified) {
    cash.update();
    who.modified=false;
  }
  graphics.drawImage(((ImageCache)who.cache).image,(int)x1,(int)y1,(int)x2,(int)y2,u1,v1,u2,v2,null);
}
