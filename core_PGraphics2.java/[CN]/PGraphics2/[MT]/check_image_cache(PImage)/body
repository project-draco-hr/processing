{
  if (who.cache == null) {
    who.cache=new BufferedImage(who.width,who.height,BufferedImage.TYPE_INT_ARGB);
    who.updatePixels();
  }
  if (who.modified) {
    BufferedImage bi=(BufferedImage)who.cache;
    bi.setRGB(who.mx1,who.my1,who.mx2 - who.mx1,who.my2 - who.my1,who.pixels,who.my1 * who.width + who.mx1,who.width);
    who.pixelsUpdated();
  }
}
