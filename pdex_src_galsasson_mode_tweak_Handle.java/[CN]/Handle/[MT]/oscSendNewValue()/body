{
  int index=varIndex;
  try {
    if (type == "int") {
      tweakClient.sendInt(index,newValue.intValue());
    }
 else     if (type == "hex") {
      tweakClient.sendInt(index,newValue.intValue());
    }
 else     if (type == "webcolor") {
      tweakClient.sendInt(index,newValue.intValue());
    }
 else     if (type == "float") {
      tweakClient.sendFloat(index,newValue.floatValue());
    }
  }
 catch (  Exception e) {
    System.out.println("error sending OSC message!");
  }
}
