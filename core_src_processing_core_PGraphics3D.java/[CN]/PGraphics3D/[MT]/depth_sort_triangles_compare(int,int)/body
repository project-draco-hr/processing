{
  if (Float.isNaN(vertices[triangles[a][VERTEX1]][TZ]) || Float.isNaN(vertices[triangles[a][VERTEX2]][TZ]) || Float.isNaN(vertices[triangles[a][VERTEX3]][TZ])|| Float.isNaN(vertices[triangles[b][VERTEX1]][TZ])|| Float.isNaN(vertices[triangles[b][VERTEX2]][TZ])|| Float.isNaN(vertices[triangles[b][VERTEX3]][TZ])) {
    System.err.println("NaN values in triangle");
  }
  return (vertices[triangles[b][VERTEX1]][TZ] + vertices[triangles[b][VERTEX2]][TZ] + vertices[triangles[b][VERTEX3]][TZ]) - (vertices[triangles[a][VERTEX1]][TZ] + vertices[triangles[a][VERTEX2]][TZ] + vertices[triangles[a][VERTEX3]][TZ]);
}
