{
  if (raw != null) {
    raw.colorMode(RGB,1);
    raw.noStroke();
    raw.fill(backgroundR,backgroundG,backgroundB);
    raw.beginShape(TRIANGLES);
    raw.vertex(0,0);
    raw.vertex(width,0);
    raw.vertex(0,height);
    raw.vertex(width,0);
    raw.vertex(width,height);
    raw.vertex(0,height);
    raw.endShape();
  }
}
