{
  if (bias) {
  }
 else {
    float sum=0;
    float bias=0;
    for (int i=0; i < connections.size(); i++) {
      Connection c=(Connection)connections.get(i);
      Neuron from=c.getFrom();
      Neuron to=c.getTo();
      if (to == this) {
        if (from.bias) {
          bias=from.getOutput() * c.getWeight();
        }
 else {
          sum+=from.getOutput() * c.getWeight();
        }
      }
    }
    output=f(bias + sum);
  }
}
