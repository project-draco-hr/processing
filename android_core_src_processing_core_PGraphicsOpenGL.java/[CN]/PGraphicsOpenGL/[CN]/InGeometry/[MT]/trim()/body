{
  if (0 < vertexCount && vertexCount < vertices.length / 3) {
    trimVertices();
    trimColors();
    trimNormals();
    trimTexcoords();
    trimStrokeColors();
    trimStrokeWeights();
    trimAmbient();
    trimSpecular();
    trimEmissive();
    trimShininess();
  }
  if (0 < edgeCount && edgeCount < edges.length) {
    trimEdges();
  }
}
