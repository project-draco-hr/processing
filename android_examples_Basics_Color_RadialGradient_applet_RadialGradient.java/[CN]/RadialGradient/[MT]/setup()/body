{
  size(200,200);
  background(0);
  smooth();
  int columns=4;
  int radius=(width / columns) / 2;
  for (int i=radius; i < width; i+=radius * 2) {
    for (int j=radius; j < height; j+=radius * 2) {
      createGradient(i,j,radius,color(PApplet.parseInt(random(255)),PApplet.parseInt(random(255)),PApplet.parseInt(random(255))),color(PApplet.parseInt(random(255)),PApplet.parseInt(random(255)),PApplet.parseInt(random(255))));
    }
  }
}
