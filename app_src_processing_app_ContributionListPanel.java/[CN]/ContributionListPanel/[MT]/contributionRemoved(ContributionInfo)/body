{
synchronized (contributionPanels) {
    Iterator<ContributionPanel> it=contributionPanels.iterator();
    while (it.hasNext()) {
      ContributionPanel panel=it.next();
      if (panel.info == contributionInfo) {
        rowForEachPanel.remove(panel);
        remove(panel);
        it.remove();
      }
    }
  }
  updateUI();
}
