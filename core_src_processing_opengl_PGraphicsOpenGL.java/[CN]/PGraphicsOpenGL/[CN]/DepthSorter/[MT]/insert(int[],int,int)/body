{
  int index=Arrays.binarySearch(array,0,count,value);
  if (index >= 0)   return false;
  index=-(index + 1);
  if (count - index > 100) {
    System.arraycopy(array,index,array,index + 1,count - index);
  }
 else {
    for (int i=count; i > index; i--) {
      array[i]=array[i - 1];
    }
  }
  array[index]=value;
  return true;
}
