{
  beginTex();
  int nTri=(in.lastVertex - in.firstVertex + 1) / 3;
  if (fill && 1 <= nTri) {
    int nInInd=3 * nTri;
    setRawSize(nInInd);
    int idx=0;
    boolean clamp=clampTriangles();
    for (int i=in.firstVertex; i < in.firstVertex + 3 * nTri; i++) {
      rawIndices[idx++]=i;
    }
    splitRawIndices(clamp);
  }
  endTex();
  tessellateEdges();
}
