{
  beginTex();
  int nTri=(in.lastVertex - in.firstVertex + 1) / 3;
  if (fill && 1 <= nTri) {
    int nInInd=3 * nTri;
    setRawSize(nInInd);
    PApplet.arrayCopy(indices,rawIndices,nInInd);
    boolean clamp=clampTriangles(indices);
    splitRawIndices(clamp);
  }
  endTex();
  tessellateEdges();
}
