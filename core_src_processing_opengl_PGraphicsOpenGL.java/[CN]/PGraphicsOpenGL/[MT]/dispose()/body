{
  super.dispose();
  if (primaryGraphics) {
    pgl.swapBuffers();
  }
  if (asyncPixelReader != null) {
    asyncPixelReader.dispose();
    asyncPixelReader=null;
  }
  deleteSurfaceTextures();
  if (primaryGraphics) {
    deleteDefaultShaders();
  }
 else {
    FrameBuffer ofb=offscreenFramebuffer.get();
    FrameBuffer mfb=multisampleFramebuffer.get();
    if (ofb != null) {
      ofb.dispose();
    }
    if (mfb != null) {
      mfb.dispose();
    }
  }
  pgl.dispose();
}
