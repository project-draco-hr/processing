{
  super.dispose();
  if (primarySurface) {
    pgl.swapBuffers();
  }
  deletePolyBuffers();
  deleteLineBuffers();
  deletePointBuffers();
  deleteSurfaceTextures();
  if (primarySurface) {
    deleteDefaultShaders();
  }
 else {
    if (offscreenFramebuffer != null) {
      offscreenFramebuffer.dispose();
    }
    if (multisampleFramebuffer != null) {
      multisampleFramebuffer.dispose();
    }
  }
  deleteFinalizedGLResources(pgl);
  if (primarySurface) {
    pgl.deleteSurface();
  }
}
