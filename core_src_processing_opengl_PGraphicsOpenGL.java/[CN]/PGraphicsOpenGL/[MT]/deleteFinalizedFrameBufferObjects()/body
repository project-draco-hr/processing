{
  Set<GLResource> finalized=new HashSet<GLResource>();
  for (  GLResource res : glFrameBuffers.keySet()) {
    if (glFrameBuffers.get(res)) {
      finalized.add(res);
      int[] temp={res.id};
      pgl.deleteFramebuffers(1,temp,0);
    }
  }
  for (  GLResource res : finalized) {
    glFrameBuffers.remove(res);
  }
}
