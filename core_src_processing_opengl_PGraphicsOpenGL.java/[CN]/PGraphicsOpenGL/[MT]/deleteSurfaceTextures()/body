{
  if (texture != null) {
    Texture tex=texture.get();
    if (tex != null) {
      tex.dispose();
    }
  }
  if (ptexture != null) {
    Texture ptex=ptexture.get();
    if (ptex != null) {
      ptex.dispose();
    }
  }
  if (filterTexture != null) {
    Texture ftex=filterTexture.get();
    if (ftex != null) {
      ftex.dispose();
    }
  }
}
