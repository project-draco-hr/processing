{
  Set<GLResource> finalized=new HashSet<GLResource>();
  for (  GLResource res : glVertexBuffers.keySet()) {
    if (glVertexBuffers.get(res)) {
      finalized.add(res);
      int[] temp={res.id};
      pgl.deleteBuffers(1,temp,0);
    }
  }
  for (  GLResource res : finalized) {
    glVertexBuffers.remove(res);
  }
}
