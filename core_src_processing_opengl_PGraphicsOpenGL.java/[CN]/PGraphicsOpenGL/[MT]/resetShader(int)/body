{
  flush();
  if (kind == TRIANGLES || kind == QUADS || kind == POLYGON) {
    polyTexShader=null;
    polyFlatShader=null;
    polyFullShader=null;
    polyLightShader=null;
  }
 else   if (kind == LINES) {
    lineShader=null;
  }
 else   if (kind == POINTS) {
    pointShader=null;
  }
 else {
    PGraphics.showWarning("Wrong shader type");
  }
}
