{
  flush();
  if (kind == TRIANGLES || kind == QUADS || kind == POLYGON) {
    textureShader=null;
    colorShader=null;
    texlightShader=null;
    lightShader=null;
  }
 else   if (kind == LINES) {
    lineShader=null;
  }
 else   if (kind == POINTS) {
    pointShader=null;
  }
 else {
    PGraphics.showWarning(UNKNOWN_SHADER_KIND_ERROR);
  }
}
