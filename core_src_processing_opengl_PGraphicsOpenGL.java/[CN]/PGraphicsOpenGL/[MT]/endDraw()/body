{
  report("top endDraw()");
  flush();
  if (!drawing) {
    showWarning("Cannot call endDraw() before beginDraw().");
    return;
  }
  if (primarySurface) {
    pgl.endDraw(clearColorBuffer0);
    pgl.flush();
  }
 else {
    endOffscreenDraw();
  }
  drawing=false;
  if (pgCurrent == pgPrimary) {
    pgCurrent=null;
  }
 else {
    pgCurrent=pgPrimary;
  }
  report("bot endDraw()");
}
