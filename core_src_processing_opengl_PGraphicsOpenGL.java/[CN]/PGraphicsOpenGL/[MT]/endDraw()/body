{
  report("top endDraw()");
  flush();
  if (!drawing) {
    showWarning("Cannot call endDraw() before beginDraw().");
    return;
  }
  if (primarySurface) {
    pgl.endOnscreenDraw(clearColorBuffer0);
    if (!pgl.initialized || parent.frameCount == 0) {
      saveSurfaceToPixels();
      restoreSurface=true;
    }
    pgl.flush();
  }
 else {
    endOffscreenDraw();
  }
  drawing=false;
  if (pgCurrent == pgPrimary) {
    pgCurrent=null;
  }
 else {
    pgCurrent=pgPrimary;
  }
  report("bot endDraw()");
}
