{
  report("top endDraw()");
  if (!drawing) {
    PGraphics.showWarning(NO_BEGIN_DRAW_ERROR);
    return;
  }
  if (!clearCalled && 0 < parent.frameCount) {
    clearEveryFrame=false;
  }
  flush();
  if (PGL.SAVE_SURFACE_TO_PIXELS && (!pgPrimary.initialized || parent.frameCount == 0)) {
    saveSurfaceToPixels();
    restoreSurface=true;
  }
  if (primarySurface) {
    endOnscreenDraw();
  }
 else {
    endOffscreenDraw();
  }
  if (pgCurrent == pgPrimary) {
    pgCurrent=null;
  }
 else {
    pgCurrent=pgPrimary;
  }
  drawing=false;
  report("bot endDraw()");
}
