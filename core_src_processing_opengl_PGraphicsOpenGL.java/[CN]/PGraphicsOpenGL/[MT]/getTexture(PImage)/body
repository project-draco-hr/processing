{
  Texture tex=(Texture)initCache(img);
  if (tex == null)   return null;
  if (img.isModified()) {
    if (img.width != tex.width || img.height != tex.height) {
      tex.init(img.width,img.height);
    }
    updateTexture(img,tex);
  }
  if (tex.hasBuffers()) {
    tex.bufferUpdate();
  }
  return tex;
}
