{
  PShader shader;
  if (lineShader == null) {
    if (defLineShader == null) {
      defLineShader=new PShader(parent,defLineShaderVertURL,defLineShaderFragURL);
    }
    shader=defLineShader;
  }
 else {
    shader=lineShader;
  }
  shader.setRenderer(this);
  shader.loadAttributes();
  shader.loadUniforms();
  return shader;
}
