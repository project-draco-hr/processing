{
  m_curr_dash=0;
  m_curr_dash_start=0.0;
  while (ds > 0.0f) {
    if (ds > m_dashes[m_curr_dash]) {
      ds-=m_dashes[m_curr_dash];
      ++m_curr_dash;
      m_curr_dash_start=0.0;
      if (m_curr_dash >= m_num_dashes)       m_curr_dash=0;
    }
 else {
      m_curr_dash_start=ds;
      ds=0.0f;
    }
  }
}
