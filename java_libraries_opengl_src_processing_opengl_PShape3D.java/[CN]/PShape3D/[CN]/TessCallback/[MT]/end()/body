{
switch (tessType) {
case TRIANGLE_FAN:
    for (int i=1; i < tessCount - 1; i++) {
      addIndex(0);
      addIndex(i);
      addIndex(i + 1);
    }
  break;
case TRIANGLE_STRIP:
for (int i=1; i < tessCount - 1; i++) {
  addIndex(i);
  if (i % 2 == 0) {
    addIndex(i - 1);
    addIndex(i + 1);
  }
 else {
    addIndex(i + 1);
    addIndex(i - 1);
  }
}
break;
case TRIANGLES:
for (int i=0; i < tessCount; i++) {
addIndex(i);
}
break;
}
}
