{
  this.papplet=parent;
  ogl=(PGraphicsOpenGL)parent.g;
  ogl.registerPGLObject(this);
  this.family=family;
  vertexCount=0;
  firstVertex=0;
  lastVertex=0;
  indexCount=0;
  firstIndex=0;
  lastIndex=0;
  Parameters params=new Parameters();
  params.drawMode=TRIANGLES;
  params.updateMode=DYNAMIC;
  setParameters(params);
  root=this;
  parent=null;
}
