{
  if (data == null) {
    System.err.println("null data");
    return;
  }
  if (0 <= i0 && i0 <= i1 && i1 - i0 < inVertexCount) {
    int n=i1 - i0 + 1;
    if (data.length == 3 * n) {
      PApplet.arrayCopy(data,0,inVertices,3 * i0,3 * i1);
    }
 else {
      System.err.println("Wrong array length");
    }
    modified=true;
  }
 else {
    System.err.println("Wrong indexes");
  }
}
