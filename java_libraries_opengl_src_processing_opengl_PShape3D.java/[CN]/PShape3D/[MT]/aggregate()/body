{
  if (root == this && parent == null) {
    aggregateImpl(false);
    initBuffers(vertexCount,indexCount);
    copyOffset=0;
    copyGeometryToRoot();
    if (0 < strokeVertexCount && 0 < strokeIndexCount) {
      initStrokeBuffers(strokeVertexCount,strokeIndexCount);
      strokeCopyOffset=0;
      copyStrokeGeometryToRoot();
    }
  }
}
