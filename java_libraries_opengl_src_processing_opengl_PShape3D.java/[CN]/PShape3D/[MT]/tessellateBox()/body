{
  float w, h, d;
  if (params.length == 1) {
    w=h=d=params[0];
  }
 else {
    w=params[0];
    h=params[1];
    d=params[2];
  }
  in.addBox(w,h,d,fill,fillColor,stroke,strokeColor,strokeWeight,ambientColor,specularColor,emissiveColor,shininess);
  in.initTessMaps();
  tessellator.tessellateQuads();
}
