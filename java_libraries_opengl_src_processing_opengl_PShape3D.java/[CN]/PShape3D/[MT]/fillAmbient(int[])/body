{
  if (family == GROUP) {
    PGraphics.showWarning("GROUP shapes don't have any vertices");
    return ambient;
  }
  updateTessellation();
  if (ambient == null || ambient.length != tess.fillAmbient.length) {
    ambient=new int[tess.fillAmbient.length];
  }
  PApplet.arrayCopy(tess.fillAmbient,ambient);
  return ambient;
}
