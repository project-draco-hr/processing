{
  float x=0, y=0, z=0;
  if (params.length == 2) {
    x=params[0];
    y=params[1];
    z=0;
  }
 else   if (params.length == 3) {
    x=params[0];
    y=params[1];
    z=params[2];
  }
  in.setColors(fillColor,strokeColor,strokeWeight,ambientColor,specularColor,emissiveColor,shininess);
  in.setNormal(normalX,normalY,normalZ);
  in.addPoint(x,y,z,fill,stroke);
  in.initTessMaps();
  tessellator.tessellatePoints();
}
