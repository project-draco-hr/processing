{
  if (family == GROUP) {
    for (int i=0; i < childCount; i++) {
      PShape3D child=(PShape3D)children[i];
      child.copyStrokeGeometryToRoot();
    }
  }
 else {
    root.copyStrokeGeometry(root.strokeCopyOffset,strokeVertexCount,strokeVertices,strokeColors,strokeOffsets,strokeIndices);
    root.strokeCopyOffset+=strokeVertexCount;
  }
}
