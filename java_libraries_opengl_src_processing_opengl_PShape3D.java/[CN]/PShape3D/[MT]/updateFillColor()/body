{
  if (!shapeEnded || tess.fillVertexCount == 0 || texture != null) {
    return;
  }
  updateTesselation();
  int size=tess.fillVertexCount;
  float[] colors=tess.fillColors;
  int index;
  for (int i=0; i < size; i++) {
    index=4 * i;
    colors[index++]=fillR;
    colors[index++]=fillG;
    colors[index++]=fillB;
    colors[index]=fillA;
  }
  modifiedFillColors=true;
  modified();
}
