{
  if (textures != null) {
    boolean childHasIt=false;
    for (int i=0; i < childCount; i++) {
      PShape3D child=(PShape3D)children[i];
      if (child.hasTexture(tex)) {
        childHasIt=true;
        break;
      }
    }
    if (!childHasIt) {
      textures.remove(tex);
      if (textures.size() == 0) {
        textures=null;
      }
    }
  }
}
