{
  if (family == GROUP) {
  }
 else {
    tess.center(cx,cy);
    modified=true;
    if (0 < tess.fillVertexCount) {
      modifiedFillVertices=true;
    }
    if (0 < tess.lineVertexCount) {
      modifiedLineVertices=true;
      modifiedLineAttributes=true;
    }
    if (0 < tess.pointVertexCount) {
      modifiedPointVertices=true;
    }
  }
}
