{
  if (shapeEnded) {
    updateTesselation();
    if (family == GROUP) {
      PVector center=new PVector();
      int count=updateCenter(center,0);
      center.x/=count;
      center.y/=count;
      float tx=cx - center.x;
      float ty=cy - center.y;
      childHasMatrix();
      applyMatrix=true;
      super.translate(tx,ty);
    }
 else {
      PVector vec=new PVector();
      int count=tess.getCenter(vec);
      vec.x/=count;
      vec.y/=count;
      float tx=cx - vec.x;
      float ty=cy - vec.y;
      translate(tx,ty);
    }
  }
}
