{
  if (family == GROUP) {
    PGraphics.showWarning("GROUP shapes don't have any vertices");
    return null;
  }
  float[] data=new float[VERTEX_FIELD_COUNT];
  data[X]=inGeo.vertices[3 * index + 0];
  data[Y]=inGeo.vertices[3 * index + 1];
  data[Z]=inGeo.vertices[3 * index + 2];
  return data;
}
