{
  if (family == GROUP) {
    for (int i=0; i < childCount; i++) {
      PShape3D child=(PShape3D)children[i];
      child.texture(tex);
    }
  }
 else {
    if (texture != null && parent != null) {
      ((PShape3D)parent).removeTexture(texture);
    }
    texture=tex;
    if (parent != null) {
      ((PShape3D)parent).addTexture(texture);
    }
  }
}
