{
  if (family == GROUP) {
    for (int i=0; i < childCount; i++) {
      PShape3D child=(PShape3D)children[i];
      count+=child.getVertexSum(sum,count);
    }
  }
 else {
    if (tessellated) {
      count+=tess.getVertexSum(sum);
    }
 else {
      count+=in.getVertexSum(sum);
    }
  }
  return count;
}
