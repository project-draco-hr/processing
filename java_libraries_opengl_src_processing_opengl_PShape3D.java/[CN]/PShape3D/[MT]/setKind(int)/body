{
  this.kind=kind;
  if (family == GEOMETRY || family == PRIMITIVE || family == PATH) {
    in=ogl.newInGeometry();
    tess=ogl.newTessGeometry();
    if (family == PRIMITIVE) {
      if (kind == BOX) {
        params=new float[1];
        params[0]=0;
      }
 else       if (kind == SPHERE) {
        params=new float[3];
        params[0]=0;
        params[1]=20;
        params[2]=20;
      }
    }
  }
}
