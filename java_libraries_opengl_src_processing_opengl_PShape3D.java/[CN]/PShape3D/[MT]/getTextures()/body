{
  HashSet<PImage> texSet=new HashSet<PImage>();
  if (family == GROUP) {
    HashSet<PImage> childSet=null;
    for (int i=0; i < childCount; i++) {
      PShape3D child=(PShape3D)children[i];
      childSet=child.getTextures();
      texSet.addAll(childSet);
    }
  }
 else {
    if (!is3D) {
      texSet.add(texture);
    }
 else {
      texSet.add(texture);
    }
  }
  return texSet;
}
