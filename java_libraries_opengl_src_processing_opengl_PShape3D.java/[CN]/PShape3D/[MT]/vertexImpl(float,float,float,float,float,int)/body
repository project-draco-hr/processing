{
  if (family != GEOMETRY || family != PATH) {
    System.err.println("Cannot add vertices to GROUP of PRIMITIVE shape");
    return;
  }
  currentVertex[0]=x;
  currentVertex[1]=y;
  currentVertex[2]=z;
  currentTexcoord[0]=u;
  currentTexcoord[1]=v;
  int code;
  if (addBreak) {
    code=BREAK;
    addBreak=false;
  }
 else {
    code=type;
  }
  in.addVertex(currentVertex,currentColor,currentNormal,currentTexcoord,currentStroke,code);
  modified=true;
}
