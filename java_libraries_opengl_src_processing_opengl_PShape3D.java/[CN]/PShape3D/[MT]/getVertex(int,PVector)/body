{
  if (family == GROUP) {
    PGraphics.showWarning("GROUP shapes don't have any vertices");
    return null;
  }
  updateTessellation();
  if (vec == null) {
    vec=new PVector();
  }
  vec.x=in.vertices[3 * index + 0];
  vec.y=in.vertices[3 * index + 1];
  vec.z=in.vertices[3 * index + 2];
  return vec;
}
