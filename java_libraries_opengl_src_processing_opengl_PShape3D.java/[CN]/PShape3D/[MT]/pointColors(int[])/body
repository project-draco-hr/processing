{
  if (family == GROUP) {
    PGraphics.showWarning("GROUP shapes don't have any vertices");
    return colors;
  }
  updateTessellation();
  if (colors == null || colors.length != tess.pointColors.length) {
    colors=new int[tess.pointColors.length];
  }
  PApplet.arrayCopy(tess.pointColors,colors);
  return colors;
}
