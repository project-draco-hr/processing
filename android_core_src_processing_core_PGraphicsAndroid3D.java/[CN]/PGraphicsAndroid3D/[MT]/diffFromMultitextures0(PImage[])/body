{
  if (1 < numMultitextures) {
    for (int i=0; i < numMultitextures; i++) {
      if (multitextureImages0[i] != images[i]) {
        return true;
      }
    }
    return false;
  }
 else   if (0 < numMultitextures) {
    return multitextureImages0[0] != images[0];
  }
 else {
    return multitextureImages0[0] != null;
  }
}
