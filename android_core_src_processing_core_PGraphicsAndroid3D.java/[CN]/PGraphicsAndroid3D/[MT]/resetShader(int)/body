{
  if (kind == FILL_SHADER_SIMPLE) {
    if (defFillShaderSimple == null) {
      defFillShaderSimple=new FillShaderSimple(parent,PGraphicsAndroid3D.class.getResource("FillShaderVertSimple.glsl").toString(),PGraphicsAndroid3D.class.getResource("FillShaderFragNoTex.glsl").toString());
    }
    fillShaderSimple=defFillShaderSimple;
  }
 else   if (kind == FILL_SHADER_LIT) {
    if (defFillShaderLit == null) {
      defFillShaderLit=new FillShaderLit(parent,PGraphicsAndroid3D.class.getResource("FillShaderVertLit.glsl").toString(),PGraphicsAndroid3D.class.getResource("FillShaderFragNoTex.glsl").toString());
    }
    fillShaderLit=defFillShaderLit;
  }
 else   if (kind == FILL_SHADER_TEX) {
    if (defFillShaderTex == null) {
      defFillShaderTex=new FillShaderTex(parent,PGraphicsAndroid3D.class.getResource("FillShaderVertTex.glsl").toString(),PGraphicsAndroid3D.class.getResource("FillShaderFragTex.glsl").toString());
    }
    fillShaderTex=defFillShaderTex;
  }
 else   if (kind == FILL_SHADER_FULL) {
    if (defFillShaderFull == null) {
      defFillShaderFull=new FillShaderFull(parent,PGraphicsAndroid3D.class.getResource("FillShaderVertFull.glsl").toString(),PGraphicsAndroid3D.class.getResource("FillShaderFragTex.glsl").toString());
    }
    fillShaderFull=defFillShaderFull;
  }
 else   if (kind == LINE_SHADER) {
    if (defLineShader == null) {
      defLineShader=new LineShader(parent,PGraphicsAndroid3D.class.getResource("LineShaderVert.glsl").toString(),PGraphicsAndroid3D.class.getResource("LineShaderFrag.glsl").toString());
    }
    lineShader=defLineShader;
  }
 else   if (kind == POINT_SHADER) {
    if (defPointShader == null) {
      defPointShader=new PointShader(parent,PGraphicsAndroid3D.class.getResource("PointShaderVert.glsl").toString(),PGraphicsAndroid3D.class.getResource("PointShaderFrag.glsl").toString());
    }
    pointShader=defPointShader;
  }
 else {
    PGraphics.showWarning("Wrong shader type");
  }
}
