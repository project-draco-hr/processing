{
  gl11.glActiveTexture(GL11.GL_TEXTURE0);
  gl11.glTexEnvi(GL11.GL_TEXTURE_ENV,GL11.GL_TEXTURE_ENV_MODE,GL11.GL_MODULATE);
  gl11.glActiveTexture(GL11.GL_TEXTURE1);
  gl11.glTexEnvi(GL11.GL_TEXTURE_ENV,GL11.GL_TEXTURE_ENV_MODE,GL11.GL_MODULATE);
  if (blend) {
    blend(blendMode);
  }
 else {
    noBlend();
  }
}
