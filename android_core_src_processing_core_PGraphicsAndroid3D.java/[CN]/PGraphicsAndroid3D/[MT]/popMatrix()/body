{
  if (USE_GBUFFER && GBUFFER_MERGE_ALL && GBUFFER_UPDATE_STACK) {
    gbuffer.stack.pop();
    return;
  }
  gl.glPopMatrix();
  if (usingGLMatrixStack) {
    if (matrixMode == PROJECTION) {
      projectionStack.pop();
      projectionUpdated=false;
    }
 else {
      modelviewStack.pop();
      modelviewUpdated=false;
    }
  }
}
