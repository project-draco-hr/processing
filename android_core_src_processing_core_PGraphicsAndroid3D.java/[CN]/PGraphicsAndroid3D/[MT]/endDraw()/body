{
  if (!clear) {
    if (fboSupported) {
      if (drawFramebuffer != null) {
        popFramebuffer();
        gl.glClearColor(0,0,0,0);
        gl.glClear(GL10.GL_COLOR_BUFFER_BIT | GL10.GL_DEPTH_BUFFER_BIT);
        renderDrawTexture(drawIndex);
        swapDrawIndex();
      }
    }
 else {
      if (screenTexID[0] != 0) {
        copyFrameToScreenTexture();
      }
    }
  }
  gl.glFlush();
  report("top endDraw()");
}
