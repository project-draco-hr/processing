{
  if (flushMode == FLUSH_WHEN_FULL && hints[DISABLE_TEXTURE_CACHE] && textureImage0 != null && textureImage == null) {
    textureImage=textureImage0;
    flush();
    textureImage=null;
  }
  tessellate(mode);
  if (flushMode == FLUSH_CONTINUOUSLY || (flushMode == FLUSH_WHEN_FULL && tessGeo.isFull())) {
    flush();
  }
}
