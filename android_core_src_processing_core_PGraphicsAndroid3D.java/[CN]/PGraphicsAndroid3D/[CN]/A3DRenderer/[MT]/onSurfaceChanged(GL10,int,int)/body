{
  gl=igl;
  try {
    gl11=(GL11)gl;
  }
 catch (  ClassCastException cce) {
    gl11=null;
  }
  try {
    gl11x=(GL11Ext)gl;
  }
 catch (  ClassCastException cce) {
    gl11x=null;
  }
  try {
    gl11xp=(GL11ExtensionPack)gl;
    int[] fbo={0};
    gl11xp.glGenFramebuffersOES(1,fbo,0);
    System.out.println("FBO id: " + fbo[0]);
  }
 catch (  ClassCastException cce) {
    gl11xp=null;
  }
  setSize(iwidth,iheight);
  gl=null;
  gl11=null;
  gl11x=null;
  gl11xp=null;
}
