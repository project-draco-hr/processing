{
  this.editor=editor;
  if (panel == null) {
    setLayout(editor,activateErrorPanel,isLoading);
  }
  contributionListPanel.setVisible(!isLoading);
  loaderLabel.setVisible(isLoading);
  errorPanel.setVisible(activateErrorPanel);
  panel.validate();
  panel.repaint();
  panel.setVisible(true);
}
