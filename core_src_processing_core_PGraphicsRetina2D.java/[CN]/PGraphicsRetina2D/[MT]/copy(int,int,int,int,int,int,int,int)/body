{
  if (hints[ENABLE_RETINA_PIXELS]) {
    super.copy(sx,sy,sw,sh,dx,dy,dw,dh);
  }
 else {
    if ((sw != dw) || (sh != dh)) {
      g2.drawImage(image,dx * 2,dy * 2,(dx + dw) * 2,(dy + dh) * 2,sx * 2,sy * 2,(sx + sw) * 2,(sy + sh) * 2,null);
    }
 else {
      dx=dx - sx;
      dy=dy - sy;
      g2.copyArea(sx * 2,sy * 2,sw * 2,sh * 2,dx * 2,dy * 2);
    }
  }
}
