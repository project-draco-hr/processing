{
  loadPixels();
  PImage outgoing;
  if (hints[ENABLE_RETINA_PIXELS]) {
    outgoing=retina.get();
    outgoing.parent=parent;
  }
 else {
    outgoing=new PImage(width,height,RGB);
    outgoing.pixels=new int[width * height];
    System.arraycopy(pixels,0,outgoing.pixels,0,outgoing.pixels.length);
    outgoing.parent=parent;
  }
  return outgoing;
}
