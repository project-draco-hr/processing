{
  image=new PImage(width,height,ALPHA);
  int bitmapSize=width * height;
  byte[] temp=new byte[bitmapSize];
  is.readFully(temp);
  int w=width;
  int h=height;
  int[] pixels=image.pixels;
  for (int y=0; y < h; y++) {
    for (int x=0; x < w; x++) {
      pixels[y * width + x]=temp[y * w + x] & 0xff;
    }
  }
  fromStream=true;
}
