{
  value=is.readInt();
  height=is.readInt();
  width=is.readInt();
  setWidth=is.readInt();
  topExtent=is.readInt();
  leftExtent=is.readInt();
  is.readInt();
  if (value == 'd') {
    if (ascent == 0)     ascent=topExtent;
  }
  if (value == 'p') {
    if (descent == 0)     descent=-topExtent + height;
  }
}
