{
  gl10=gl;
  texWidth=w;
  texHeight=h;
  addTexture(gl);
  offsetX=0;
  offsetY=0;
  lineHeight=0;
  try {
    Method meth=this.getClass().getMethod("recreateResource",new Class[]{PGraphicsAndroid3D.class});
    recreateResourceIdx=a3d.addRecreateResourceMethod(this,meth);
  }
 catch (  Exception e) {
    recreateResourceIdx=-1;
  }
}
