{
  if (sdk == null) {
    try {
      sdk=AndroidSDK.load();
      if (sdk == null) {
        sdk=AndroidSDK.locate(parent);
      }
    }
 catch (    BadSDKException e) {
      e.printStackTrace();
    }
catch (    IOException e) {
      e.printStackTrace();
    }
  }
  if (sdk == null) {
    Base.showWarning("It's gonna be a bad day","The Android SDK could not be loaded.\n" + "Use of Android mode will be all but disabled.",null);
  }
}
