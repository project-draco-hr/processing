{
  String filename="welcome/" + (sketchbook ? "sketchbook.html" : "generic.html");
  File htmlFile=new File("../build/shared/lib/" + filename);
  if (htmlFile.exists()) {
    return htmlFile;
  }
  htmlFile=Base.getContentFile("../../../../../shared/lib/" + filename);
  if (htmlFile.exists()) {
    return htmlFile;
  }
  try {
    return Base.getLibFile(filename);
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
}
