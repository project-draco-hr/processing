{
  EventQueue.invokeLater(new Runnable(){
    @Override public void run(){
      Welcome browser=new Welcome();
      browser.setVisible(true);
      Base.initPlatform();
      try {
        File indexFile=new File("../build/shared/lib/welcome/index.html");
        browser.loadFile(indexFile);
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
);
}
