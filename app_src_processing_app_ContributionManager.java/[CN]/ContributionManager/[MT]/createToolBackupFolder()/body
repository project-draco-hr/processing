{
  File toolsBackupFolder=new File(editor.getBase().getSketchbookToolsFolder(),"old");
  if (!toolsBackupFolder.exists() || !toolsBackupFolder.isDirectory()) {
    if (!toolsBackupFolder.mkdirs()) {
      Base.showWarning("Trouble creating folder to store old libraries in","Could not create folder " + toolsBackupFolder.getAbsolutePath() + ".\n"+ "That's gonna prevent us from replacing the library.",null);
      return null;
    }
  }
  return toolsBackupFolder;
}
