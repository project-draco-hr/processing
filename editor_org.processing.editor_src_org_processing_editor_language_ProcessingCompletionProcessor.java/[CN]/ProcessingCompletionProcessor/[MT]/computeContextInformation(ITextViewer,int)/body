{
  IContextInformation[] result=new IContextInformation[5];
  for (int i=0; i < result.length; i++)   result[i]=new ContextInformation(MessageFormat.format(ProcessingEditorMessages.getString("CompletionProcessor.ContextInfo.display.pattern"),new Object[]{new Integer(i),new Integer(documentOffset)}),MessageFormat.format(ProcessingEditorMessages.getString("CompletionProcessor.ContextInfo.value.pattern"),new Object[]{new Integer(i),new Integer(documentOffset - 5),new Integer(documentOffset + 5)}));
  return result;
}
