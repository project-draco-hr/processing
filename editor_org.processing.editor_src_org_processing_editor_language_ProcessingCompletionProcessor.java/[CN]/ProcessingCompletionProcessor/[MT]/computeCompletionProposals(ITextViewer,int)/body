{
  ICompletionProposal[] result=new ICompletionProposal[fgProposals.length];
  for (int i=0; i < fgProposals.length; i++) {
    IContextInformation info=new ContextInformation(fgProposals[i],MessageFormat.format(ProcessingEditorMessages.getString("CompletionProcessor.Proposal.ContextInfo.pattern"),new Object[]{fgProposals[i]}));
    result[i]=new CompletionProposal(fgProposals[i],documentOffset,0,fgProposals[i].length(),null,fgProposals[i],info,MessageFormat.format(ProcessingEditorMessages.getString("CompletionProcessor.Proposal.hoverinfo.pattern"),new Object[]{fgProposals[i]}));
  }
  return result;
}
