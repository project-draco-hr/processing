{
  if (debugger.isStarted()) {
    debugger.stopDebug();
  }
 else {
    toolbar.activate(JavaToolbar.STOP);
    try {
      if (runtime != null) {
        runtime.close();
        runtime=null;
      }
    }
 catch (    Exception e) {
      statusError(e);
    }
    toolbar.deactivate(JavaToolbar.RUN);
    toolbar.deactivate(JavaToolbar.STOP);
    toFront();
  }
}
