{
  try {
    if (file.isDirectory()) {
      String list[]=file.list();
      for (int i=0; i < list.length; i++) {
        load(new File(file,list[i]),kind);
      }
    }
 else {
      new MacLF(file,kind);
    }
  }
 catch (  Exception e) {
    System.err.println(file);
    e.printStackTrace();
  }
}
