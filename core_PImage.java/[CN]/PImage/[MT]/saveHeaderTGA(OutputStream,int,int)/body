{
  try {
    byte header[]=new byte[18];
    header[2]=0x02;
    header[12]=(byte)(width & 0xff);
    header[13]=(byte)(width >> 8);
    header[14]=(byte)(height & 0xff);
    header[15]=(byte)(height >> 8);
    header[16]=32;
    header[17]=8;
    output.write(header);
    return true;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return false;
}
