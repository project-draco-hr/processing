{
  int f=(b & ALPHA_MASK) >>> 24;
  return (low(((a & ALPHA_MASK) >>> 24) + f,0xff) << 24 | high(((a & RED_MASK) - ((b & RED_MASK) >> 8) * f),GREEN_MASK) & RED_MASK | high(((a & GREEN_MASK) - ((b & GREEN_MASK) >> 8) * f),BLUE_MASK) & GREEN_MASK | high((a & BLUE_MASK) - (((b & BLUE_MASK) * f) >> 8),0));
}
