{
  try {
    if (!saveHeaderTIFF(output,width,height)) {
      return false;
    }
    for (int i=0; i < pixels.length; i++) {
      output.write((pixels[i] >> 16) & 0xff);
      output.write((pixels[i] >> 8) & 0xff);
      output.write(pixels[i] & 0xff);
    }
    output.flush();
    return true;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return false;
}
