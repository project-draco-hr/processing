{
  for (int i=0; i < this.getSketch().getCodeCount(); i++) {
    if (this.getSketch().getCode(i).getExtension().equals("java")) {
      compilationCheckEnabled=false;
      JOptionPane.showMessageDialog(new Frame(),this.getSketch().getName() + " contains .java tabs. Live compilation error checking isn't " + "supported for java tabs. Only "+ "syntax errors will be reported for .pde tabs.");
      break;
    }
  }
}
