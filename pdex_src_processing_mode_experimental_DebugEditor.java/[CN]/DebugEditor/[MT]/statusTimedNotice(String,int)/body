{
  SwingWorker s=new SwingWorker<Void,Void>(){
    @Override protected Void doInBackground() throws Exception {
      statusNotice(msg);
      try {
        Thread.sleep(secs * 1000);
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
      statusEmpty();
      return null;
    }
  }
;
  s.execute();
}
