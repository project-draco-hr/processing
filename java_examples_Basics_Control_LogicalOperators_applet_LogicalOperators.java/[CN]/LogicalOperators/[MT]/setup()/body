{
  size(200,200);
  background(126);
  boolean op=false;
  for (int i=5; i <= 195; i+=5) {
    stroke(0);
    if ((i > 35) && (i < 100)) {
      line(5,i,95,i);
      op=false;
    }
    stroke(76);
    if ((i <= 35) || (i >= 100)) {
      line(105,i,195,i);
      op=true;
    }
    if (op) {
      stroke(0);
      point(width / 2,i);
    }
    if (!op) {
      stroke(255);
      point(width / 4,i);
    }
  }
  noLoop();
}
