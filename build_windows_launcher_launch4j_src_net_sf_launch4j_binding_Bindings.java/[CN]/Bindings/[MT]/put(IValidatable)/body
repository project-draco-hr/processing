{
  for (Iterator iter=_optComponents.values().iterator(); iter.hasNext(); ) {
    ((Binding)iter.next()).put(bean);
  }
  for (Iterator iter=_bindings.values().iterator(); iter.hasNext(); ) {
    Binding b=(Binding)iter.next();
    if (isPropertyNull(bean,b)) {
      b.clear(null);
    }
 else {
      b.put(bean);
    }
  }
  _modified=false;
}
