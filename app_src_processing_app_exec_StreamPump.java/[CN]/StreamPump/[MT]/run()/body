{
  try {
    String line;
    while ((line=reader.readLine()) != null) {
      for (      final LineProcessor out : outs) {
        try {
          out.processLine(line);
        }
 catch (        final Exception e) {
        }
      }
    }
  }
 catch (  final IOException e) {
    throw new RuntimeException("Inside " + this + " for "+ name,e);
  }
}
