{
  super.createActions();
  IAction a=new TextOperationAction(ProcessingEditorMessages.getResourceBundle(),"ContentAssistProposal.",this,ISourceViewer.CONTENTASSIST_PROPOSALS);
  a.setActionDefinitionId(ITextEditorActionDefinitionIds.CONTENT_ASSIST_PROPOSALS);
  setAction("ContentAssistProposal",a);
  a=new TextOperationAction(ProcessingEditorMessages.getResourceBundle(),"ContentAssistTip.",this,ISourceViewer.CONTENTASSIST_CONTEXT_INFORMATION);
  a.setActionDefinitionId(ITextEditorActionDefinitionIds.CONTENT_ASSIST_CONTEXT_INFORMATION);
  setAction("ContentAssistTip",a);
  a=new DefineFoldingRegionAction(ProcessingEditorMessages.getResourceBundle(),"DefineFoldingRegion.",this);
  setAction("DefineFoldingRegion",a);
}
