{
  this.context=parent;
  try {
    compassEventMethod=parent.getClass().getMethod("compassEvent",new Class[]{Float.TYPE,Float.TYPE,Float.TYPE});
  }
 catch (  Exception e) {
  }
  try {
    directionEventMethod=parent.getClass().getMethod("directionEvent",new Class[]{Float.TYPE});
  }
 catch (  Exception e) {
  }
  resume();
}
