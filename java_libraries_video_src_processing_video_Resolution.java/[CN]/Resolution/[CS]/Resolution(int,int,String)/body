{
  this.width=width;
  this.height=height;
  String[] parts=fpsString.split("/");
  if (parts.length == 2) {
    int fpsDenominator=PApplet.parseInt(parts[0]);
    int fpsNumerator=PApplet.parseInt(parts[1]);
    this.fps=(float)fpsDenominator / (float)fpsNumerator;
    this.fpsString=fpsString;
  }
 else {
    this.fps=0.0f;
    this.fpsString="";
  }
}
