{
  try {
    capture.stop();
    channel.settingsDialog();
    capture.startPreview();
  }
 catch (  StdQTException qte) {
    int errorCode=qte.errorCode();
    if (errorCode != Errors.userCanceledErr) {
      qte.printStackTrace();
      throw new RuntimeException("error inside Capture.settings()");
    }
  }
}
