{
  String lower=node.getToken().toLowerCase();
  if (!lower.endsWith("f") && !lower.endsWith("d")) {
    for (int i=0; i < node.getToken().length(); i++) {
      if (node.getToken().charAt(i) == '.') {
        String s=node.getToken() + "f";
        node.setToken(s);
        break;
      }
    }
  }
  return true;
}
