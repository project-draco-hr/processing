{
  if (hoverRegion != null) {
    try {
      if (hoverRegion.getLength() > -1)       return textViewer.getDocument().get(hoverRegion.getOffset(),hoverRegion.getLength());
    }
 catch (    BadLocationException x) {
    }
  }
  return ProcessingEditorMessages.getString("ProcessingTextHover.emptySelection");
}
