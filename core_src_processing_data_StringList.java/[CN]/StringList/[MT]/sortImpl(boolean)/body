{
  new Sort(){
    @Override public int size(){
      return count;
    }
    @Override public float compare(    int a,    int b){
      float diff=data[a].compareToIgnoreCase(data[b]);
      return reverse ? -diff : diff;
    }
    @Override public void swap(    int a,    int b){
      int temp=a;
      a=b;
      b=temp;
    }
  }
.run();
}
