{
  boolean changed=false;
  if (value == null) {
    for (int i=0; i < count; i++) {
      if (data[i] == null) {
        data[i]=newValue;
        changed=true;
      }
    }
  }
 else {
    for (int i=0; i < count; i++) {
      if (value.equals(data[i])) {
        data[i]=newValue;
        changed=true;
      }
    }
  }
  return changed;
}
