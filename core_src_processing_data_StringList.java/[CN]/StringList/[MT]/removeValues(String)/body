{
  int ii=0;
  if (value == null) {
    for (int i=0; i < count; i++) {
      if (data[i] != null) {
        data[ii++]=data[i];
      }
    }
  }
 else {
    for (int i=0; i < count; i++) {
      if (!value.equals(data[i])) {
        data[ii++]=data[i];
      }
    }
  }
  boolean changed=count == ii;
  count=ii;
  return changed;
}
