{
  this.sketch=sketch;
  sketchWidth=sketch.sketchWidth();
  sketchHeight=sketch.sketchHeight();
  if (window != null) {
    NewtCanvasAWT canvas=new NewtCanvasAWT(window);
    canvas.setBounds(0,0,window.getWidth(),window.getHeight());
    canvas.setFocusable(true);
    return canvas;
  }
  return null;
}
