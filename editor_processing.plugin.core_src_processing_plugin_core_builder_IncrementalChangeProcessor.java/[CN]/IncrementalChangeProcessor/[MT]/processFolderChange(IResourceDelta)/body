{
  if (!sp.getProject().exists(delta.getProjectRelativePath()))   return;
switch (delta.getKind()) {
case IResourceDelta.ADDED:
    break;
case IResourceDelta.REMOVED:
  if (delta.getFlags() == IResourceDelta.MOVED_TO)   processInterestingFolderMove(delta);
 else   processInterestingFolderRemoval(delta);
break;
case IResourceDelta.CHANGED:
processInterestingFolderChange(delta);
break;
default :
break;
}
}
