{
  editor=dedit;
  if (timeOut < 5) {
    saveTime=-1;
    throw new IllegalArgumentException("");
  }
 else {
    saveTime=timeOut * 60 * 1000;
    ExperimentalMode.log("AutoSaver Interval(mins): " + timeOut);
  }
  autosaveDir=new File(editor.getSketch().getFolder().getAbsolutePath() + File.separator + "_autosave");
}
