{
  try {
    if (vertexShader != 0) {
      pg.finalizeGLSLVertShaderObject(vertexShader,context.code());
    }
    if (fragmentShader != 0) {
      pg.finalizeGLSLFragShaderObject(fragmentShader,context.code());
    }
    if (programObject != 0) {
      pg.finalizeGLSLProgramObject(programObject,context.code());
    }
  }
  finally {
    super.finalize();
  }
}
