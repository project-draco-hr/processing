{
  try {
    if (vertexShader != 0) {
      pgMain.finalizeGLSLVertShaderObject(vertexShader,context.code());
    }
    if (fragmentShader != 0) {
      pgMain.finalizeGLSLFragShaderObject(fragmentShader,context.code());
    }
    if (programObject != 0) {
      pgMain.finalizeGLSLProgramObject(programObject,context.code());
    }
  }
  finally {
    super.finalize();
  }
}
