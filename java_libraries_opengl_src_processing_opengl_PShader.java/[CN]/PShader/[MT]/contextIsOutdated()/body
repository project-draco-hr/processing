{
  boolean outdated=!pgl.contextIsCurrent(context);
  if (outdated) {
    pg.removeGLSLProgramObject(programObject);
    pg.removeGLSLVertShaderObject(vertexShader);
    pg.removeGLSLFragShaderObject(fragmentShader);
    programObject=0;
    vertexShader=0;
    fragmentShader=0;
  }
  return outdated;
}
