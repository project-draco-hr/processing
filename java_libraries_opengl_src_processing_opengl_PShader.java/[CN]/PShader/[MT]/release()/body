{
  if (vertexShader != 0) {
    ogl.finalizeGLSLVertShaderObject(vertexShader);
    vertexShader=0;
  }
  if (fragmentShader != 0) {
    ogl.finalizeGLSLFragShaderObject(fragmentShader);
    fragmentShader=0;
  }
  if (programObject != 0) {
    ogl.finalizeGLSLProgramObject(programObject);
    programObject=0;
  }
}
