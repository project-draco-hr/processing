{
  if (glVertex != 0) {
    pgMain.deleteGLSLVertShaderObject(glVertex,context.code());
    glVertex=0;
  }
  if (glFragment != 0) {
    pgMain.deleteGLSLFragShaderObject(glFragment,context.code());
    glFragment=0;
  }
  if (glProgram != 0) {
    pgMain.deleteGLSLProgramObject(glProgram,context.code());
    glProgram=0;
  }
}
