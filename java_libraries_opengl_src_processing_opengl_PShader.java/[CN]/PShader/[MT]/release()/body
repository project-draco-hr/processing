{
  if (vertexShader != 0) {
    pgMain.deleteGLSLVertShaderObject(vertexShader,context.code());
    vertexShader=0;
  }
  if (fragmentShader != 0) {
    pgMain.deleteGLSLFragShaderObject(fragmentShader,context.code());
    fragmentShader=0;
  }
  if (programObject != 0) {
    pgMain.deleteGLSLProgramObject(programObject,context.code());
    programObject=0;
  }
}
