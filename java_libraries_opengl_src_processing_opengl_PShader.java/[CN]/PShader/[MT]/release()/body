{
  if (vertexShader != 0) {
    ogl.deleteGLSLVertShaderObject(vertexShader);
    vertexShader=0;
  }
  if (fragmentShader != 0) {
    ogl.deleteGLSLFragShaderObject(fragmentShader);
    fragmentShader=0;
  }
  if (programObject != 0) {
    ogl.deleteGLSLProgramObject(programObject);
    programObject=0;
  }
}
