{
  if (!visible)   return;
  g.pushMatrix();
  if (drawMode == PConstants.CENTER) {
    g.translate(x - width / 2,y - height / 2);
  }
 else   if ((drawMode == PConstants.CORNER) || (drawMode == PConstants.CORNERS)) {
    g.translate(x,y);
  }
  pre(g);
  drawImpl(g);
  post(g);
  g.popMatrix();
}
