{
  PShape res=null;
  if (family == GROUP) {
    res=g.createShape(GROUP);
    copyGroup(g,res);
  }
 else   if (family == PRIMITIVE) {
    res=g.createShape(kind,params);
    copyPrimitive(res);
  }
 else   if (family == GEOMETRY) {
    res=g.createShape(kind);
    copyGeometry(res);
  }
 else   if (family == PATH) {
    res=g.createShape(PATH);
    copyPath(res);
  }
  res.setName(name);
  return res;
}
