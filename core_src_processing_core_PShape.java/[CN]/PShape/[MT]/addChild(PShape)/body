{
  if (children == null) {
    children=new PShape[2];
  }
  if (childCount == children.length) {
    children=(PShape[])PApplet.expand(children);
  }
  children[childCount++]=who;
  who.parent=this;
  if (table == null) {
    table=new HashMap<String,PShape>();
  }
  table.put(who.getName(),who);
}
