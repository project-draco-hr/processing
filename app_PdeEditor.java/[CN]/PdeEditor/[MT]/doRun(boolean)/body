{
  doClose();
  running=true;
  buttons.run();
  for (int i=0; i < 10; i++)   System.out.println();
  presenting=present;
  if (presenting) {
    presentationWindow.show();
    presentationWindow.toFront();
  }
  try {
    sketch.run();
  }
 catch (  PdeException e) {
    error(e);
  }
catch (  Exception e) {
    e.printStackTrace();
  }
}
