{
  if (presenting) {
    presentationWindow.hide();
  }
 else {
    try {
      if (runtime.window != null) {
        appletLocation=runtime.window.getLocation();
      }
    }
 catch (    NullPointerException e) {
    }
  }
  if (running) {
    doStop();
  }
  try {
    if (runtime != null) {
      runtime.close();
      runtime=null;
    }
  }
 catch (  Exception e) {
  }
  cleanTempFiles();
  base.toFront();
}
