{
  if (presenting) {
    presentationWindow.hide();
  }
 else {
    try {
      appletLocation=pdeRuntime.window.getLocation();
    }
 catch (    NullPointerException e) {
    }
  }
  if (running) {
    doStop();
  }
  try {
    if (pdeRuntime != null) {
      pdeRuntime.close();
      pdeRuntime=null;
    }
  }
 catch (  Exception e) {
  }
  cleanTempFiles();
}
