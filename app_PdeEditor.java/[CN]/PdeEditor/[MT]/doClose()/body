{
  try {
    if ((presentationWindow == null) || (!presentationWindow.isVisible())) {
      appletLocation=engine.window.getLocation();
    }
  }
 catch (  NullPointerException e) {
  }
  if (running) {
    doStop();
  }
  try {
    engine.close();
    engine=null;
  }
 catch (  Exception e) {
  }
  buttons.clear();
}
