{
  try {
    FileInputStream from=new FileInputStream(afile);
    FileOutputStream to=new FileOutputStream(bfile);
    byte[] buffer=new byte[4096];
    int bytesRead;
    while ((bytesRead=from.read(buffer)) != -1) {
      to.write(buffer,0,bytesRead);
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
