{
  shapeLast=vertexCount;
  if (vertexCount == 0) {
    shape=0;
    return;
  }
  if (stroke) {
    endShapeStroke(mode);
  }
  if (fill) {
    endShapeFill();
  }
  if (!hints[ENABLE_DEPTH_SORT]) {
    if (fill) {
      renderTriangles(0,triangleCount);
      if (raw != null) {
      }
      triangleCount=0;
    }
    if (stroke) {
      renderLines(0,lineCount);
      if (raw != null) {
      }
      lineCount=0;
    }
    pathCount=0;
  }
  shape=0;
}
