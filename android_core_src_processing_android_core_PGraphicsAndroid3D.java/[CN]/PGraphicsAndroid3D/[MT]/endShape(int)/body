{
  shapeLast=vertexCount;
  shapeLastPlusClipped=shapeLast;
  if (vertexCount == 0) {
    shape=0;
    return;
  }
  if (stroke) {
    endShapeStroke(mode);
  }
  if (fill) {
    endShapeFill();
  }
  if (fill)   renderTriangles(0,triangleCount);
  if (stroke) {
    renderLines(0,lineCount);
    pathCount=0;
  }
  shape=0;
}
