{
  final PrintWriter writer=PApplet.createWriter(file);
  final String sdkPath=sdk.getSdkFolder().getAbsolutePath();
  if (Base.isWindows()) {
    writer.println("sdk.dir=" + sdkPath.replace('\\','/'));
  }
 else {
    writer.println("sdk.dir=" + sdkPath);
  }
  writer.flush();
  writer.close();
}
