{
  try {
    if (t.frameCount() == 0) {
      System.out.println("call stack empty");
    }
 else {
      Location l=t.frame(0).location();
      printSourceLocation(l);
    }
  }
 catch (  IncompatibleThreadStateException ex) {
    Logger.getLogger(Debugger.class.getName()).log(Level.SEVERE,null,ex);
  }
}
