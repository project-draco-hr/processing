{
  try {
    array.setValue(index,value);
  }
 catch (  InvalidTypeException ex) {
    Logger.getLogger(ArrayFieldNode.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  ClassNotLoadedException ex) {
    Logger.getLogger(ArrayFieldNode.class.getName()).log(Level.SEVERE,null,ex);
  }
  this.value=value;
}
