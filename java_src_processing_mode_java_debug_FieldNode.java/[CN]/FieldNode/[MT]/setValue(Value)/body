{
  try {
    obj.setValue(field,value);
  }
 catch (  InvalidTypeException ex) {
    Logger.getLogger(FieldNode.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  ClassNotLoadedException ex) {
    Logger.getLogger(FieldNode.class.getName()).log(Level.SEVERE,null,ex);
  }
  this.value=value;
}
