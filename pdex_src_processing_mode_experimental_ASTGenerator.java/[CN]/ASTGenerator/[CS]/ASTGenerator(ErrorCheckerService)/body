{
  this.errorCheckerService=ecs;
  this.editor=ecs.getEditor();
  frame2=new JFrame();
  jtree=new JTree();
  frame2.setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
  frame2.setBounds(new Rectangle(100,100,460,620));
