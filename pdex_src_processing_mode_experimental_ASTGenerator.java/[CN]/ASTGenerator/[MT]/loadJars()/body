{
  try {
    factory=new ClassPathFactory();
    StringBuffer tehPath=new StringBuffer(System.getProperty("java.class.path") + File.pathSeparatorChar + System.getProperty("java.home")+ "/lib/rt.jar");
    if (errorCheckerService.classpathJars != null) {
      for (      URL jarPath : errorCheckerService.classpathJars) {
        tehPath.append(jarPath.getPath() + File.pathSeparatorChar);
      }
    }
    classPath=factory.createFromPath(tehPath.toString());
    RegExpResourceFilter regExpResourceFilter=new RegExpResourceFilter(".*","Vec3D.class");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
