{
  UIManager.put("TabbedPane.selected",Color.BLACK);
  UIManager.put("TabbedPane.tabsOverlapBorder",true);
  super.installDefaults();
  highlight=Color.RED;
  lightHighlight=Color.GRAY;
  shadow=Color.GREEN;
  darkShadow=Color.BLACK;
