{
  String fullName=name;
  if (nsPrefix != null) {
    fullName=nsPrefix + ':' + name;
  }
  if (this.stack.empty()) {
    parent.set(fullName,nsURI,systemID,lineNr);
    stack.push(parent);
    root=parent;
  }
 else {
    XMLElement top=(XMLElement)this.stack.peek();
    XMLElement elt=new XMLElement(fullName,nsURI,systemID,lineNr);
    top.addChild(elt);
    stack.push(elt);
  }
}
