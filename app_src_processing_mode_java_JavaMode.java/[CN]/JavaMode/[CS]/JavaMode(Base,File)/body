{
  super(base,folder);
  try {
    loadKeywords();
  }
 catch (  IOException e) {
    Base.showError("Problem loading keywords","Could not load keywords.txt, please re-install Processing.",e);
  }
  try {
    theme=new Settings(new File(folder,"theme.txt"));
  }
 catch (  IOException e) {
    Base.showError("Problem loading theme.txt","Could not load theme.txt, please re-install Processing",e);
  }
}
