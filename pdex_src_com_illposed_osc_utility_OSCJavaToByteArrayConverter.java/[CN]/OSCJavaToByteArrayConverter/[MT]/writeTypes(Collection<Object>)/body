{
  for (  Object type : types) {
    if (null == type) {
      continue;
    }
    if (type.getClass().isArray()) {
      stream.write('[');
      writeTypesArray((Object[])type);
      stream.write(']');
      continue;
    }
    if (Boolean.TRUE.equals(type)) {
      stream.write('T');
      continue;
    }
    if (Boolean.FALSE.equals(type)) {
      stream.write('F');
      continue;
    }
    writeType(type.getClass());
  }
  appendNullCharToAlignStream();
}
