{
  if (Integer.class.equals(c)) {
    stream.write('i');
  }
 else   if (java.math.BigInteger.class.equals(c)) {
    stream.write('h');
  }
 else   if (Float.class.equals(c)) {
    stream.write('f');
  }
 else   if (Double.class.equals(c)) {
    stream.write('d');
  }
 else   if (String.class.equals(c)) {
    stream.write('s');
  }
 else   if (Character.class.equals(c)) {
    stream.write('c');
  }
}
