{
  out.write((int)(v >>> 56) & 0xff);
  out.write((int)(v >>> 48) & 0xff);
  out.write((int)(v >>> 40) & 0xff);
  out.write((int)(v >>> 32) & 0xff);
  out.write((int)(v >>> 24) & 0xff);
  out.write((int)(v >>> 16) & 0xff);
  out.write((int)(v >>> 8) & 0xff);
  out.write((int)(v >>> 0) & 0xff);
  incCount(8);
}
