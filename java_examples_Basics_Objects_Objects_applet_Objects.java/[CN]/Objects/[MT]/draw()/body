{
  background(0);
  r1.display();
  r2.display();
  r3.display();
  r4.display();
  r1.move(mouseX - (width / 2),mouseY + (height * 0.1f),30);
  r2.move((mouseX + (width * 0.05f)) % width,mouseY + (height * 0.025f),20);
  r3.move(mouseX / 4,mouseY - (height * 0.025f),40);
  r4.move(mouseX - (width / 2),(height - mouseY),50);
}
